nqubits = 5;
name = "5v1";
nstates = 10;

amplitude[x_,y_] := (Exp[-10.0 I y]*(Exp[-10.0 I y] (1 Cos[x]^5 + 1 (-I Sin[x])^5) + Exp[-2.0 I y] (5 (-I Sin[x])^1 Cos[x]^4 + 5 (-I Sin[x])^4 Cos[x]^1) + Exp[2.0 I y] (10 (-I Sin[x])^2 Cos[x]^3 + 10 (-I Sin[x])^3 Cos[x]^2))*(1 (-I Sin[x])^1 Cos[x]^4 + 1 (-I Sin[x])^4 Cos[x]^1) + Exp[-2.0 I y]*(Exp[-10.0 I y] (1 (-I Sin[x])^1 Cos[x]^4 + 1 (-I Sin[x])^4 Cos[x]^1) + Exp[-2.0 I y] (1 Cos[x]^5 + 1 (-I Sin[x])^5 + 4 (-I Sin[x])^2 Cos[x]^3 + 4 (-I Sin[x])^3 Cos[x]^2) + Exp[2.0 I y] (4 (-I Sin[x])^1 Cos[x]^4 + 4 (-I Sin[x])^4 Cos[x]^1 + 6 (-I Sin[x])^3 Cos[x]^2 + 6 (-I Sin[x])^2 Cos[x]^3))*(1 Cos[x]^5 + 1 (-I Sin[x])^5 + 4 (-I Sin[x])^2 Cos[x]^3 + 4 (-I Sin[x])^3 Cos[x]^2) + Exp[2.0 I y]*(Exp[-10.0 I y] (1 (-I Sin[x])^2 Cos[x]^3 + 1 (-I Sin[x])^3 Cos[x]^2) + Exp[-2.0 I y] (2 (-I Sin[x])^1 Cos[x]^4 + 2 (-I Sin[x])^4 Cos[x]^1 + 3 (-I Sin[x])^3 Cos[x]^2 + 3 (-I Sin[x])^2 Cos[x]^3) + Exp[2.0 I y] (1 Cos[x]^5 + 1 (-I Sin[x])^5 + 6 (-I Sin[x])^2 Cos[x]^3 + 6 (-I Sin[x])^3 Cos[x]^2 + 3 (-I Sin[x])^1 Cos[x]^4 + 3 (-I Sin[x])^4 Cos[x]^1))*(4 (-I Sin[x])^1 Cos[x]^4 + 4 (-I Sin[x])^4 Cos[x]^1 + 6 (-I Sin[x])^3 Cos[x]^2 + 6 (-I Sin[x])^2 Cos[x]^3))/Sqrt[2^nqubits];
probability[x_,y_] := Abs[amplitude[x,y]]^2;

result = Maximize[{nstates*probability[a, b], 0 <= a < Pi/2, 0 <= b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

n = Pi;
