nqubits = 6;
name = "6v3";
nstates = 20;

amplitude[x_,y_] := (Exp[-15.0 I y]*(Exp[-15.0 I y] (1 Cos[x]^6 + 1 (-I Sin[x])^6) + Exp[-5.0 I y] (6 (-I Sin[x])^1 Cos[x]^5 + 6 (-I Sin[x])^5 Cos[x]^1) + Exp[1.0 I y] (15 (-I Sin[x])^2 Cos[x]^4 + 15 (-I Sin[x])^4 Cos[x]^2) + Exp[3.0 I y] (20 (-I Sin[x])^3 Cos[x]^3))*(2 (-I Sin[x])^3 Cos[x]^3) + Exp[-5.0 I y]*(Exp[-15.0 I y] (1 (-I Sin[x])^1 Cos[x]^5 + 1 (-I Sin[x])^5 Cos[x]^1) + Exp[-5.0 I y] (1 Cos[x]^6 + 1 (-I Sin[x])^6 + 5 (-I Sin[x])^2 Cos[x]^4 + 5 (-I Sin[x])^4 Cos[x]^2) + Exp[1.0 I y] (5 (-I Sin[x])^1 Cos[x]^5 + 5 (-I Sin[x])^5 Cos[x]^1 + 20 (-I Sin[x])^3 Cos[x]^3) + Exp[3.0 I y] (10 (-I Sin[x])^2 Cos[x]^4 + 10 (-I Sin[x])^4 Cos[x]^2))*(6 (-I Sin[x])^2 Cos[x]^4 + 6 (-I Sin[x])^4 Cos[x]^2) + Exp[1.0 I y]*(Exp[-15.0 I y] (1 (-I Sin[x])^2 Cos[x]^4 + 1 (-I Sin[x])^4 Cos[x]^2) + Exp[-5.0 I y] (2 (-I Sin[x])^1 Cos[x]^5 + 2 (-I Sin[x])^5 Cos[x]^1 + 8 (-I Sin[x])^3 Cos[x]^3) + Exp[1.0 I y] (1 Cos[x]^6 + 1 (-I Sin[x])^6 + 14 (-I Sin[x])^2 Cos[x]^4 + 14 (-I Sin[x])^4 Cos[x]^2) + Exp[3.0 I y] (4 (-I Sin[x])^1 Cos[x]^5 + 4 (-I Sin[x])^5 Cos[x]^1 + 12 (-I Sin[x])^3 Cos[x]^3))*(6 (-I Sin[x])^1 Cos[x]^5 + 6 (-I Sin[x])^5 Cos[x]^1 + 18 (-I Sin[x])^3 Cos[x]^3) + Exp[3.0 I y]*(Exp[-15.0 I y] (2 (-I Sin[x])^3 Cos[x]^3) + Exp[-5.0 I y] (6 (-I Sin[x])^2 Cos[x]^4 + 6 (-I Sin[x])^4 Cos[x]^2) + Exp[1.0 I y] (6 (-I Sin[x])^1 Cos[x]^5 + 6 (-I Sin[x])^5 Cos[x]^1 + 18 (-I Sin[x])^3 Cos[x]^3) + Exp[3.0 I y] (1 Cos[x]^6 + 1 (-I Sin[x])^6 + 9 (-I Sin[x])^2 Cos[x]^4 + 9 (-I Sin[x])^4 Cos[x]^2))*(1 Cos[x]^6 + 1 (-I Sin[x])^6 + 9 (-I Sin[x])^2 Cos[x]^4 + 9 (-I Sin[x])^4 Cos[x]^2))/Sqrt[2^nqubits];
probability[x_,y_] := Abs[amplitude[x,y]]^2;

result = Maximize[{nstates*probability[a, b], 0 <= a < Pi/2, 0 <= b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

n = Pi;
