nqubits = 7;
name = "7v0";
nstates = 2;

amplitude[x_,y_] := (Exp[-21.0 I y]*(Exp[-21.0 I y] (1 Cos[x]^7 + 1 (-I Sin[x])^7) + Exp[-9.0 I y] (7 (-I Sin[x])^1 Cos[x]^6 + 7 (-I Sin[x])^6 Cos[x]^1) + Exp[-1.0 I y] (21 (-I Sin[x])^2 Cos[x]^5 + 21 (-I Sin[x])^5 Cos[x]^2) + Exp[3.0 I y] (35 (-I Sin[x])^3 Cos[x]^4 + 35 (-I Sin[x])^4 Cos[x]^3))*(1 Cos[x]^7 + 1 (-I Sin[x])^7) + Exp[-9.0 I y]*(Exp[-21.0 I y] (1 (-I Sin[x])^1 Cos[x]^6 + 1 (-I Sin[x])^6 Cos[x]^1) + Exp[-9.0 I y] (1 Cos[x]^7 + 1 (-I Sin[x])^7 + 6 (-I Sin[x])^2 Cos[x]^5 + 6 (-I Sin[x])^5 Cos[x]^2) + Exp[-1.0 I y] (6 (-I Sin[x])^1 Cos[x]^6 + 6 (-I Sin[x])^6 Cos[x]^1 + 15 (-I Sin[x])^3 Cos[x]^4 + 15 (-I Sin[x])^4 Cos[x]^3) + Exp[3.0 I y] (15 (-I Sin[x])^2 Cos[x]^5 + 15 (-I Sin[x])^5 Cos[x]^2 + 20 (-I Sin[x])^4 Cos[x]^3 + 20 (-I Sin[x])^3 Cos[x]^4))*(7 (-I Sin[x])^1 Cos[x]^6 + 7 (-I Sin[x])^6 Cos[x]^1) + Exp[-1.0 I y]*(Exp[-21.0 I y] (1 (-I Sin[x])^2 Cos[x]^5 + 1 (-I Sin[x])^5 Cos[x]^2) + Exp[-9.0 I y] (2 (-I Sin[x])^1 Cos[x]^6 + 2 (-I Sin[x])^6 Cos[x]^1 + 5 (-I Sin[x])^3 Cos[x]^4 + 5 (-I Sin[x])^4 Cos[x]^3) + Exp[-1.0 I y] (1 Cos[x]^7 + 1 (-I Sin[x])^7 + 10 (-I Sin[x])^2 Cos[x]^5 + 10 (-I Sin[x])^5 Cos[x]^2 + 10 (-I Sin[x])^4 Cos[x]^3 + 10 (-I Sin[x])^3 Cos[x]^4) + Exp[3.0 I y] (5 (-I Sin[x])^1 Cos[x]^6 + 5 (-I Sin[x])^6 Cos[x]^1 + 20 (-I Sin[x])^3 Cos[x]^4 + 20 (-I Sin[x])^4 Cos[x]^3 + 10 (-I Sin[x])^2 Cos[x]^5 + 10 (-I Sin[x])^5 Cos[x]^2))*(21 (-I Sin[x])^2 Cos[x]^5 + 21 (-I Sin[x])^5 Cos[x]^2) + Exp[3.0 I y]*(Exp[-21.0 I y] (1 (-I Sin[x])^3 Cos[x]^4 + 1 (-I Sin[x])^4 Cos[x]^3) + Exp[-9.0 I y] (3 (-I Sin[x])^2 Cos[x]^5 + 3 (-I Sin[x])^5 Cos[x]^2 + 4 (-I Sin[x])^4 Cos[x]^3 + 4 (-I Sin[x])^3 Cos[x]^4) + Exp[-1.0 I y] (3 (-I Sin[x])^1 Cos[x]^6 + 3 (-I Sin[x])^6 Cos[x]^1 + 12 (-I Sin[x])^3 Cos[x]^4 + 12 (-I Sin[x])^4 Cos[x]^3 + 6 (-I Sin[x])^5 Cos[x]^2 + 6 (-I Sin[x])^2 Cos[x]^5) + Exp[3.0 I y] (1 Cos[x]^7 + 1 (-I Sin[x])^7 + 12 (-I Sin[x])^2 Cos[x]^5 + 12 (-I Sin[x])^5 Cos[x]^2 + 4 (-I Sin[x])^1 Cos[x]^6 + 4 (-I Sin[x])^6 Cos[x]^1 + 18 (-I Sin[x])^4 Cos[x]^3 + 18 (-I Sin[x])^3 Cos[x]^4))*(35 (-I Sin[x])^3 Cos[x]^4 + 35 (-I Sin[x])^4 Cos[x]^3))/Sqrt[2^nqubits];
probability[x_,y_] := Abs[amplitude[x,y]]^2;

result = Maximize[{nstates*probability[a, b], 0 <= a < Pi/2, 0 <= b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

n = Pi;
