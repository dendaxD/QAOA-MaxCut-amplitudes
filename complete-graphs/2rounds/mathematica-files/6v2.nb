nqubits = 6;
name = "6v2";
nstates = 30;

amplitude[x1_,y1_,x2_,y2_] := (Exp[-15.0 I y2]*(Exp[-15.0 I y1] (1 Cos[x1]^6 + 1 (-I Sin[x1])^6) + Exp[-5.0 I y1] (6 (-I Sin[x1])^1 Cos[x1]^5 + 6 (-I Sin[x1])^5 Cos[x1]^1) + Exp[1.0 I y1] (15 (-I Sin[x1])^2 Cos[x1]^4 + 15 (-I Sin[x1])^4 Cos[x1]^2) + Exp[3.0 I y1] (20 (-I Sin[x1])^3 Cos[x1]^3))*(1 (-I Sin[x2])^2 Cos[x2]^4 + 1 (-I Sin[x2])^4 Cos[x2]^2) + Exp[-5.0 I y2]*(Exp[-15.0 I y1] (1 (-I Sin[x1])^1 Cos[x1]^5 + 1 (-I Sin[x1])^5 Cos[x1]^1) + Exp[-5.0 I y1] (1 Cos[x1]^6 + 1 (-I Sin[x1])^6 + 5 (-I Sin[x1])^2 Cos[x1]^4 + 5 (-I Sin[x1])^4 Cos[x1]^2) + Exp[1.0 I y1] (5 (-I Sin[x1])^1 Cos[x1]^5 + 5 (-I Sin[x1])^5 Cos[x1]^1 + 20 (-I Sin[x1])^3 Cos[x1]^3) + Exp[3.0 I y1] (10 (-I Sin[x1])^2 Cos[x1]^4 + 10 (-I Sin[x1])^4 Cos[x1]^2))*(2 (-I Sin[x2])^1 Cos[x2]^5 + 2 (-I Sin[x2])^5 Cos[x2]^1 + 8 (-I Sin[x2])^3 Cos[x2]^3) + Exp[1.0 I y2]*(Exp[-15.0 I y1] (1 (-I Sin[x1])^2 Cos[x1]^4 + 1 (-I Sin[x1])^4 Cos[x1]^2) + Exp[-5.0 I y1] (2 (-I Sin[x1])^1 Cos[x1]^5 + 2 (-I Sin[x1])^5 Cos[x1]^1 + 8 (-I Sin[x1])^3 Cos[x1]^3) + Exp[1.0 I y1] (1 Cos[x1]^6 + 1 (-I Sin[x1])^6 + 14 (-I Sin[x1])^2 Cos[x1]^4 + 14 (-I Sin[x1])^4 Cos[x1]^2) + Exp[3.0 I y1] (4 (-I Sin[x1])^1 Cos[x1]^5 + 4 (-I Sin[x1])^5 Cos[x1]^1 + 12 (-I Sin[x1])^3 Cos[x1]^3))*(1 Cos[x2]^6 + 1 (-I Sin[x2])^6 + 14 (-I Sin[x2])^2 Cos[x2]^4 + 14 (-I Sin[x2])^4 Cos[x2]^2) + Exp[3.0 I y2]*(Exp[-15.0 I y1] (2 (-I Sin[x1])^3 Cos[x1]^3) + Exp[-5.0 I y1] (6 (-I Sin[x1])^2 Cos[x1]^4 + 6 (-I Sin[x1])^4 Cos[x1]^2) + Exp[1.0 I y1] (6 (-I Sin[x1])^1 Cos[x1]^5 + 6 (-I Sin[x1])^5 Cos[x1]^1 + 18 (-I Sin[x1])^3 Cos[x1]^3) + Exp[3.0 I y1] (1 Cos[x1]^6 + 1 (-I Sin[x1])^6 + 9 (-I Sin[x1])^2 Cos[x1]^4 + 9 (-I Sin[x1])^4 Cos[x1]^2))*(4 (-I Sin[x2])^1 Cos[x2]^5 + 4 (-I Sin[x2])^5 Cos[x2]^1 + 12 (-I Sin[x2])^3 Cos[x2]^3))/Sqrt[2^nqubits];
probability[x1_,y1_,x2_,y2_] := Abs[amplitude[x1,y1,x2,y2]]^2;

result = Maximize[{nstates*probability[a, b, c, d], 0 <= a < Pi/2, 0 <= b < Pi, 0 <= c < Pi/2, 0 <= d < Pi}, {a, b, c, d}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

n = Pi;
