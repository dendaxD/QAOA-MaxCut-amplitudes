amplitude2[x_,y_] := ((Exp[-190.0 I y] ((I Sin[x])^1 Cos[x]^19 + (I Sin[x])^19 Cos[x]^1) + Exp[-152.0 I y] (Cos[x]^20 + (I Sin[x])^20 + 19 (I Sin[x])^2 Cos[x]^18 + 19 (I Sin[x])^18 Cos[x]^2) + Exp[-118.0 I y] (19 (I Sin[x])^1 Cos[x]^19 + 19 (I Sin[x])^19 Cos[x]^1 + 171 (I Sin[x])^3 Cos[x]^17 + 171 (I Sin[x])^17 Cos[x]^3) + Exp[-88.0 I y] (171 (I Sin[x])^2 Cos[x]^18 + 171 (I Sin[x])^18 Cos[x]^2 + 969 (I Sin[x])^4 Cos[x]^16 + 969 (I Sin[x])^16 Cos[x]^4) + Exp[-62.0 I y] (969 (I Sin[x])^3 Cos[x]^17 + 969 (I Sin[x])^17 Cos[x]^3 + 3876 (I Sin[x])^5 Cos[x]^15 + 3876 (I Sin[x])^15 Cos[x]^5) + Exp[-40.0 I y] (3876 (I Sin[x])^4 Cos[x]^16 + 3876 (I Sin[x])^16 Cos[x]^4 + 11628 (I Sin[x])^6 Cos[x]^14 + 11628 (I Sin[x])^14 Cos[x]^6) + Exp[-22.0 I y] (11628 (I Sin[x])^5 Cos[x]^15 + 11628 (I Sin[x])^15 Cos[x]^5 + 27132 (I Sin[x])^7 Cos[x]^13 + 27132 (I Sin[x])^13 Cos[x]^7) + Exp[-8.0 I y] (27132 (I Sin[x])^6 Cos[x]^14 + 27132 (I Sin[x])^14 Cos[x]^6 + 50388 (I Sin[x])^8 Cos[x]^12 + 50388 (I Sin[x])^12 Cos[x]^8) + Exp[2.0 I y] (50388 (I Sin[x])^7 Cos[x]^13 + 50388 (I Sin[x])^13 Cos[x]^7 + 75582 (I Sin[x])^9 Cos[x]^11 + 75582 (I Sin[x])^11 Cos[x]^9) + Exp[8.0 I y] (75582 (I Sin[x])^8 Cos[x]^12 + 75582 (I Sin[x])^12 Cos[x]^8 + 184756 (I Sin[x])^10 Cos[x]^10) + Exp[10.0 I y] (92378 (I Sin[x])^9 Cos[x]^11 + 92378 (I Sin[x])^11 Cos[x]^9))/Sqrt[2^n]);
