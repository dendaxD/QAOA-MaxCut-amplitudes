nqubits = 11;
name = "11v2";
nstates = 110;

amplitude[x_,y_] := (Exp[-55.0 I y] (1 (I Sin[x])^2 Cos[x]^9 + 1 (I Sin[x])^9 Cos[x]^2) + Exp[-45.0 I y] (2 (I Sin[x])^1 Cos[x]^10 + 2 (I Sin[x])^10 Cos[x]^1 + 9 (I Sin[x])^3 Cos[x]^8 + 9 (I Sin[x])^8 Cos[x]^3) + Exp[-37.0 I y] (1 Cos[x]^11 + 1 (I Sin[x])^11 + 18 (I Sin[x])^2 Cos[x]^9 + 18 (I Sin[x])^9 Cos[x]^2 + 36 (I Sin[x])^4 Cos[x]^7 + 36 (I Sin[x])^7 Cos[x]^4) + Exp[-31.0 I y] (9 (I Sin[x])^1 Cos[x]^10 + 9 (I Sin[x])^10 Cos[x]^1 + 72 (I Sin[x])^3 Cos[x]^8 + 72 (I Sin[x])^8 Cos[x]^3 + 84 (I Sin[x])^5 Cos[x]^6 + 84 (I Sin[x])^6 Cos[x]^5) + Exp[-27.0 I y] (36 (I Sin[x])^2 Cos[x]^9 + 36 (I Sin[x])^9 Cos[x]^2 + 168 (I Sin[x])^4 Cos[x]^7 + 168 (I Sin[x])^7 Cos[x]^4 + 126 (I Sin[x])^6 Cos[x]^5 + 126 (I Sin[x])^5 Cos[x]^6) + Exp[-25.0 I y] (84 (I Sin[x])^3 Cos[x]^8 + 84 (I Sin[x])^8 Cos[x]^3 + 252 (I Sin[x])^5 Cos[x]^6 + 252 (I Sin[x])^6 Cos[x]^5 + 126 (I Sin[x])^4 Cos[x]^7 + 126 (I Sin[x])^7 Cos[x]^4))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-55.0 I y] (1 (I Sin[x])^2 Cos[x]^9 + 1 (I Sin[x])^9 Cos[x]^2) + Exp[-45.0 I y] (2 (I Sin[x])^1 Cos[x]^10 + 2 (I Sin[x])^10 Cos[x]^1 + 9 (I Sin[x])^3 Cos[x]^8 + 9 (I Sin[x])^8 Cos[x]^3) + Exp[-37.0 I y] (1 Cos[x]^11 + 1 (I Sin[x])^11 + 18 (I Sin[x])^2 Cos[x]^9 + 18 (I Sin[x])^9 Cos[x]^2 + 36 (I Sin[x])^4 Cos[x]^7 + 36 (I Sin[x])^7 Cos[x]^4) + Exp[-31.0 I y] (9 (I Sin[x])^1 Cos[x]^10 + 9 (I Sin[x])^10 Cos[x]^1 + 72 (I Sin[x])^3 Cos[x]^8 + 72 (I Sin[x])^8 Cos[x]^3 + 84 (I Sin[x])^5 Cos[x]^6 + 84 (I Sin[x])^6 Cos[x]^5) + Exp[-27.0 I y] (36 (I Sin[x])^2 Cos[x]^9 + 36 (I Sin[x])^9 Cos[x]^2 + 168 (I Sin[x])^4 Cos[x]^7 + 168 (I Sin[x])^7 Cos[x]^4 + 126 (I Sin[x])^6 Cos[x]^5 + 126 (I Sin[x])^5 Cos[x]^6) + Exp[-25.0 I y] (84 (I Sin[x])^3 Cos[x]^8 + 84 (I Sin[x])^8 Cos[x]^3 + 252 (I Sin[x])^5 Cos[x]^6 + 252 (I Sin[x])^6 Cos[x]^5 + 126 (I Sin[x])^4 Cos[x]^7 + 126 (I Sin[x])^7 Cos[x]^4));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = Maximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

f = probability[c, d]; n = Pi;

Plot3D[f, {c, 0, n/2}, {d, -n, n}, PlotRange -> All]
ContourPlot[probability[x, y], {x, 0, n/2}, {y, 0, n}, PlotLegends -> Automatic, Contours -> 30]
