nqubits = 16;
label = "16v2";
nstates = 240;

amplitude[x_,y_] := ((Exp[-120.0 I y] ((I Sin[x])^2 Cos[x]^14 + (I Sin[x])^14 Cos[x]^2) + Exp[-90.0 I y] (2 (I Sin[x])^1 Cos[x]^15 + 2 (I Sin[x])^15 Cos[x]^1 + 14 (I Sin[x])^3 Cos[x]^13 + 14 (I Sin[x])^13 Cos[x]^3) + Exp[-64.0 I y] (Cos[x]^16 + (I Sin[x])^16 + 28 (I Sin[x])^2 Cos[x]^14 + 28 (I Sin[x])^14 Cos[x]^2 + 91 (I Sin[x])^4 Cos[x]^12 + 91 (I Sin[x])^12 Cos[x]^4) + Exp[-42.0 I y] (14 (I Sin[x])^1 Cos[x]^15 + 14 (I Sin[x])^15 Cos[x]^1 + 182 (I Sin[x])^3 Cos[x]^13 + 182 (I Sin[x])^13 Cos[x]^3 + 364 (I Sin[x])^5 Cos[x]^11 + 364 (I Sin[x])^11 Cos[x]^5) + Exp[-24.0 I y] (91 (I Sin[x])^2 Cos[x]^14 + 91 (I Sin[x])^14 Cos[x]^2 + 728 (I Sin[x])^4 Cos[x]^12 + 728 (I Sin[x])^12 Cos[x]^4 + 1001 (I Sin[x])^6 Cos[x]^10 + 1001 (I Sin[x])^10 Cos[x]^6) + Exp[-10.0 I y] (364 (I Sin[x])^3 Cos[x]^13 + 364 (I Sin[x])^13 Cos[x]^3 + 2002 (I Sin[x])^5 Cos[x]^11 + 2002 (I Sin[x])^11 Cos[x]^5 + 2002 (I Sin[x])^7 Cos[x]^9 + 2002 (I Sin[x])^9 Cos[x]^7) + Exp[-0.0 I y] (1001 (I Sin[x])^4 Cos[x]^12 + 1001 (I Sin[x])^12 Cos[x]^4 + 4004 (I Sin[x])^6 Cos[x]^10 + 4004 (I Sin[x])^10 Cos[x]^6 + 6006 (I Sin[x])^8 Cos[x]^8) + Exp[6.0 I y] (2002 (I Sin[x])^5 Cos[x]^11 + 2002 (I Sin[x])^11 Cos[x]^5 + 9438 (I Sin[x])^7 Cos[x]^9 + 9438 (I Sin[x])^9 Cos[x]^7) + Exp[8.0 I y] (3003 (I Sin[x])^6 Cos[x]^10 + 3003 (I Sin[x])^10 Cos[x]^6 + 6864 (I Sin[x])^8 Cos[x]^8))/Sqrt[2^n])/Sqrt[2^nqubits];
amplitude2[x_,y_] := ((Exp[-120.0 I y] ((I Sin[x])^2 Cos[x]^14 + (I Sin[x])^14 Cos[x]^2) + Exp[-90.0 I y] (2 (I Sin[x])^1 Cos[x]^15 + 2 (I Sin[x])^15 Cos[x]^1 + 14 (I Sin[x])^3 Cos[x]^13 + 14 (I Sin[x])^13 Cos[x]^3) + Exp[-64.0 I y] (Cos[x]^16 + (I Sin[x])^16 + 28 (I Sin[x])^2 Cos[x]^14 + 28 (I Sin[x])^14 Cos[x]^2 + 91 (I Sin[x])^4 Cos[x]^12 + 91 (I Sin[x])^12 Cos[x]^4) + Exp[-42.0 I y] (14 (I Sin[x])^1 Cos[x]^15 + 14 (I Sin[x])^15 Cos[x]^1 + 182 (I Sin[x])^3 Cos[x]^13 + 182 (I Sin[x])^13 Cos[x]^3 + 364 (I Sin[x])^5 Cos[x]^11 + 364 (I Sin[x])^11 Cos[x]^5) + Exp[-24.0 I y] (91 (I Sin[x])^2 Cos[x]^14 + 91 (I Sin[x])^14 Cos[x]^2 + 728 (I Sin[x])^4 Cos[x]^12 + 728 (I Sin[x])^12 Cos[x]^4 + 1001 (I Sin[x])^6 Cos[x]^10 + 1001 (I Sin[x])^10 Cos[x]^6) + Exp[-10.0 I y] (364 (I Sin[x])^3 Cos[x]^13 + 364 (I Sin[x])^13 Cos[x]^3 + 2002 (I Sin[x])^5 Cos[x]^11 + 2002 (I Sin[x])^11 Cos[x]^5 + 2002 (I Sin[x])^7 Cos[x]^9 + 2002 (I Sin[x])^9 Cos[x]^7) + Exp[-0.0 I y] (1001 (I Sin[x])^4 Cos[x]^12 + 1001 (I Sin[x])^12 Cos[x]^4 + 4004 (I Sin[x])^6 Cos[x]^10 + 4004 (I Sin[x])^10 Cos[x]^6 + 6006 (I Sin[x])^8 Cos[x]^8) + Exp[6.0 I y] (2002 (I Sin[x])^5 Cos[x]^11 + 2002 (I Sin[x])^11 Cos[x]^5 + 9438 (I Sin[x])^7 Cos[x]^9 + 9438 (I Sin[x])^9 Cos[x]^7) + Exp[8.0 I y] (3003 (I Sin[x])^6 Cos[x]^10 + 3003 (I Sin[x])^10 Cos[x]^6 + 6864 (I Sin[x])^8 Cos[x]^8))/Sqrt[2^n]);
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = Maximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[label,  ": ", result]

f = probability[c, d]; n = Pi;

