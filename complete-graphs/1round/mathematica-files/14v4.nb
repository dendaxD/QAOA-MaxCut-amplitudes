amplitude[x_,y_] := (Exp[-91.0 I y] (1 (I Sin[x])^4 Cos[x]^10 + 1 (I Sin[x])^10 Cos[x]^4) + Exp[-78.0 I y] (4 (I Sin[x])^3 Cos[x]^11 + 4 (I Sin[x])^11 Cos[x]^3 + 10 (I Sin[x])^5 Cos[x]^9 + 10 (I Sin[x])^9 Cos[x]^5) + Exp[-67.0 I y] (6 (I Sin[x])^2 Cos[x]^12 + 6 (I Sin[x])^12 Cos[x]^2 + 40 (I Sin[x])^4 Cos[x]^10 + 40 (I Sin[x])^10 Cos[x]^4 + 45 (I Sin[x])^6 Cos[x]^8 + 45 (I Sin[x])^8 Cos[x]^6) + Exp[-58.0 I y] (4 (I Sin[x])^1 Cos[x]^13 + 4 (I Sin[x])^13 Cos[x]^1 + 60 (I Sin[x])^3 Cos[x]^11 + 60 (I Sin[x])^11 Cos[x]^3 + 180 (I Sin[x])^5 Cos[x]^9 + 180 (I Sin[x])^9 Cos[x]^5 + 240 (I Sin[x])^7 Cos[x]^7) + Exp[-51.0 I y] (1 Cos[x]^14 + 1 (I Sin[x])^14 + 40 (I Sin[x])^2 Cos[x]^12 + 40 (I Sin[x])^12 Cos[x]^2 + 270 (I Sin[x])^4 Cos[x]^10 + 270 (I Sin[x])^10 Cos[x]^4 + 690 (I Sin[x])^6 Cos[x]^8 + 690 (I Sin[x])^8 Cos[x]^6) + Exp[-46.0 I y] (10 (I Sin[x])^1 Cos[x]^13 + 10 (I Sin[x])^13 Cos[x]^1 + 180 (I Sin[x])^3 Cos[x]^11 + 180 (I Sin[x])^11 Cos[x]^3 + 972 (I Sin[x])^5 Cos[x]^9 + 972 (I Sin[x])^9 Cos[x]^5 + 1680 (I Sin[x])^7 Cos[x]^7) + Exp[-43.0 I y] (45 (I Sin[x])^2 Cos[x]^12 + 45 (I Sin[x])^12 Cos[x]^2 + 690 (I Sin[x])^4 Cos[x]^10 + 690 (I Sin[x])^10 Cos[x]^4 + 2268 (I Sin[x])^6 Cos[x]^8 + 2268 (I Sin[x])^8 Cos[x]^6) + Exp[-42.0 I y] (120 (I Sin[x])^3 Cos[x]^11 + 120 (I Sin[x])^11 Cos[x]^3 + 840 (I Sin[x])^5 Cos[x]^9 + 840 (I Sin[x])^9 Cos[x]^5 + 1512 (I Sin[x])^7 Cos[x]^7))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-91.0 I y] (1 (I Sin[x])^4 Cos[x]^10 + 1 (I Sin[x])^10 Cos[x]^4) + Exp[-78.0 I y] (4 (I Sin[x])^3 Cos[x]^11 + 4 (I Sin[x])^11 Cos[x]^3 + 10 (I Sin[x])^5 Cos[x]^9 + 10 (I Sin[x])^9 Cos[x]^5) + Exp[-67.0 I y] (6 (I Sin[x])^2 Cos[x]^12 + 6 (I Sin[x])^12 Cos[x]^2 + 40 (I Sin[x])^4 Cos[x]^10 + 40 (I Sin[x])^10 Cos[x]^4 + 45 (I Sin[x])^6 Cos[x]^8 + 45 (I Sin[x])^8 Cos[x]^6) + Exp[-58.0 I y] (4 (I Sin[x])^1 Cos[x]^13 + 4 (I Sin[x])^13 Cos[x]^1 + 60 (I Sin[x])^3 Cos[x]^11 + 60 (I Sin[x])^11 Cos[x]^3 + 180 (I Sin[x])^5 Cos[x]^9 + 180 (I Sin[x])^9 Cos[x]^5 + 240 (I Sin[x])^7 Cos[x]^7) + Exp[-51.0 I y] (1 Cos[x]^14 + 1 (I Sin[x])^14 + 40 (I Sin[x])^2 Cos[x]^12 + 40 (I Sin[x])^12 Cos[x]^2 + 270 (I Sin[x])^4 Cos[x]^10 + 270 (I Sin[x])^10 Cos[x]^4 + 690 (I Sin[x])^6 Cos[x]^8 + 690 (I Sin[x])^8 Cos[x]^6) + Exp[-46.0 I y] (10 (I Sin[x])^1 Cos[x]^13 + 10 (I Sin[x])^13 Cos[x]^1 + 180 (I Sin[x])^3 Cos[x]^11 + 180 (I Sin[x])^11 Cos[x]^3 + 972 (I Sin[x])^5 Cos[x]^9 + 972 (I Sin[x])^9 Cos[x]^5 + 1680 (I Sin[x])^7 Cos[x]^7) + Exp[-43.0 I y] (45 (I Sin[x])^2 Cos[x]^12 + 45 (I Sin[x])^12 Cos[x]^2 + 690 (I Sin[x])^4 Cos[x]^10 + 690 (I Sin[x])^10 Cos[x]^4 + 2268 (I Sin[x])^6 Cos[x]^8 + 2268 (I Sin[x])^8 Cos[x]^6) + Exp[-42.0 I y] (120 (I Sin[x])^3 Cos[x]^11 + 120 (I Sin[x])^11 Cos[x]^3 + 840 (I Sin[x])^5 Cos[x]^9 + 840 (I Sin[x])^9 Cos[x]^5 + 1512 (I Sin[x])^7 Cos[x]^7));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = Maximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

