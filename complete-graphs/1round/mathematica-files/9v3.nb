nqubits = 9;
name = "9v3";
nstates = 168;

amplitude[x_,y_] := (Exp[-36.0 I y] (1 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^3) + Exp[-28.0 I y] (3 (I Sin[x])^2 Cos[x]^7 + 3 (I Sin[x])^7 Cos[x]^2 + 6 (I Sin[x])^4 Cos[x]^5 + 6 (I Sin[x])^5 Cos[x]^4) + Exp[-22.0 I y] (3 (I Sin[x])^1 Cos[x]^8 + 3 (I Sin[x])^8 Cos[x]^1 + 18 (I Sin[x])^3 Cos[x]^6 + 18 (I Sin[x])^6 Cos[x]^3 + 15 (I Sin[x])^5 Cos[x]^4 + 15 (I Sin[x])^4 Cos[x]^5) + Exp[-18.0 I y] (1 Cos[x]^9 + 1 (I Sin[x])^9 + 18 (I Sin[x])^2 Cos[x]^7 + 18 (I Sin[x])^7 Cos[x]^2 + 45 (I Sin[x])^4 Cos[x]^5 + 45 (I Sin[x])^5 Cos[x]^4 + 20 (I Sin[x])^6 Cos[x]^3 + 20 (I Sin[x])^3 Cos[x]^6) + Exp[-16.0 I y] (6 (I Sin[x])^1 Cos[x]^8 + 6 (I Sin[x])^8 Cos[x]^1 + 45 (I Sin[x])^3 Cos[x]^6 + 45 (I Sin[x])^6 Cos[x]^3 + 15 (I Sin[x])^2 Cos[x]^7 + 15 (I Sin[x])^7 Cos[x]^2 + 60 (I Sin[x])^5 Cos[x]^4 + 60 (I Sin[x])^4 Cos[x]^5))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-36.0 I y] (1 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^3) + Exp[-28.0 I y] (3 (I Sin[x])^2 Cos[x]^7 + 3 (I Sin[x])^7 Cos[x]^2 + 6 (I Sin[x])^4 Cos[x]^5 + 6 (I Sin[x])^5 Cos[x]^4) + Exp[-22.0 I y] (3 (I Sin[x])^1 Cos[x]^8 + 3 (I Sin[x])^8 Cos[x]^1 + 18 (I Sin[x])^3 Cos[x]^6 + 18 (I Sin[x])^6 Cos[x]^3 + 15 (I Sin[x])^5 Cos[x]^4 + 15 (I Sin[x])^4 Cos[x]^5) + Exp[-18.0 I y] (1 Cos[x]^9 + 1 (I Sin[x])^9 + 18 (I Sin[x])^2 Cos[x]^7 + 18 (I Sin[x])^7 Cos[x]^2 + 45 (I Sin[x])^4 Cos[x]^5 + 45 (I Sin[x])^5 Cos[x]^4 + 20 (I Sin[x])^6 Cos[x]^3 + 20 (I Sin[x])^3 Cos[x]^6) + Exp[-16.0 I y] (6 (I Sin[x])^1 Cos[x]^8 + 6 (I Sin[x])^8 Cos[x]^1 + 45 (I Sin[x])^3 Cos[x]^6 + 45 (I Sin[x])^6 Cos[x]^3 + 15 (I Sin[x])^2 Cos[x]^7 + 15 (I Sin[x])^7 Cos[x]^2 + 60 (I Sin[x])^5 Cos[x]^4 + 60 (I Sin[x])^4 Cos[x]^5));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = Maximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

f = probability[c, d]; n = Pi;

Plot3D[f, {c, 0, n/2}, {d, -n, n}, PlotRange -> All]
ContourPlot[probability[x, y], {x, 0, n/2}, {y, 0, n}, PlotLegends -> Automatic, Contours -> 30]
