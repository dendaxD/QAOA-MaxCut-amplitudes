nqubits = 11;
name = "11v3";
nstates = 330;

amplitude[x_,y_] := (Exp[-55.0 I y] (1 (I Sin[x])^3 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^3) + Exp[-35.0 I y] (3 (I Sin[x])^2 Cos[x]^9 + 3 (I Sin[x])^9 Cos[x]^2 + 8 (I Sin[x])^4 Cos[x]^7 + 8 (I Sin[x])^7 Cos[x]^4) + Exp[-19.0 I y] (3 (I Sin[x])^1 Cos[x]^10 + 3 (I Sin[x])^10 Cos[x]^1 + 24 (I Sin[x])^3 Cos[x]^8 + 24 (I Sin[x])^8 Cos[x]^3 + 28 (I Sin[x])^5 Cos[x]^6 + 28 (I Sin[x])^6 Cos[x]^5) + Exp[-7.0 I y] (1 Cos[x]^11 + 1 (I Sin[x])^11 + 24 (I Sin[x])^2 Cos[x]^9 + 24 (I Sin[x])^9 Cos[x]^2 + 84 (I Sin[x])^4 Cos[x]^7 + 84 (I Sin[x])^7 Cos[x]^4 + 56 (I Sin[x])^6 Cos[x]^5 + 56 (I Sin[x])^5 Cos[x]^6) + Exp[1.0 I y] (8 (I Sin[x])^1 Cos[x]^10 + 8 (I Sin[x])^10 Cos[x]^1 + 84 (I Sin[x])^3 Cos[x]^8 + 84 (I Sin[x])^8 Cos[x]^3 + 168 (I Sin[x])^5 Cos[x]^6 + 168 (I Sin[x])^6 Cos[x]^5 + 70 (I Sin[x])^7 Cos[x]^4 + 70 (I Sin[x])^4 Cos[x]^7) + Exp[5.0 I y] (28 (I Sin[x])^2 Cos[x]^9 + 28 (I Sin[x])^9 Cos[x]^2 + 168 (I Sin[x])^4 Cos[x]^7 + 168 (I Sin[x])^7 Cos[x]^4 + 56 (I Sin[x])^3 Cos[x]^8 + 56 (I Sin[x])^8 Cos[x]^3 + 210 (I Sin[x])^6 Cos[x]^5 + 210 (I Sin[x])^5 Cos[x]^6))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-55.0 I y] (1 (I Sin[x])^3 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^3) + Exp[-35.0 I y] (3 (I Sin[x])^2 Cos[x]^9 + 3 (I Sin[x])^9 Cos[x]^2 + 8 (I Sin[x])^4 Cos[x]^7 + 8 (I Sin[x])^7 Cos[x]^4) + Exp[-19.0 I y] (3 (I Sin[x])^1 Cos[x]^10 + 3 (I Sin[x])^10 Cos[x]^1 + 24 (I Sin[x])^3 Cos[x]^8 + 24 (I Sin[x])^8 Cos[x]^3 + 28 (I Sin[x])^5 Cos[x]^6 + 28 (I Sin[x])^6 Cos[x]^5) + Exp[-7.0 I y] (1 Cos[x]^11 + 1 (I Sin[x])^11 + 24 (I Sin[x])^2 Cos[x]^9 + 24 (I Sin[x])^9 Cos[x]^2 + 84 (I Sin[x])^4 Cos[x]^7 + 84 (I Sin[x])^7 Cos[x]^4 + 56 (I Sin[x])^6 Cos[x]^5 + 56 (I Sin[x])^5 Cos[x]^6) + Exp[1.0 I y] (8 (I Sin[x])^1 Cos[x]^10 + 8 (I Sin[x])^10 Cos[x]^1 + 84 (I Sin[x])^3 Cos[x]^8 + 84 (I Sin[x])^8 Cos[x]^3 + 168 (I Sin[x])^5 Cos[x]^6 + 168 (I Sin[x])^6 Cos[x]^5 + 70 (I Sin[x])^7 Cos[x]^4 + 70 (I Sin[x])^4 Cos[x]^7) + Exp[5.0 I y] (28 (I Sin[x])^2 Cos[x]^9 + 28 (I Sin[x])^9 Cos[x]^2 + 168 (I Sin[x])^4 Cos[x]^7 + 168 (I Sin[x])^7 Cos[x]^4 + 56 (I Sin[x])^3 Cos[x]^8 + 56 (I Sin[x])^8 Cos[x]^3 + 210 (I Sin[x])^6 Cos[x]^5 + 210 (I Sin[x])^5 Cos[x]^6));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = Maximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

f = probability[c, d]; n = Pi;

Plot3D[f, {c, 0, n/2}, {d, -n, n}, PlotRange -> All]
ContourPlot[probability[x, y], {x, 0, n/2}, {y, 0, n}, PlotLegends -> Automatic, Contours -> 30]
