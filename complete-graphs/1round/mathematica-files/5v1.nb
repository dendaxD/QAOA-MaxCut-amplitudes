nqubits = 5;
label = "5v1";
nstates = 10;

amplitude[x_,y_] := (Exp[-10.0 I y] ((I Sin[x])^1 Cos[x]^4 + (I Sin[x])^4 Cos[x]^1) + Exp[-2.0 I y] (Cos[x]^5 + (I Sin[x])^5 + 4 (I Sin[x])^2 Cos[x]^3 + 4 (I Sin[x])^3 Cos[x]^2) + Exp[2.0 I y] (4 (I Sin[x])^1 Cos[x]^4 + 4 (I Sin[x])^4 Cos[x]^1 + 6 (I Sin[x])^3 Cos[x]^2 + 6 (I Sin[x])^2 Cos[x]^3))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-10.0 I y] ((I Sin[x])^1 Cos[x]^4 + (I Sin[x])^4 Cos[x]^1) + Exp[-2.0 I y] (Cos[x]^5 + (I Sin[x])^5 + 4 (I Sin[x])^2 Cos[x]^3 + 4 (I Sin[x])^3 Cos[x]^2) + Exp[2.0 I y] (4 (I Sin[x])^1 Cos[x]^4 + 4 (I Sin[x])^4 Cos[x]^1 + 6 (I Sin[x])^3 Cos[x]^2 + 6 (I Sin[x])^2 Cos[x]^3));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = Maximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[label,  ": ", result]

f = probability[c, d]; n = Pi;

Plot3D[f, {c, 0, n/2}, {d, -n, n}, PlotRange -> All]
ContourPlot[probability[x, y], {x, 0, n/2}, {y, 0, n}, PlotLegends -> Automatic, Contours -> 30]
