nqubits = 15;
name = "15v2";
nstates = 210;

amplitude[x_,y_] := (Exp[-105.0 I y] (1 (I Sin[x])^2 Cos[x]^13 + 1 (I Sin[x])^13 Cos[x]^2) + Exp[-91.0 I y] (2 (I Sin[x])^1 Cos[x]^14 + 2 (I Sin[x])^14 Cos[x]^1 + 13 (I Sin[x])^3 Cos[x]^12 + 13 (I Sin[x])^12 Cos[x]^3) + Exp[-79.0 I y] (1 Cos[x]^15 + 1 (I Sin[x])^15 + 26 (I Sin[x])^2 Cos[x]^13 + 26 (I Sin[x])^13 Cos[x]^2 + 78 (I Sin[x])^4 Cos[x]^11 + 78 (I Sin[x])^11 Cos[x]^4) + Exp[-69.0 I y] (13 (I Sin[x])^1 Cos[x]^14 + 13 (I Sin[x])^14 Cos[x]^1 + 156 (I Sin[x])^3 Cos[x]^12 + 156 (I Sin[x])^12 Cos[x]^3 + 286 (I Sin[x])^5 Cos[x]^10 + 286 (I Sin[x])^10 Cos[x]^5) + Exp[-61.0 I y] (78 (I Sin[x])^2 Cos[x]^13 + 78 (I Sin[x])^13 Cos[x]^2 + 572 (I Sin[x])^4 Cos[x]^11 + 572 (I Sin[x])^11 Cos[x]^4 + 715 (I Sin[x])^6 Cos[x]^9 + 715 (I Sin[x])^9 Cos[x]^6) + Exp[-55.0 I y] (286 (I Sin[x])^3 Cos[x]^12 + 286 (I Sin[x])^12 Cos[x]^3 + 1430 (I Sin[x])^5 Cos[x]^10 + 1430 (I Sin[x])^10 Cos[x]^5 + 1287 (I Sin[x])^7 Cos[x]^8 + 1287 (I Sin[x])^8 Cos[x]^7) + Exp[-51.0 I y] (715 (I Sin[x])^4 Cos[x]^11 + 715 (I Sin[x])^11 Cos[x]^4 + 2574 (I Sin[x])^6 Cos[x]^9 + 2574 (I Sin[x])^9 Cos[x]^6 + 1716 (I Sin[x])^8 Cos[x]^7 + 1716 (I Sin[x])^7 Cos[x]^8) + Exp[-49.0 I y] (1287 (I Sin[x])^5 Cos[x]^10 + 1287 (I Sin[x])^10 Cos[x]^5 + 3432 (I Sin[x])^7 Cos[x]^8 + 3432 (I Sin[x])^8 Cos[x]^7 + 1716 (I Sin[x])^6 Cos[x]^9 + 1716 (I Sin[x])^9 Cos[x]^6))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-105.0 I y] (1 (I Sin[x])^2 Cos[x]^13 + 1 (I Sin[x])^13 Cos[x]^2) + Exp[-91.0 I y] (2 (I Sin[x])^1 Cos[x]^14 + 2 (I Sin[x])^14 Cos[x]^1 + 13 (I Sin[x])^3 Cos[x]^12 + 13 (I Sin[x])^12 Cos[x]^3) + Exp[-79.0 I y] (1 Cos[x]^15 + 1 (I Sin[x])^15 + 26 (I Sin[x])^2 Cos[x]^13 + 26 (I Sin[x])^13 Cos[x]^2 + 78 (I Sin[x])^4 Cos[x]^11 + 78 (I Sin[x])^11 Cos[x]^4) + Exp[-69.0 I y] (13 (I Sin[x])^1 Cos[x]^14 + 13 (I Sin[x])^14 Cos[x]^1 + 156 (I Sin[x])^3 Cos[x]^12 + 156 (I Sin[x])^12 Cos[x]^3 + 286 (I Sin[x])^5 Cos[x]^10 + 286 (I Sin[x])^10 Cos[x]^5) + Exp[-61.0 I y] (78 (I Sin[x])^2 Cos[x]^13 + 78 (I Sin[x])^13 Cos[x]^2 + 572 (I Sin[x])^4 Cos[x]^11 + 572 (I Sin[x])^11 Cos[x]^4 + 715 (I Sin[x])^6 Cos[x]^9 + 715 (I Sin[x])^9 Cos[x]^6) + Exp[-55.0 I y] (286 (I Sin[x])^3 Cos[x]^12 + 286 (I Sin[x])^12 Cos[x]^3 + 1430 (I Sin[x])^5 Cos[x]^10 + 1430 (I Sin[x])^10 Cos[x]^5 + 1287 (I Sin[x])^7 Cos[x]^8 + 1287 (I Sin[x])^8 Cos[x]^7) + Exp[-51.0 I y] (715 (I Sin[x])^4 Cos[x]^11 + 715 (I Sin[x])^11 Cos[x]^4 + 2574 (I Sin[x])^6 Cos[x]^9 + 2574 (I Sin[x])^9 Cos[x]^6 + 1716 (I Sin[x])^8 Cos[x]^7 + 1716 (I Sin[x])^7 Cos[x]^8) + Exp[-49.0 I y] (1287 (I Sin[x])^5 Cos[x]^10 + 1287 (I Sin[x])^10 Cos[x]^5 + 3432 (I Sin[x])^7 Cos[x]^8 + 3432 (I Sin[x])^8 Cos[x]^7 + 1716 (I Sin[x])^6 Cos[x]^9 + 1716 (I Sin[x])^9 Cos[x]^6));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = Maximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

