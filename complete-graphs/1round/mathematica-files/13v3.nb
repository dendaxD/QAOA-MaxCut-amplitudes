amplitude[x_,y_] := (Exp[-78.0 I y] (1 (I Sin[x])^3 Cos[x]^10 + 1 (I Sin[x])^10 Cos[x]^3) + Exp[-66.0 I y] (3 (I Sin[x])^2 Cos[x]^11 + 3 (I Sin[x])^11 Cos[x]^2 + 10 (I Sin[x])^4 Cos[x]^9 + 10 (I Sin[x])^9 Cos[x]^4) + Exp[-56.0 I y] (3 (I Sin[x])^1 Cos[x]^12 + 3 (I Sin[x])^12 Cos[x]^1 + 30 (I Sin[x])^3 Cos[x]^10 + 30 (I Sin[x])^10 Cos[x]^3 + 45 (I Sin[x])^5 Cos[x]^8 + 45 (I Sin[x])^8 Cos[x]^5) + Exp[-48.0 I y] (1 Cos[x]^13 + 1 (I Sin[x])^13 + 30 (I Sin[x])^2 Cos[x]^11 + 30 (I Sin[x])^11 Cos[x]^2 + 135 (I Sin[x])^4 Cos[x]^9 + 135 (I Sin[x])^9 Cos[x]^4 + 120 (I Sin[x])^6 Cos[x]^7 + 120 (I Sin[x])^7 Cos[x]^6) + Exp[-42.0 I y] (10 (I Sin[x])^1 Cos[x]^12 + 10 (I Sin[x])^12 Cos[x]^1 + 135 (I Sin[x])^3 Cos[x]^10 + 135 (I Sin[x])^10 Cos[x]^3 + 360 (I Sin[x])^5 Cos[x]^8 + 360 (I Sin[x])^8 Cos[x]^5 + 210 (I Sin[x])^7 Cos[x]^6 + 210 (I Sin[x])^6 Cos[x]^7) + Exp[-38.0 I y] (45 (I Sin[x])^2 Cos[x]^11 + 45 (I Sin[x])^11 Cos[x]^2 + 360 (I Sin[x])^4 Cos[x]^9 + 360 (I Sin[x])^9 Cos[x]^4 + 630 (I Sin[x])^6 Cos[x]^7 + 630 (I Sin[x])^7 Cos[x]^6 + 252 (I Sin[x])^8 Cos[x]^5 + 252 (I Sin[x])^5 Cos[x]^8) + Exp[-36.0 I y] (120 (I Sin[x])^3 Cos[x]^10 + 120 (I Sin[x])^10 Cos[x]^3 + 630 (I Sin[x])^5 Cos[x]^8 + 630 (I Sin[x])^8 Cos[x]^5 + 210 (I Sin[x])^4 Cos[x]^9 + 210 (I Sin[x])^9 Cos[x]^4 + 756 (I Sin[x])^7 Cos[x]^6 + 756 (I Sin[x])^6 Cos[x]^7))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-78.0 I y] (1 (I Sin[x])^3 Cos[x]^10 + 1 (I Sin[x])^10 Cos[x]^3) + Exp[-66.0 I y] (3 (I Sin[x])^2 Cos[x]^11 + 3 (I Sin[x])^11 Cos[x]^2 + 10 (I Sin[x])^4 Cos[x]^9 + 10 (I Sin[x])^9 Cos[x]^4) + Exp[-56.0 I y] (3 (I Sin[x])^1 Cos[x]^12 + 3 (I Sin[x])^12 Cos[x]^1 + 30 (I Sin[x])^3 Cos[x]^10 + 30 (I Sin[x])^10 Cos[x]^3 + 45 (I Sin[x])^5 Cos[x]^8 + 45 (I Sin[x])^8 Cos[x]^5) + Exp[-48.0 I y] (1 Cos[x]^13 + 1 (I Sin[x])^13 + 30 (I Sin[x])^2 Cos[x]^11 + 30 (I Sin[x])^11 Cos[x]^2 + 135 (I Sin[x])^4 Cos[x]^9 + 135 (I Sin[x])^9 Cos[x]^4 + 120 (I Sin[x])^6 Cos[x]^7 + 120 (I Sin[x])^7 Cos[x]^6) + Exp[-42.0 I y] (10 (I Sin[x])^1 Cos[x]^12 + 10 (I Sin[x])^12 Cos[x]^1 + 135 (I Sin[x])^3 Cos[x]^10 + 135 (I Sin[x])^10 Cos[x]^3 + 360 (I Sin[x])^5 Cos[x]^8 + 360 (I Sin[x])^8 Cos[x]^5 + 210 (I Sin[x])^7 Cos[x]^6 + 210 (I Sin[x])^6 Cos[x]^7) + Exp[-38.0 I y] (45 (I Sin[x])^2 Cos[x]^11 + 45 (I Sin[x])^11 Cos[x]^2 + 360 (I Sin[x])^4 Cos[x]^9 + 360 (I Sin[x])^9 Cos[x]^4 + 630 (I Sin[x])^6 Cos[x]^7 + 630 (I Sin[x])^7 Cos[x]^6 + 252 (I Sin[x])^8 Cos[x]^5 + 252 (I Sin[x])^5 Cos[x]^8) + Exp[-36.0 I y] (120 (I Sin[x])^3 Cos[x]^10 + 120 (I Sin[x])^10 Cos[x]^3 + 630 (I Sin[x])^5 Cos[x]^8 + 630 (I Sin[x])^8 Cos[x]^5 + 210 (I Sin[x])^4 Cos[x]^9 + 210 (I Sin[x])^9 Cos[x]^4 + 756 (I Sin[x])^7 Cos[x]^6 + 756 (I Sin[x])^6 Cos[x]^7));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = Maximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

