nqubits = 9;
name = "9v2";
nstates = 72;

amplitude[x_,y_] := (Exp[-36.0 I y] (1 (I Sin[x])^2 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^2) + Exp[-28.0 I y] (2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 7 (I Sin[x])^3 Cos[x]^6 + 7 (I Sin[x])^6 Cos[x]^3) + Exp[-22.0 I y] (1 Cos[x]^9 + 1 (I Sin[x])^9 + 14 (I Sin[x])^2 Cos[x]^7 + 14 (I Sin[x])^7 Cos[x]^2 + 21 (I Sin[x])^4 Cos[x]^5 + 21 (I Sin[x])^5 Cos[x]^4) + Exp[-18.0 I y] (7 (I Sin[x])^1 Cos[x]^8 + 7 (I Sin[x])^8 Cos[x]^1 + 42 (I Sin[x])^3 Cos[x]^6 + 42 (I Sin[x])^6 Cos[x]^3 + 35 (I Sin[x])^5 Cos[x]^4 + 35 (I Sin[x])^4 Cos[x]^5) + Exp[-16.0 I y] (21 (I Sin[x])^2 Cos[x]^7 + 21 (I Sin[x])^7 Cos[x]^2 + 70 (I Sin[x])^4 Cos[x]^5 + 70 (I Sin[x])^5 Cos[x]^4 + 35 (I Sin[x])^3 Cos[x]^6 + 35 (I Sin[x])^6 Cos[x]^3))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-36.0 I y] (1 (I Sin[x])^2 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^2) + Exp[-28.0 I y] (2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 7 (I Sin[x])^3 Cos[x]^6 + 7 (I Sin[x])^6 Cos[x]^3) + Exp[-22.0 I y] (1 Cos[x]^9 + 1 (I Sin[x])^9 + 14 (I Sin[x])^2 Cos[x]^7 + 14 (I Sin[x])^7 Cos[x]^2 + 21 (I Sin[x])^4 Cos[x]^5 + 21 (I Sin[x])^5 Cos[x]^4) + Exp[-18.0 I y] (7 (I Sin[x])^1 Cos[x]^8 + 7 (I Sin[x])^8 Cos[x]^1 + 42 (I Sin[x])^3 Cos[x]^6 + 42 (I Sin[x])^6 Cos[x]^3 + 35 (I Sin[x])^5 Cos[x]^4 + 35 (I Sin[x])^4 Cos[x]^5) + Exp[-16.0 I y] (21 (I Sin[x])^2 Cos[x]^7 + 21 (I Sin[x])^7 Cos[x]^2 + 70 (I Sin[x])^4 Cos[x]^5 + 70 (I Sin[x])^5 Cos[x]^4 + 35 (I Sin[x])^3 Cos[x]^6 + 35 (I Sin[x])^6 Cos[x]^3));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = Maximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

f = probability[c, d]; n = Pi;

Plot3D[f, {c, 0, n/2}, {d, -n, n}, PlotRange -> All]
ContourPlot[probability[x, y], {x, 0, n/2}, {y, 0, n}, PlotLegends -> Automatic, Contours -> 30]
