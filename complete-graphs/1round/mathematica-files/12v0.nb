nqubits = 12;
name = "12v0";
nstates = 2;

amplitude[x_,y_] := (Exp[-66.0 I y] (1 Cos[x]^12 + 1 (I Sin[x])^12) + Exp[-55.0 I y] (12 (I Sin[x])^1 Cos[x]^11 + 12 (I Sin[x])^11 Cos[x]^1) + Exp[-46.0 I y] (66 (I Sin[x])^2 Cos[x]^10 + 66 (I Sin[x])^10 Cos[x]^2) + Exp[-39.0 I y] (220 (I Sin[x])^3 Cos[x]^9 + 220 (I Sin[x])^9 Cos[x]^3) + Exp[-34.0 I y] (495 (I Sin[x])^4 Cos[x]^8 + 495 (I Sin[x])^8 Cos[x]^4) + Exp[-31.0 I y] (792 (I Sin[x])^5 Cos[x]^7 + 792 (I Sin[x])^7 Cos[x]^5) + Exp[-30.0 I y] (924 (I Sin[x])^6 Cos[x]^6))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-66.0 I y] (1 Cos[x]^12 + 1 (I Sin[x])^12) + Exp[-55.0 I y] (12 (I Sin[x])^1 Cos[x]^11 + 12 (I Sin[x])^11 Cos[x]^1) + Exp[-46.0 I y] (66 (I Sin[x])^2 Cos[x]^10 + 66 (I Sin[x])^10 Cos[x]^2) + Exp[-39.0 I y] (220 (I Sin[x])^3 Cos[x]^9 + 220 (I Sin[x])^9 Cos[x]^3) + Exp[-34.0 I y] (495 (I Sin[x])^4 Cos[x]^8 + 495 (I Sin[x])^8 Cos[x]^4) + Exp[-31.0 I y] (792 (I Sin[x])^5 Cos[x]^7 + 792 (I Sin[x])^7 Cos[x]^5) + Exp[-30.0 I y] (924 (I Sin[x])^6 Cos[x]^6));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = Maximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

f = probability[c, d]; n = Pi;

Plot3D[f, {c, 0, n/2}, {d, -n, n}, PlotRange -> All]
ContourPlot[probability[x, y], {x, 0, n/2}, {y, 0, n}, PlotLegends -> Automatic, Contours -> 30]
