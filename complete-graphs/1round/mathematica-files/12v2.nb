nqubits = 12;
name = "12v2";
nstates = 132;

amplitude[x_,y_] := (Exp[-66.0 I y] (1 (I Sin[x])^2 Cos[x]^10 + 1 (I Sin[x])^10 Cos[x]^2) + Exp[-55.0 I y] (2 (I Sin[x])^1 Cos[x]^11 + 2 (I Sin[x])^11 Cos[x]^1 + 10 (I Sin[x])^3 Cos[x]^9 + 10 (I Sin[x])^9 Cos[x]^3) + Exp[-46.0 I y] (1 Cos[x]^12 + 1 (I Sin[x])^12 + 20 (I Sin[x])^2 Cos[x]^10 + 20 (I Sin[x])^10 Cos[x]^2 + 45 (I Sin[x])^4 Cos[x]^8 + 45 (I Sin[x])^8 Cos[x]^4) + Exp[-39.0 I y] (10 (I Sin[x])^1 Cos[x]^11 + 10 (I Sin[x])^11 Cos[x]^1 + 90 (I Sin[x])^3 Cos[x]^9 + 90 (I Sin[x])^9 Cos[x]^3 + 120 (I Sin[x])^5 Cos[x]^7 + 120 (I Sin[x])^7 Cos[x]^5) + Exp[-34.0 I y] (45 (I Sin[x])^2 Cos[x]^10 + 45 (I Sin[x])^10 Cos[x]^2 + 240 (I Sin[x])^4 Cos[x]^8 + 240 (I Sin[x])^8 Cos[x]^4 + 420 (I Sin[x])^6 Cos[x]^6) + Exp[-31.0 I y] (120 (I Sin[x])^3 Cos[x]^9 + 120 (I Sin[x])^9 Cos[x]^3 + 672 (I Sin[x])^5 Cos[x]^7 + 672 (I Sin[x])^7 Cos[x]^5) + Exp[-30.0 I y] (210 (I Sin[x])^4 Cos[x]^8 + 210 (I Sin[x])^8 Cos[x]^4 + 504 (I Sin[x])^6 Cos[x]^6))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-66.0 I y] (1 (I Sin[x])^2 Cos[x]^10 + 1 (I Sin[x])^10 Cos[x]^2) + Exp[-55.0 I y] (2 (I Sin[x])^1 Cos[x]^11 + 2 (I Sin[x])^11 Cos[x]^1 + 10 (I Sin[x])^3 Cos[x]^9 + 10 (I Sin[x])^9 Cos[x]^3) + Exp[-46.0 I y] (1 Cos[x]^12 + 1 (I Sin[x])^12 + 20 (I Sin[x])^2 Cos[x]^10 + 20 (I Sin[x])^10 Cos[x]^2 + 45 (I Sin[x])^4 Cos[x]^8 + 45 (I Sin[x])^8 Cos[x]^4) + Exp[-39.0 I y] (10 (I Sin[x])^1 Cos[x]^11 + 10 (I Sin[x])^11 Cos[x]^1 + 90 (I Sin[x])^3 Cos[x]^9 + 90 (I Sin[x])^9 Cos[x]^3 + 120 (I Sin[x])^5 Cos[x]^7 + 120 (I Sin[x])^7 Cos[x]^5) + Exp[-34.0 I y] (45 (I Sin[x])^2 Cos[x]^10 + 45 (I Sin[x])^10 Cos[x]^2 + 240 (I Sin[x])^4 Cos[x]^8 + 240 (I Sin[x])^8 Cos[x]^4 + 420 (I Sin[x])^6 Cos[x]^6) + Exp[-31.0 I y] (120 (I Sin[x])^3 Cos[x]^9 + 120 (I Sin[x])^9 Cos[x]^3 + 672 (I Sin[x])^5 Cos[x]^7 + 672 (I Sin[x])^7 Cos[x]^5) + Exp[-30.0 I y] (210 (I Sin[x])^4 Cos[x]^8 + 210 (I Sin[x])^8 Cos[x]^4 + 504 (I Sin[x])^6 Cos[x]^6));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = Maximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

f = probability[c, d]; n = Pi;

Plot3D[f, {c, 0, n/2}, {d, -n, n}, PlotRange -> All]
ContourPlot[probability[x, y], {x, 0, n/2}, {y, 0, n}, PlotLegends -> Automatic, Contours -> 30]
