amplitude2[x_,y_] := ((Exp[-120.0 I y] ((I Sin[x])^7 Cos[x]^9 + (I Sin[x])^9 Cos[x]^7) + Exp[-90.0 I y] (7 (I Sin[x])^6 Cos[x]^10 + 7 (I Sin[x])^10 Cos[x]^6 + 18 (I Sin[x])^8 Cos[x]^8) + Exp[-64.0 I y] (21 (I Sin[x])^5 Cos[x]^11 + 21 (I Sin[x])^11 Cos[x]^5 + 99 (I Sin[x])^7 Cos[x]^9 + 99 (I Sin[x])^9 Cos[x]^7) + Exp[-42.0 I y] (35 (I Sin[x])^4 Cos[x]^12 + 35 (I Sin[x])^12 Cos[x]^4 + 273 (I Sin[x])^6 Cos[x]^10 + 273 (I Sin[x])^10 Cos[x]^6 + 504 (I Sin[x])^8 Cos[x]^8) + Exp[-24.0 I y] (35 (I Sin[x])^3 Cos[x]^13 + 35 (I Sin[x])^13 Cos[x]^3 + 441 (I Sin[x])^5 Cos[x]^11 + 441 (I Sin[x])^11 Cos[x]^5 + 1344 (I Sin[x])^7 Cos[x]^9 + 1344 (I Sin[x])^9 Cos[x]^7) + Exp[-10.0 I y] (21 (I Sin[x])^2 Cos[x]^14 + 21 (I Sin[x])^14 Cos[x]^2 + 441 (I Sin[x])^4 Cos[x]^12 + 441 (I Sin[x])^12 Cos[x]^4 + 2142 (I Sin[x])^6 Cos[x]^10 + 2142 (I Sin[x])^10 Cos[x]^6 + 3528 (I Sin[x])^8 Cos[x]^8) + Exp[-0.0 I y] (7 (I Sin[x])^1 Cos[x]^15 + 7 (I Sin[x])^15 Cos[x]^1 + 273 (I Sin[x])^3 Cos[x]^13 + 273 (I Sin[x])^13 Cos[x]^3 + 2142 (I Sin[x])^5 Cos[x]^11 + 2142 (I Sin[x])^11 Cos[x]^5 + 5586 (I Sin[x])^7 Cos[x]^9 + 5586 (I Sin[x])^9 Cos[x]^7) + Exp[6.0 I y] (Cos[x]^16 + (I Sin[x])^16 + 99 (I Sin[x])^2 Cos[x]^14 + 99 (I Sin[x])^14 Cos[x]^2 + 1344 (I Sin[x])^4 Cos[x]^12 + 1344 (I Sin[x])^12 Cos[x]^4 + 5586 (I Sin[x])^6 Cos[x]^10 + 5586 (I Sin[x])^10 Cos[x]^6 + 8820 (I Sin[x])^8 Cos[x]^8) + Exp[8.0 I y] (9 (I Sin[x])^1 Cos[x]^15 + 9 (I Sin[x])^15 Cos[x]^1 + 252 (I Sin[x])^3 Cos[x]^13 + 252 (I Sin[x])^13 Cos[x]^3 + 1764 (I Sin[x])^5 Cos[x]^11 + 1764 (I Sin[x])^11 Cos[x]^5 + 4410 (I Sin[x])^7 Cos[x]^9 + 4410 (I Sin[x])^9 Cos[x]^7))/Sqrt[2^n]);
