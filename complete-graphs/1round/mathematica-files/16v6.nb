nqubits = 16;
name = "16v6";
nstates = 16016;

amplitude[x_,y_] := (Exp[-120.0 I y] (1 (I Sin[x])^6 Cos[x]^10 + 1 (I Sin[x])^10 Cos[x]^6) + Exp[-105.0 I y] (6 (I Sin[x])^5 Cos[x]^11 + 6 (I Sin[x])^11 Cos[x]^5 + 10 (I Sin[x])^7 Cos[x]^9 + 10 (I Sin[x])^9 Cos[x]^7) + Exp[-92.0 I y] (15 (I Sin[x])^4 Cos[x]^12 + 15 (I Sin[x])^12 Cos[x]^4 + 60 (I Sin[x])^6 Cos[x]^10 + 60 (I Sin[x])^10 Cos[x]^6 + 90 (I Sin[x])^8 Cos[x]^8) + Exp[-81.0 I y] (20 (I Sin[x])^3 Cos[x]^13 + 20 (I Sin[x])^13 Cos[x]^3 + 150 (I Sin[x])^5 Cos[x]^11 + 150 (I Sin[x])^11 Cos[x]^5 + 390 (I Sin[x])^7 Cos[x]^9 + 390 (I Sin[x])^9 Cos[x]^7) + Exp[-72.0 I y] (15 (I Sin[x])^2 Cos[x]^14 + 15 (I Sin[x])^14 Cos[x]^2 + 200 (I Sin[x])^4 Cos[x]^12 + 200 (I Sin[x])^12 Cos[x]^4 + 885 (I Sin[x])^6 Cos[x]^10 + 885 (I Sin[x])^10 Cos[x]^6 + 1440 (I Sin[x])^8 Cos[x]^8) + Exp[-65.0 I y] (6 (I Sin[x])^1 Cos[x]^15 + 6 (I Sin[x])^15 Cos[x]^1 + 150 (I Sin[x])^3 Cos[x]^13 + 150 (I Sin[x])^13 Cos[x]^3 + 1152 (I Sin[x])^5 Cos[x]^11 + 1152 (I Sin[x])^11 Cos[x]^5 + 3060 (I Sin[x])^7 Cos[x]^9 + 3060 (I Sin[x])^9 Cos[x]^7) + Exp[-60.0 I y] (1 Cos[x]^16 + 1 (I Sin[x])^16 + 60 (I Sin[x])^2 Cos[x]^14 + 60 (I Sin[x])^14 Cos[x]^2 + 885 (I Sin[x])^4 Cos[x]^12 + 885 (I Sin[x])^12 Cos[x]^4 + 3912 (I Sin[x])^6 Cos[x]^10 + 3912 (I Sin[x])^10 Cos[x]^6 + 6300 (I Sin[x])^8 Cos[x]^8) + Exp[-57.0 I y] (10 (I Sin[x])^1 Cos[x]^15 + 10 (I Sin[x])^15 Cos[x]^1 + 390 (I Sin[x])^3 Cos[x]^13 + 390 (I Sin[x])^13 Cos[x]^3 + 3060 (I Sin[x])^5 Cos[x]^11 + 3060 (I Sin[x])^11 Cos[x]^5 + 7980 (I Sin[x])^7 Cos[x]^9 + 7980 (I Sin[x])^9 Cos[x]^7) + Exp[-56.0 I y] (45 (I Sin[x])^2 Cos[x]^14 + 45 (I Sin[x])^14 Cos[x]^2 + 720 (I Sin[x])^4 Cos[x]^12 + 720 (I Sin[x])^12 Cos[x]^4 + 3150 (I Sin[x])^6 Cos[x]^10 + 3150 (I Sin[x])^10 Cos[x]^6 + 5040 (I Sin[x])^8 Cos[x]^8))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-120.0 I y] (1 (I Sin[x])^6 Cos[x]^10 + 1 (I Sin[x])^10 Cos[x]^6) + Exp[-105.0 I y] (6 (I Sin[x])^5 Cos[x]^11 + 6 (I Sin[x])^11 Cos[x]^5 + 10 (I Sin[x])^7 Cos[x]^9 + 10 (I Sin[x])^9 Cos[x]^7) + Exp[-92.0 I y] (15 (I Sin[x])^4 Cos[x]^12 + 15 (I Sin[x])^12 Cos[x]^4 + 60 (I Sin[x])^6 Cos[x]^10 + 60 (I Sin[x])^10 Cos[x]^6 + 90 (I Sin[x])^8 Cos[x]^8) + Exp[-81.0 I y] (20 (I Sin[x])^3 Cos[x]^13 + 20 (I Sin[x])^13 Cos[x]^3 + 150 (I Sin[x])^5 Cos[x]^11 + 150 (I Sin[x])^11 Cos[x]^5 + 390 (I Sin[x])^7 Cos[x]^9 + 390 (I Sin[x])^9 Cos[x]^7) + Exp[-72.0 I y] (15 (I Sin[x])^2 Cos[x]^14 + 15 (I Sin[x])^14 Cos[x]^2 + 200 (I Sin[x])^4 Cos[x]^12 + 200 (I Sin[x])^12 Cos[x]^4 + 885 (I Sin[x])^6 Cos[x]^10 + 885 (I Sin[x])^10 Cos[x]^6 + 1440 (I Sin[x])^8 Cos[x]^8) + Exp[-65.0 I y] (6 (I Sin[x])^1 Cos[x]^15 + 6 (I Sin[x])^15 Cos[x]^1 + 150 (I Sin[x])^3 Cos[x]^13 + 150 (I Sin[x])^13 Cos[x]^3 + 1152 (I Sin[x])^5 Cos[x]^11 + 1152 (I Sin[x])^11 Cos[x]^5 + 3060 (I Sin[x])^7 Cos[x]^9 + 3060 (I Sin[x])^9 Cos[x]^7) + Exp[-60.0 I y] (1 Cos[x]^16 + 1 (I Sin[x])^16 + 60 (I Sin[x])^2 Cos[x]^14 + 60 (I Sin[x])^14 Cos[x]^2 + 885 (I Sin[x])^4 Cos[x]^12 + 885 (I Sin[x])^12 Cos[x]^4 + 3912 (I Sin[x])^6 Cos[x]^10 + 3912 (I Sin[x])^10 Cos[x]^6 + 6300 (I Sin[x])^8 Cos[x]^8) + Exp[-57.0 I y] (10 (I Sin[x])^1 Cos[x]^15 + 10 (I Sin[x])^15 Cos[x]^1 + 390 (I Sin[x])^3 Cos[x]^13 + 390 (I Sin[x])^13 Cos[x]^3 + 3060 (I Sin[x])^5 Cos[x]^11 + 3060 (I Sin[x])^11 Cos[x]^5 + 7980 (I Sin[x])^7 Cos[x]^9 + 7980 (I Sin[x])^9 Cos[x]^7) + Exp[-56.0 I y] (45 (I Sin[x])^2 Cos[x]^14 + 45 (I Sin[x])^14 Cos[x]^2 + 720 (I Sin[x])^4 Cos[x]^12 + 720 (I Sin[x])^12 Cos[x]^4 + 3150 (I Sin[x])^6 Cos[x]^10 + 3150 (I Sin[x])^10 Cos[x]^6 + 5040 (I Sin[x])^8 Cos[x]^8));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = Maximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

