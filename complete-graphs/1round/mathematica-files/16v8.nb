amplitude[x_,y_] := ((Exp[-120.0 I y] (2 (I Sin[x])^8 Cos[x]^8) + Exp[-90.0 I y] (16 (I Sin[x])^7 Cos[x]^9 + 16 (I Sin[x])^9 Cos[x]^7) + Exp[-64.0 I y] (56 (I Sin[x])^6 Cos[x]^10 + 56 (I Sin[x])^10 Cos[x]^6 + 128 (I Sin[x])^8 Cos[x]^8) + Exp[-42.0 I y] (112 (I Sin[x])^5 Cos[x]^11 + 112 (I Sin[x])^11 Cos[x]^5 + 448 (I Sin[x])^7 Cos[x]^9 + 448 (I Sin[x])^9 Cos[x]^7) + Exp[-24.0 I y] (140 (I Sin[x])^4 Cos[x]^12 + 140 (I Sin[x])^12 Cos[x]^4 + 896 (I Sin[x])^6 Cos[x]^10 + 896 (I Sin[x])^10 Cos[x]^6 + 1568 (I Sin[x])^8 Cos[x]^8) + Exp[-10.0 I y] (112 (I Sin[x])^3 Cos[x]^13 + 112 (I Sin[x])^13 Cos[x]^3 + 1120 (I Sin[x])^5 Cos[x]^11 + 1120 (I Sin[x])^11 Cos[x]^5 + 3136 (I Sin[x])^7 Cos[x]^9 + 3136 (I Sin[x])^9 Cos[x]^7) + Exp[-0.0 I y] (56 (I Sin[x])^2 Cos[x]^14 + 56 (I Sin[x])^14 Cos[x]^2 + 896 (I Sin[x])^4 Cos[x]^12 + 896 (I Sin[x])^12 Cos[x]^4 + 3920 (I Sin[x])^6 Cos[x]^10 + 3920 (I Sin[x])^10 Cos[x]^6 + 6272 (I Sin[x])^8 Cos[x]^8) + Exp[6.0 I y] (16 (I Sin[x])^1 Cos[x]^15 + 16 (I Sin[x])^15 Cos[x]^1 + 448 (I Sin[x])^3 Cos[x]^13 + 448 (I Sin[x])^13 Cos[x]^3 + 3136 (I Sin[x])^5 Cos[x]^11 + 3136 (I Sin[x])^11 Cos[x]^5 + 7840 (I Sin[x])^7 Cos[x]^9 + 7840 (I Sin[x])^9 Cos[x]^7) + Exp[8.0 I y] (Cos[x]^16 + (I Sin[x])^16 + 64 (I Sin[x])^2 Cos[x]^14 + 64 (I Sin[x])^14 Cos[x]^2 + 784 (I Sin[x])^4 Cos[x]^12 + 784 (I Sin[x])^12 Cos[x]^4 + 3136 (I Sin[x])^6 Cos[x]^10 + 3136 (I Sin[x])^10 Cos[x]^6 + 4900 (I Sin[x])^8 Cos[x]^8))/Sqrt[2^n])/Sqrt[2^nqubits];
amplitude2[x_,y_] := ((Exp[-120.0 I y] (2 (I Sin[x])^8 Cos[x]^8) + Exp[-90.0 I y] (16 (I Sin[x])^7 Cos[x]^9 + 16 (I Sin[x])^9 Cos[x]^7) + Exp[-64.0 I y] (56 (I Sin[x])^6 Cos[x]^10 + 56 (I Sin[x])^10 Cos[x]^6 + 128 (I Sin[x])^8 Cos[x]^8) + Exp[-42.0 I y] (112 (I Sin[x])^5 Cos[x]^11 + 112 (I Sin[x])^11 Cos[x]^5 + 448 (I Sin[x])^7 Cos[x]^9 + 448 (I Sin[x])^9 Cos[x]^7) + Exp[-24.0 I y] (140 (I Sin[x])^4 Cos[x]^12 + 140 (I Sin[x])^12 Cos[x]^4 + 896 (I Sin[x])^6 Cos[x]^10 + 896 (I Sin[x])^10 Cos[x]^6 + 1568 (I Sin[x])^8 Cos[x]^8) + Exp[-10.0 I y] (112 (I Sin[x])^3 Cos[x]^13 + 112 (I Sin[x])^13 Cos[x]^3 + 1120 (I Sin[x])^5 Cos[x]^11 + 1120 (I Sin[x])^11 Cos[x]^5 + 3136 (I Sin[x])^7 Cos[x]^9 + 3136 (I Sin[x])^9 Cos[x]^7) + Exp[-0.0 I y] (56 (I Sin[x])^2 Cos[x]^14 + 56 (I Sin[x])^14 Cos[x]^2 + 896 (I Sin[x])^4 Cos[x]^12 + 896 (I Sin[x])^12 Cos[x]^4 + 3920 (I Sin[x])^6 Cos[x]^10 + 3920 (I Sin[x])^10 Cos[x]^6 + 6272 (I Sin[x])^8 Cos[x]^8) + Exp[6.0 I y] (16 (I Sin[x])^1 Cos[x]^15 + 16 (I Sin[x])^15 Cos[x]^1 + 448 (I Sin[x])^3 Cos[x]^13 + 448 (I Sin[x])^13 Cos[x]^3 + 3136 (I Sin[x])^5 Cos[x]^11 + 3136 (I Sin[x])^11 Cos[x]^5 + 7840 (I Sin[x])^7 Cos[x]^9 + 7840 (I Sin[x])^9 Cos[x]^7) + Exp[8.0 I y] (Cos[x]^16 + (I Sin[x])^16 + 64 (I Sin[x])^2 Cos[x]^14 + 64 (I Sin[x])^14 Cos[x]^2 + 784 (I Sin[x])^4 Cos[x]^12 + 784 (I Sin[x])^12 Cos[x]^4 + 3136 (I Sin[x])^6 Cos[x]^10 + 3136 (I Sin[x])^10 Cos[x]^6 + 4900 (I Sin[x])^8 Cos[x]^8))/Sqrt[2^n]);
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = Maximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[label,  ": ", result]

