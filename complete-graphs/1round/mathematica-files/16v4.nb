nqubits = 16;
label = "16v4";
nstates = 3640;

amplitude[x_,y_] := ((Exp[-120.0 I y] ((I Sin[x])^4 Cos[x]^12 + (I Sin[x])^12 Cos[x]^4) + Exp[-90.0 I y] (4 (I Sin[x])^3 Cos[x]^13 + 4 (I Sin[x])^13 Cos[x]^3 + 12 (I Sin[x])^5 Cos[x]^11 + 12 (I Sin[x])^11 Cos[x]^5) + Exp[-64.0 I y] (6 (I Sin[x])^2 Cos[x]^14 + 6 (I Sin[x])^14 Cos[x]^2 + 48 (I Sin[x])^4 Cos[x]^12 + 48 (I Sin[x])^12 Cos[x]^4 + 66 (I Sin[x])^6 Cos[x]^10 + 66 (I Sin[x])^10 Cos[x]^6) + Exp[-42.0 I y] (4 (I Sin[x])^1 Cos[x]^15 + 4 (I Sin[x])^15 Cos[x]^1 + 72 (I Sin[x])^3 Cos[x]^13 + 72 (I Sin[x])^13 Cos[x]^3 + 264 (I Sin[x])^5 Cos[x]^11 + 264 (I Sin[x])^11 Cos[x]^5 + 220 (I Sin[x])^7 Cos[x]^9 + 220 (I Sin[x])^9 Cos[x]^7) + Exp[-24.0 I y] (Cos[x]^16 + (I Sin[x])^16 + 48 (I Sin[x])^2 Cos[x]^14 + 48 (I Sin[x])^14 Cos[x]^2 + 396 (I Sin[x])^4 Cos[x]^12 + 396 (I Sin[x])^12 Cos[x]^4 + 880 (I Sin[x])^6 Cos[x]^10 + 880 (I Sin[x])^10 Cos[x]^6 + 990 (I Sin[x])^8 Cos[x]^8) + Exp[-10.0 I y] (12 (I Sin[x])^1 Cos[x]^15 + 12 (I Sin[x])^15 Cos[x]^1 + 264 (I Sin[x])^3 Cos[x]^13 + 264 (I Sin[x])^13 Cos[x]^3 + 1320 (I Sin[x])^5 Cos[x]^11 + 1320 (I Sin[x])^11 Cos[x]^5 + 2772 (I Sin[x])^7 Cos[x]^9 + 2772 (I Sin[x])^9 Cos[x]^7) + Exp[-0.0 I y] (66 (I Sin[x])^2 Cos[x]^14 + 66 (I Sin[x])^14 Cos[x]^2 + 880 (I Sin[x])^4 Cos[x]^12 + 880 (I Sin[x])^12 Cos[x]^4 + 3894 (I Sin[x])^6 Cos[x]^10 + 3894 (I Sin[x])^10 Cos[x]^6 + 6336 (I Sin[x])^8 Cos[x]^8) + Exp[6.0 I y] (220 (I Sin[x])^3 Cos[x]^13 + 220 (I Sin[x])^13 Cos[x]^3 + 2772 (I Sin[x])^5 Cos[x]^11 + 2772 (I Sin[x])^11 Cos[x]^5 + 8448 (I Sin[x])^7 Cos[x]^9 + 8448 (I Sin[x])^9 Cos[x]^7) + Exp[8.0 I y] (495 (I Sin[x])^4 Cos[x]^12 + 495 (I Sin[x])^12 Cos[x]^4 + 3168 (I Sin[x])^6 Cos[x]^10 + 3168 (I Sin[x])^10 Cos[x]^6 + 5544 (I Sin[x])^8 Cos[x]^8))/Sqrt[2^n])/Sqrt[2^nqubits];
amplitude2[x_,y_] := ((Exp[-120.0 I y] ((I Sin[x])^4 Cos[x]^12 + (I Sin[x])^12 Cos[x]^4) + Exp[-90.0 I y] (4 (I Sin[x])^3 Cos[x]^13 + 4 (I Sin[x])^13 Cos[x]^3 + 12 (I Sin[x])^5 Cos[x]^11 + 12 (I Sin[x])^11 Cos[x]^5) + Exp[-64.0 I y] (6 (I Sin[x])^2 Cos[x]^14 + 6 (I Sin[x])^14 Cos[x]^2 + 48 (I Sin[x])^4 Cos[x]^12 + 48 (I Sin[x])^12 Cos[x]^4 + 66 (I Sin[x])^6 Cos[x]^10 + 66 (I Sin[x])^10 Cos[x]^6) + Exp[-42.0 I y] (4 (I Sin[x])^1 Cos[x]^15 + 4 (I Sin[x])^15 Cos[x]^1 + 72 (I Sin[x])^3 Cos[x]^13 + 72 (I Sin[x])^13 Cos[x]^3 + 264 (I Sin[x])^5 Cos[x]^11 + 264 (I Sin[x])^11 Cos[x]^5 + 220 (I Sin[x])^7 Cos[x]^9 + 220 (I Sin[x])^9 Cos[x]^7) + Exp[-24.0 I y] (Cos[x]^16 + (I Sin[x])^16 + 48 (I Sin[x])^2 Cos[x]^14 + 48 (I Sin[x])^14 Cos[x]^2 + 396 (I Sin[x])^4 Cos[x]^12 + 396 (I Sin[x])^12 Cos[x]^4 + 880 (I Sin[x])^6 Cos[x]^10 + 880 (I Sin[x])^10 Cos[x]^6 + 990 (I Sin[x])^8 Cos[x]^8) + Exp[-10.0 I y] (12 (I Sin[x])^1 Cos[x]^15 + 12 (I Sin[x])^15 Cos[x]^1 + 264 (I Sin[x])^3 Cos[x]^13 + 264 (I Sin[x])^13 Cos[x]^3 + 1320 (I Sin[x])^5 Cos[x]^11 + 1320 (I Sin[x])^11 Cos[x]^5 + 2772 (I Sin[x])^7 Cos[x]^9 + 2772 (I Sin[x])^9 Cos[x]^7) + Exp[-0.0 I y] (66 (I Sin[x])^2 Cos[x]^14 + 66 (I Sin[x])^14 Cos[x]^2 + 880 (I Sin[x])^4 Cos[x]^12 + 880 (I Sin[x])^12 Cos[x]^4 + 3894 (I Sin[x])^6 Cos[x]^10 + 3894 (I Sin[x])^10 Cos[x]^6 + 6336 (I Sin[x])^8 Cos[x]^8) + Exp[6.0 I y] (220 (I Sin[x])^3 Cos[x]^13 + 220 (I Sin[x])^13 Cos[x]^3 + 2772 (I Sin[x])^5 Cos[x]^11 + 2772 (I Sin[x])^11 Cos[x]^5 + 8448 (I Sin[x])^7 Cos[x]^9 + 8448 (I Sin[x])^9 Cos[x]^7) + Exp[8.0 I y] (495 (I Sin[x])^4 Cos[x]^12 + 495 (I Sin[x])^12 Cos[x]^4 + 3168 (I Sin[x])^6 Cos[x]^10 + 3168 (I Sin[x])^10 Cos[x]^6 + 5544 (I Sin[x])^8 Cos[x]^8))/Sqrt[2^n]);
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = Maximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[label,  ": ", result]

