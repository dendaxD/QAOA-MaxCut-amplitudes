nqubits = 20;
label = "20v0";
nstates = 2;

amplitude[x_,y_] := ((Exp[-190.0 I y] (Cos[x]^20 + (I Sin[x])^20) + Exp[-152.0 I y] (20 (I Sin[x])^1 Cos[x]^19 + 20 (I Sin[x])^19 Cos[x]^1) + Exp[-118.0 I y] (190 (I Sin[x])^2 Cos[x]^18 + 190 (I Sin[x])^18 Cos[x]^2) + Exp[-88.0 I y] (1140 (I Sin[x])^3 Cos[x]^17 + 1140 (I Sin[x])^17 Cos[x]^3) + Exp[-62.0 I y] (4845 (I Sin[x])^4 Cos[x]^16 + 4845 (I Sin[x])^16 Cos[x]^4) + Exp[-40.0 I y] (15504 (I Sin[x])^5 Cos[x]^15 + 15504 (I Sin[x])^15 Cos[x]^5) + Exp[-22.0 I y] (38760 (I Sin[x])^6 Cos[x]^14 + 38760 (I Sin[x])^14 Cos[x]^6) + Exp[-8.0 I y] (77520 (I Sin[x])^7 Cos[x]^13 + 77520 (I Sin[x])^13 Cos[x]^7) + Exp[2.0 I y] (125970 (I Sin[x])^8 Cos[x]^12 + 125970 (I Sin[x])^12 Cos[x]^8) + Exp[8.0 I y] (167960 (I Sin[x])^9 Cos[x]^11 + 167960 (I Sin[x])^11 Cos[x]^9) + Exp[10.0 I y] (184756 (I Sin[x])^10 Cos[x]^10))/Sqrt[2^n])/Sqrt[2^nqubits];
amplitude2[x_,y_] := ((Exp[-190.0 I y] (Cos[x]^20 + (I Sin[x])^20) + Exp[-152.0 I y] (20 (I Sin[x])^1 Cos[x]^19 + 20 (I Sin[x])^19 Cos[x]^1) + Exp[-118.0 I y] (190 (I Sin[x])^2 Cos[x]^18 + 190 (I Sin[x])^18 Cos[x]^2) + Exp[-88.0 I y] (1140 (I Sin[x])^3 Cos[x]^17 + 1140 (I Sin[x])^17 Cos[x]^3) + Exp[-62.0 I y] (4845 (I Sin[x])^4 Cos[x]^16 + 4845 (I Sin[x])^16 Cos[x]^4) + Exp[-40.0 I y] (15504 (I Sin[x])^5 Cos[x]^15 + 15504 (I Sin[x])^15 Cos[x]^5) + Exp[-22.0 I y] (38760 (I Sin[x])^6 Cos[x]^14 + 38760 (I Sin[x])^14 Cos[x]^6) + Exp[-8.0 I y] (77520 (I Sin[x])^7 Cos[x]^13 + 77520 (I Sin[x])^13 Cos[x]^7) + Exp[2.0 I y] (125970 (I Sin[x])^8 Cos[x]^12 + 125970 (I Sin[x])^12 Cos[x]^8) + Exp[8.0 I y] (167960 (I Sin[x])^9 Cos[x]^11 + 167960 (I Sin[x])^11 Cos[x]^9) + Exp[10.0 I y] (184756 (I Sin[x])^10 Cos[x]^10))/Sqrt[2^n]);
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = Maximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[label,  ": ", result]

f = probability[c, d]; n = Pi;

Plot3D[f, {c, 0, n/2}, {d, -n, n}, PlotRange -> All]
