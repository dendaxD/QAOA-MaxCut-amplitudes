
k = 0.1;

max = function[0,0];x = 0;y=0;
For[β= 0 , β <= Pi, β = β+k,
	For[γ= 0 , γ<= Pi, γ = γ+k,
		max2 = function[β,γ];
		If[max2>max,{x = β,y=γ}];
		max=Max[max, max2];
	]
]

