amplitude2[x_,y_] := (Exp[-9 I y] (1 (I Sin[x])^4 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^4) + Exp[-7 I y] (10 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^4 Cos[x]^6 + 4 (I Sin[x])^6 Cos[x]^4) + Exp[-5 I y] (6 (I Sin[x])^3 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^3 + 28 (I Sin[x])^5 Cos[x]^5 + 16 (I Sin[x])^4 Cos[x]^6 + 16 (I Sin[x])^6 Cos[x]^4) + Exp[-3 I y] (44 (I Sin[x])^6 Cos[x]^4 + 44 (I Sin[x])^4 Cos[x]^6 + 14 (I Sin[x])^3 Cos[x]^7 + 14 (I Sin[x])^7 Cos[x]^3 + 52 (I Sin[x])^5 Cos[x]^5) + Exp[-1 I y] (11 (I Sin[x])^2 Cos[x]^8 + 11 (I Sin[x])^8 Cos[x]^2 + 55 (I Sin[x])^4 Cos[x]^6 + 55 (I Sin[x])^6 Cos[x]^4 + 52 (I Sin[x])^5 Cos[x]^5 + 34 (I Sin[x])^3 Cos[x]^7 + 34 (I Sin[x])^7 Cos[x]^3) + Exp[1 I y] (32 (I Sin[x])^7 Cos[x]^3 + 32 (I Sin[x])^3 Cos[x]^7 + 68 (I Sin[x])^5 Cos[x]^5 + 50 (I Sin[x])^4 Cos[x]^6 + 50 (I Sin[x])^6 Cos[x]^4 + 10 (I Sin[x])^8 Cos[x]^2 + 10 (I Sin[x])^2 Cos[x]^8) + Exp[3 I y] (22 (I Sin[x])^3 Cos[x]^7 + 22 (I Sin[x])^7 Cos[x]^3 + 6 (I Sin[x])^1 Cos[x]^9 + 6 (I Sin[x])^9 Cos[x]^1 + 32 (I Sin[x])^5 Cos[x]^5 + 24 (I Sin[x])^4 Cos[x]^6 + 24 (I Sin[x])^6 Cos[x]^4 + 16 (I Sin[x])^2 Cos[x]^8 + 16 (I Sin[x])^8 Cos[x]^2) + Exp[5 I y] (14 (I Sin[x])^6 Cos[x]^4 + 14 (I Sin[x])^4 Cos[x]^6 + 6 (I Sin[x])^8 Cos[x]^2 + 6 (I Sin[x])^2 Cos[x]^8 + 8 (I Sin[x])^5 Cos[x]^5 + 10 (I Sin[x])^7 Cos[x]^3 + 10 (I Sin[x])^3 Cos[x]^7 + 2 (I Sin[x])^9 Cos[x]^1 + 2 (I Sin[x])^1 Cos[x]^9) + Exp[7 I y] (2 (I Sin[x])^4 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^4 + 2 (I Sin[x])^2 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^2 + 1 Cos[x]^10 + 1 (I Sin[x])^10 + 2 (I Sin[x])^3 Cos[x]^7 + 2 (I Sin[x])^7 Cos[x]^3 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1) + Exp[9 I y] (2 (I Sin[x])^5 Cos[x]^5));
