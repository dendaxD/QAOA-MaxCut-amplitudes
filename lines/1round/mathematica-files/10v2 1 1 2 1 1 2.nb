nstates = 2;

amplitude[x_,y_] := (Exp[-9 I y] (1 (I Sin[x])^4 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^4) + Exp[-7 I y] (10 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^6 Cos[x]^4 + 4 (I Sin[x])^4 Cos[x]^6) + Exp[-5 I y] (12 (I Sin[x])^5 Cos[x]^5 + 14 (I Sin[x])^3 Cos[x]^7 + 14 (I Sin[x])^7 Cos[x]^3 + 12 (I Sin[x])^4 Cos[x]^6 + 12 (I Sin[x])^6 Cos[x]^4 + 4 (I Sin[x])^2 Cos[x]^8 + 4 (I Sin[x])^8 Cos[x]^2) + Exp[-3 I y] (44 (I Sin[x])^4 Cos[x]^6 + 44 (I Sin[x])^6 Cos[x]^4 + 14 (I Sin[x])^7 Cos[x]^3 + 14 (I Sin[x])^3 Cos[x]^7 + 52 (I Sin[x])^5 Cos[x]^5) + Exp[-1 I y] (51 (I Sin[x])^6 Cos[x]^4 + 51 (I Sin[x])^4 Cos[x]^6 + 15 (I Sin[x])^2 Cos[x]^8 + 15 (I Sin[x])^8 Cos[x]^2 + 26 (I Sin[x])^3 Cos[x]^7 + 26 (I Sin[x])^7 Cos[x]^3 + 60 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^1 Cos[x]^9 + 4 (I Sin[x])^9 Cos[x]^1) + Exp[1 I y] (32 (I Sin[x])^3 Cos[x]^7 + 32 (I Sin[x])^7 Cos[x]^3 + 68 (I Sin[x])^5 Cos[x]^5 + 50 (I Sin[x])^6 Cos[x]^4 + 50 (I Sin[x])^4 Cos[x]^6 + 10 (I Sin[x])^2 Cos[x]^8 + 10 (I Sin[x])^8 Cos[x]^2) + Exp[3 I y] (36 (I Sin[x])^5 Cos[x]^5 + 22 (I Sin[x])^7 Cos[x]^3 + 22 (I Sin[x])^3 Cos[x]^7 + 30 (I Sin[x])^4 Cos[x]^6 + 30 (I Sin[x])^6 Cos[x]^4 + 4 (I Sin[x])^1 Cos[x]^9 + 4 (I Sin[x])^9 Cos[x]^1 + 9 (I Sin[x])^2 Cos[x]^8 + 9 (I Sin[x])^8 Cos[x]^2 + 1 Cos[x]^10 + 1 (I Sin[x])^10) + Exp[5 I y] (14 (I Sin[x])^4 Cos[x]^6 + 14 (I Sin[x])^6 Cos[x]^4 + 6 (I Sin[x])^2 Cos[x]^8 + 6 (I Sin[x])^8 Cos[x]^2 + 8 (I Sin[x])^5 Cos[x]^5 + 10 (I Sin[x])^3 Cos[x]^7 + 10 (I Sin[x])^7 Cos[x]^3 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1) + Exp[7 I y] (4 (I Sin[x])^4 Cos[x]^6 + 4 (I Sin[x])^6 Cos[x]^4 + 1 (I Sin[x])^8 Cos[x]^2 + 1 (I Sin[x])^2 Cos[x]^8 + 4 (I Sin[x])^5 Cos[x]^5 + 2 (I Sin[x])^7 Cos[x]^3 + 2 (I Sin[x])^3 Cos[x]^7) + Exp[9 I y] (2 (I Sin[x])^5 Cos[x]^5))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-9 I y] (1 (I Sin[x])^4 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^4) + Exp[-7 I y] (10 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^6 Cos[x]^4 + 4 (I Sin[x])^4 Cos[x]^6) + Exp[-5 I y] (12 (I Sin[x])^5 Cos[x]^5 + 14 (I Sin[x])^3 Cos[x]^7 + 14 (I Sin[x])^7 Cos[x]^3 + 12 (I Sin[x])^4 Cos[x]^6 + 12 (I Sin[x])^6 Cos[x]^4 + 4 (I Sin[x])^2 Cos[x]^8 + 4 (I Sin[x])^8 Cos[x]^2) + Exp[-3 I y] (44 (I Sin[x])^4 Cos[x]^6 + 44 (I Sin[x])^6 Cos[x]^4 + 14 (I Sin[x])^7 Cos[x]^3 + 14 (I Sin[x])^3 Cos[x]^7 + 52 (I Sin[x])^5 Cos[x]^5) + Exp[-1 I y] (51 (I Sin[x])^6 Cos[x]^4 + 51 (I Sin[x])^4 Cos[x]^6 + 15 (I Sin[x])^2 Cos[x]^8 + 15 (I Sin[x])^8 Cos[x]^2 + 26 (I Sin[x])^3 Cos[x]^7 + 26 (I Sin[x])^7 Cos[x]^3 + 60 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^1 Cos[x]^9 + 4 (I Sin[x])^9 Cos[x]^1) + Exp[1 I y] (32 (I Sin[x])^3 Cos[x]^7 + 32 (I Sin[x])^7 Cos[x]^3 + 68 (I Sin[x])^5 Cos[x]^5 + 50 (I Sin[x])^6 Cos[x]^4 + 50 (I Sin[x])^4 Cos[x]^6 + 10 (I Sin[x])^2 Cos[x]^8 + 10 (I Sin[x])^8 Cos[x]^2) + Exp[3 I y] (36 (I Sin[x])^5 Cos[x]^5 + 22 (I Sin[x])^7 Cos[x]^3 + 22 (I Sin[x])^3 Cos[x]^7 + 30 (I Sin[x])^4 Cos[x]^6 + 30 (I Sin[x])^6 Cos[x]^4 + 4 (I Sin[x])^1 Cos[x]^9 + 4 (I Sin[x])^9 Cos[x]^1 + 9 (I Sin[x])^2 Cos[x]^8 + 9 (I Sin[x])^8 Cos[x]^2 + 1 Cos[x]^10 + 1 (I Sin[x])^10) + Exp[5 I y] (14 (I Sin[x])^4 Cos[x]^6 + 14 (I Sin[x])^6 Cos[x]^4 + 6 (I Sin[x])^2 Cos[x]^8 + 6 (I Sin[x])^8 Cos[x]^2 + 8 (I Sin[x])^5 Cos[x]^5 + 10 (I Sin[x])^3 Cos[x]^7 + 10 (I Sin[x])^7 Cos[x]^3 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1) + Exp[7 I y] (4 (I Sin[x])^4 Cos[x]^6 + 4 (I Sin[x])^6 Cos[x]^4 + 1 (I Sin[x])^8 Cos[x]^2 + 1 (I Sin[x])^2 Cos[x]^8 + 4 (I Sin[x])^5 Cos[x]^5 + 2 (I Sin[x])^7 Cos[x]^3 + 2 (I Sin[x])^3 Cos[x]^7) + Exp[9 I y] (2 (I Sin[x])^5 Cos[x]^5));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = NMaximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

