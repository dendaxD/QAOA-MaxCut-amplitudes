amplitude2[x_,y_] := (Exp[-9 I y] (2 (I Sin[x])^5 Cos[x]^5) + Exp[-7 I y] (5 (I Sin[x])^6 Cos[x]^4 + 5 (I Sin[x])^4 Cos[x]^6 + 8 (I Sin[x])^5 Cos[x]^5) + Exp[-5 I y] (20 (I Sin[x])^4 Cos[x]^6 + 20 (I Sin[x])^6 Cos[x]^4 + 6 (I Sin[x])^3 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^3 + 20 (I Sin[x])^5 Cos[x]^5) + Exp[-3 I y] (48 (I Sin[x])^5 Cos[x]^5 + 20 (I Sin[x])^3 Cos[x]^7 + 20 (I Sin[x])^7 Cos[x]^3 + 37 (I Sin[x])^6 Cos[x]^4 + 37 (I Sin[x])^4 Cos[x]^6 + 3 (I Sin[x])^8 Cos[x]^2 + 3 (I Sin[x])^2 Cos[x]^8) + Exp[-1 I y] (56 (I Sin[x])^5 Cos[x]^5 + 36 (I Sin[x])^3 Cos[x]^7 + 36 (I Sin[x])^7 Cos[x]^3 + 44 (I Sin[x])^4 Cos[x]^6 + 44 (I Sin[x])^6 Cos[x]^4 + 16 (I Sin[x])^2 Cos[x]^8 + 16 (I Sin[x])^8 Cos[x]^2 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1) + Exp[1 I y] (54 (I Sin[x])^6 Cos[x]^4 + 54 (I Sin[x])^4 Cos[x]^6 + 12 (I Sin[x])^2 Cos[x]^8 + 12 (I Sin[x])^8 Cos[x]^2 + 60 (I Sin[x])^5 Cos[x]^5 + 28 (I Sin[x])^3 Cos[x]^7 + 28 (I Sin[x])^7 Cos[x]^3 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1) + Exp[3 I y] (32 (I Sin[x])^4 Cos[x]^6 + 32 (I Sin[x])^6 Cos[x]^4 + 18 (I Sin[x])^3 Cos[x]^7 + 18 (I Sin[x])^7 Cos[x]^3 + 32 (I Sin[x])^5 Cos[x]^5 + 11 (I Sin[x])^2 Cos[x]^8 + 11 (I Sin[x])^8 Cos[x]^2 + 6 (I Sin[x])^1 Cos[x]^9 + 6 (I Sin[x])^9 Cos[x]^1 + 1 Cos[x]^10 + 1 (I Sin[x])^10) + Exp[5 I y] (24 (I Sin[x])^5 Cos[x]^5 + 8 (I Sin[x])^3 Cos[x]^7 + 8 (I Sin[x])^7 Cos[x]^3 + 15 (I Sin[x])^6 Cos[x]^4 + 15 (I Sin[x])^4 Cos[x]^6 + 1 (I Sin[x])^2 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^2) + Exp[7 I y] (2 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^3 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^3 + 2 (I Sin[x])^2 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^2 + 2 (I Sin[x])^4 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^4) + Exp[9 I y] (1 (I Sin[x])^4 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^4));
