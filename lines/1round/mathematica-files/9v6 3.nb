amplitude[x_,y_] := (Exp[-8 I y] (1 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^3) + Exp[-7 I y] (2 (I Sin[x])^2 Cos[x]^7 + 2 (I Sin[x])^7 Cos[x]^2 + 2 (I Sin[x])^5 Cos[x]^4 + 2 (I Sin[x])^4 Cos[x]^5 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 1 Cos[x]^9 + 1 (I Sin[x])^9 + 1 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^3) + Exp[-6 I y] (6 (I Sin[x])^2 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^2 + 13 (I Sin[x])^4 Cos[x]^5 + 13 (I Sin[x])^5 Cos[x]^4 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 7 (I Sin[x])^3 Cos[x]^6 + 7 (I Sin[x])^6 Cos[x]^3) + Exp[-5 I y] (5 (I Sin[x])^1 Cos[x]^8 + 5 (I Sin[x])^8 Cos[x]^1 + 18 (I Sin[x])^4 Cos[x]^5 + 18 (I Sin[x])^5 Cos[x]^4 + 20 (I Sin[x])^3 Cos[x]^6 + 20 (I Sin[x])^6 Cos[x]^3 + 13 (I Sin[x])^2 Cos[x]^7 + 13 (I Sin[x])^7 Cos[x]^2) + Exp[-4 I y] (27 (I Sin[x])^3 Cos[x]^6 + 27 (I Sin[x])^6 Cos[x]^3 + 35 (I Sin[x])^5 Cos[x]^4 + 35 (I Sin[x])^4 Cos[x]^5 + 8 (I Sin[x])^2 Cos[x]^7 + 8 (I Sin[x])^7 Cos[x]^2) + Exp[-3 I y] (7 (I Sin[x])^2 Cos[x]^7 + 7 (I Sin[x])^7 Cos[x]^2 + 18 (I Sin[x])^3 Cos[x]^6 + 18 (I Sin[x])^6 Cos[x]^3 + 31 (I Sin[x])^4 Cos[x]^5 + 31 (I Sin[x])^5 Cos[x]^4) + Exp[-2 I y] (21 (I Sin[x])^4 Cos[x]^5 + 21 (I Sin[x])^5 Cos[x]^4 + 7 (I Sin[x])^6 Cos[x]^3 + 7 (I Sin[x])^3 Cos[x]^6) + Exp[-1 I y] (3 (I Sin[x])^3 Cos[x]^6 + 3 (I Sin[x])^6 Cos[x]^3 + 5 (I Sin[x])^4 Cos[x]^5 + 5 (I Sin[x])^5 Cos[x]^4) + Exp[0 I y] (1 (I Sin[x])^5 Cos[x]^4 + 1 (I Sin[x])^4 Cos[x]^5))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-8 I y] (1 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^3) + Exp[-7 I y] (2 (I Sin[x])^2 Cos[x]^7 + 2 (I Sin[x])^7 Cos[x]^2 + 2 (I Sin[x])^5 Cos[x]^4 + 2 (I Sin[x])^4 Cos[x]^5 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 1 Cos[x]^9 + 1 (I Sin[x])^9 + 1 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^3) + Exp[-6 I y] (6 (I Sin[x])^2 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^2 + 13 (I Sin[x])^4 Cos[x]^5 + 13 (I Sin[x])^5 Cos[x]^4 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 7 (I Sin[x])^3 Cos[x]^6 + 7 (I Sin[x])^6 Cos[x]^3) + Exp[-5 I y] (5 (I Sin[x])^1 Cos[x]^8 + 5 (I Sin[x])^8 Cos[x]^1 + 18 (I Sin[x])^4 Cos[x]^5 + 18 (I Sin[x])^5 Cos[x]^4 + 20 (I Sin[x])^3 Cos[x]^6 + 20 (I Sin[x])^6 Cos[x]^3 + 13 (I Sin[x])^2 Cos[x]^7 + 13 (I Sin[x])^7 Cos[x]^2) + Exp[-4 I y] (27 (I Sin[x])^3 Cos[x]^6 + 27 (I Sin[x])^6 Cos[x]^3 + 35 (I Sin[x])^5 Cos[x]^4 + 35 (I Sin[x])^4 Cos[x]^5 + 8 (I Sin[x])^2 Cos[x]^7 + 8 (I Sin[x])^7 Cos[x]^2) + Exp[-3 I y] (7 (I Sin[x])^2 Cos[x]^7 + 7 (I Sin[x])^7 Cos[x]^2 + 18 (I Sin[x])^3 Cos[x]^6 + 18 (I Sin[x])^6 Cos[x]^3 + 31 (I Sin[x])^4 Cos[x]^5 + 31 (I Sin[x])^5 Cos[x]^4) + Exp[-2 I y] (21 (I Sin[x])^4 Cos[x]^5 + 21 (I Sin[x])^5 Cos[x]^4 + 7 (I Sin[x])^6 Cos[x]^3 + 7 (I Sin[x])^3 Cos[x]^6) + Exp[-1 I y] (3 (I Sin[x])^3 Cos[x]^6 + 3 (I Sin[x])^6 Cos[x]^3 + 5 (I Sin[x])^4 Cos[x]^5 + 5 (I Sin[x])^5 Cos[x]^4) + Exp[0 I y] (1 (I Sin[x])^5 Cos[x]^4 + 1 (I Sin[x])^4 Cos[x]^5));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = NMaximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

