amplitude[x_,y_] := (Exp[-9 I y] (1 (I Sin[x])^2 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^2) + Exp[-8 I y] (4 (I Sin[x])^3 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^3 + 3 (I Sin[x])^4 Cos[x]^6 + 3 (I Sin[x])^6 Cos[x]^4 + 2 (I Sin[x])^5 Cos[x]^5 + 1 (I Sin[x])^2 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^2) + Exp[-7 I y] (10 (I Sin[x])^3 Cos[x]^7 + 10 (I Sin[x])^7 Cos[x]^3 + 4 (I Sin[x])^1 Cos[x]^9 + 4 (I Sin[x])^9 Cos[x]^1 + 4 (I Sin[x])^2 Cos[x]^8 + 4 (I Sin[x])^8 Cos[x]^2 + 11 (I Sin[x])^4 Cos[x]^6 + 11 (I Sin[x])^6 Cos[x]^4 + 1 Cos[x]^10 + 1 (I Sin[x])^10 + 12 (I Sin[x])^5 Cos[x]^5) + Exp[-6 I y] (8 (I Sin[x])^2 Cos[x]^8 + 8 (I Sin[x])^8 Cos[x]^2 + 36 (I Sin[x])^6 Cos[x]^4 + 36 (I Sin[x])^4 Cos[x]^6 + 16 (I Sin[x])^3 Cos[x]^7 + 16 (I Sin[x])^7 Cos[x]^3 + 44 (I Sin[x])^5 Cos[x]^5 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1) + Exp[-5 I y] (21 (I Sin[x])^2 Cos[x]^8 + 21 (I Sin[x])^8 Cos[x]^2 + 45 (I Sin[x])^4 Cos[x]^6 + 45 (I Sin[x])^6 Cos[x]^4 + 32 (I Sin[x])^3 Cos[x]^7 + 32 (I Sin[x])^7 Cos[x]^3 + 48 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^1 Cos[x]^9 + 4 (I Sin[x])^9 Cos[x]^1) + Exp[-4 I y] (32 (I Sin[x])^3 Cos[x]^7 + 32 (I Sin[x])^7 Cos[x]^3 + 68 (I Sin[x])^5 Cos[x]^5 + 53 (I Sin[x])^4 Cos[x]^6 + 53 (I Sin[x])^6 Cos[x]^4 + 7 (I Sin[x])^2 Cos[x]^8 + 7 (I Sin[x])^8 Cos[x]^2) + Exp[-3 I y] (22 (I Sin[x])^3 Cos[x]^7 + 22 (I Sin[x])^7 Cos[x]^3 + 44 (I Sin[x])^5 Cos[x]^5 + 37 (I Sin[x])^4 Cos[x]^6 + 37 (I Sin[x])^6 Cos[x]^4 + 3 (I Sin[x])^2 Cos[x]^8 + 3 (I Sin[x])^8 Cos[x]^2) + Exp[-2 I y] (20 (I Sin[x])^4 Cos[x]^6 + 20 (I Sin[x])^6 Cos[x]^4 + 24 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^3 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^3) + Exp[-1 I y] (5 (I Sin[x])^4 Cos[x]^6 + 5 (I Sin[x])^6 Cos[x]^4 + 8 (I Sin[x])^5 Cos[x]^5) + Exp[0 I y] (2 (I Sin[x])^5 Cos[x]^5))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-9 I y] (1 (I Sin[x])^2 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^2) + Exp[-8 I y] (4 (I Sin[x])^3 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^3 + 3 (I Sin[x])^4 Cos[x]^6 + 3 (I Sin[x])^6 Cos[x]^4 + 2 (I Sin[x])^5 Cos[x]^5 + 1 (I Sin[x])^2 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^2) + Exp[-7 I y] (10 (I Sin[x])^3 Cos[x]^7 + 10 (I Sin[x])^7 Cos[x]^3 + 4 (I Sin[x])^1 Cos[x]^9 + 4 (I Sin[x])^9 Cos[x]^1 + 4 (I Sin[x])^2 Cos[x]^8 + 4 (I Sin[x])^8 Cos[x]^2 + 11 (I Sin[x])^4 Cos[x]^6 + 11 (I Sin[x])^6 Cos[x]^4 + 1 Cos[x]^10 + 1 (I Sin[x])^10 + 12 (I Sin[x])^5 Cos[x]^5) + Exp[-6 I y] (8 (I Sin[x])^2 Cos[x]^8 + 8 (I Sin[x])^8 Cos[x]^2 + 36 (I Sin[x])^6 Cos[x]^4 + 36 (I Sin[x])^4 Cos[x]^6 + 16 (I Sin[x])^3 Cos[x]^7 + 16 (I Sin[x])^7 Cos[x]^3 + 44 (I Sin[x])^5 Cos[x]^5 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1) + Exp[-5 I y] (21 (I Sin[x])^2 Cos[x]^8 + 21 (I Sin[x])^8 Cos[x]^2 + 45 (I Sin[x])^4 Cos[x]^6 + 45 (I Sin[x])^6 Cos[x]^4 + 32 (I Sin[x])^3 Cos[x]^7 + 32 (I Sin[x])^7 Cos[x]^3 + 48 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^1 Cos[x]^9 + 4 (I Sin[x])^9 Cos[x]^1) + Exp[-4 I y] (32 (I Sin[x])^3 Cos[x]^7 + 32 (I Sin[x])^7 Cos[x]^3 + 68 (I Sin[x])^5 Cos[x]^5 + 53 (I Sin[x])^4 Cos[x]^6 + 53 (I Sin[x])^6 Cos[x]^4 + 7 (I Sin[x])^2 Cos[x]^8 + 7 (I Sin[x])^8 Cos[x]^2) + Exp[-3 I y] (22 (I Sin[x])^3 Cos[x]^7 + 22 (I Sin[x])^7 Cos[x]^3 + 44 (I Sin[x])^5 Cos[x]^5 + 37 (I Sin[x])^4 Cos[x]^6 + 37 (I Sin[x])^6 Cos[x]^4 + 3 (I Sin[x])^2 Cos[x]^8 + 3 (I Sin[x])^8 Cos[x]^2) + Exp[-2 I y] (20 (I Sin[x])^4 Cos[x]^6 + 20 (I Sin[x])^6 Cos[x]^4 + 24 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^3 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^3) + Exp[-1 I y] (5 (I Sin[x])^4 Cos[x]^6 + 5 (I Sin[x])^6 Cos[x]^4 + 8 (I Sin[x])^5 Cos[x]^5) + Exp[0 I y] (2 (I Sin[x])^5 Cos[x]^5));
