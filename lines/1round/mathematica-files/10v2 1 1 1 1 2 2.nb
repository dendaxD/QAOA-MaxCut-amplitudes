amplitude[x_,y_] := (Exp[-9 I y] (1 (I Sin[x])^4 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^4) + Exp[-8 I y] (10 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^6 Cos[x]^4 + 4 (I Sin[x])^4 Cos[x]^6) + Exp[-7 I y] (14 (I Sin[x])^5 Cos[x]^5 + 13 (I Sin[x])^3 Cos[x]^7 + 13 (I Sin[x])^7 Cos[x]^3 + 13 (I Sin[x])^4 Cos[x]^6 + 13 (I Sin[x])^6 Cos[x]^4 + 3 (I Sin[x])^2 Cos[x]^8 + 3 (I Sin[x])^8 Cos[x]^2) + Exp[-6 I y] (44 (I Sin[x])^4 Cos[x]^6 + 44 (I Sin[x])^6 Cos[x]^4 + 54 (I Sin[x])^5 Cos[x]^5 + 13 (I Sin[x])^3 Cos[x]^7 + 13 (I Sin[x])^7 Cos[x]^3) + Exp[-5 I y] (48 (I Sin[x])^4 Cos[x]^6 + 48 (I Sin[x])^6 Cos[x]^4 + 18 (I Sin[x])^2 Cos[x]^8 + 18 (I Sin[x])^8 Cos[x]^2 + 54 (I Sin[x])^5 Cos[x]^5 + 29 (I Sin[x])^3 Cos[x]^7 + 29 (I Sin[x])^7 Cos[x]^3 + 4 (I Sin[x])^1 Cos[x]^9 + 4 (I Sin[x])^9 Cos[x]^1) + Exp[-4 I y] (62 (I Sin[x])^5 Cos[x]^5 + 35 (I Sin[x])^7 Cos[x]^3 + 35 (I Sin[x])^3 Cos[x]^7 + 50 (I Sin[x])^4 Cos[x]^6 + 50 (I Sin[x])^6 Cos[x]^4 + 10 (I Sin[x])^2 Cos[x]^8 + 10 (I Sin[x])^8 Cos[x]^2) + Exp[-3 I y] (19 (I Sin[x])^3 Cos[x]^7 + 19 (I Sin[x])^7 Cos[x]^3 + 42 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^1 Cos[x]^9 + 4 (I Sin[x])^9 Cos[x]^1 + 33 (I Sin[x])^6 Cos[x]^4 + 33 (I Sin[x])^4 Cos[x]^6 + 6 (I Sin[x])^2 Cos[x]^8 + 6 (I Sin[x])^8 Cos[x]^2 + 1 Cos[x]^10 + 1 (I Sin[x])^10) + Exp[-2 I y] (14 (I Sin[x])^6 Cos[x]^4 + 14 (I Sin[x])^4 Cos[x]^6 + 6 (I Sin[x])^8 Cos[x]^2 + 6 (I Sin[x])^2 Cos[x]^8 + 7 (I Sin[x])^3 Cos[x]^7 + 7 (I Sin[x])^7 Cos[x]^3 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1 + 14 (I Sin[x])^5 Cos[x]^5) + Exp[-1 I y] (2 (I Sin[x])^2 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^2 + 3 (I Sin[x])^6 Cos[x]^4 + 3 (I Sin[x])^4 Cos[x]^6 + 3 (I Sin[x])^7 Cos[x]^3 + 3 (I Sin[x])^3 Cos[x]^7 + 2 (I Sin[x])^5 Cos[x]^5) + Exp[0 I y] (1 (I Sin[x])^7 Cos[x]^3 + 1 (I Sin[x])^3 Cos[x]^7))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-9 I y] (1 (I Sin[x])^4 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^4) + Exp[-8 I y] (10 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^6 Cos[x]^4 + 4 (I Sin[x])^4 Cos[x]^6) + Exp[-7 I y] (14 (I Sin[x])^5 Cos[x]^5 + 13 (I Sin[x])^3 Cos[x]^7 + 13 (I Sin[x])^7 Cos[x]^3 + 13 (I Sin[x])^4 Cos[x]^6 + 13 (I Sin[x])^6 Cos[x]^4 + 3 (I Sin[x])^2 Cos[x]^8 + 3 (I Sin[x])^8 Cos[x]^2) + Exp[-6 I y] (44 (I Sin[x])^4 Cos[x]^6 + 44 (I Sin[x])^6 Cos[x]^4 + 54 (I Sin[x])^5 Cos[x]^5 + 13 (I Sin[x])^3 Cos[x]^7 + 13 (I Sin[x])^7 Cos[x]^3) + Exp[-5 I y] (48 (I Sin[x])^4 Cos[x]^6 + 48 (I Sin[x])^6 Cos[x]^4 + 18 (I Sin[x])^2 Cos[x]^8 + 18 (I Sin[x])^8 Cos[x]^2 + 54 (I Sin[x])^5 Cos[x]^5 + 29 (I Sin[x])^3 Cos[x]^7 + 29 (I Sin[x])^7 Cos[x]^3 + 4 (I Sin[x])^1 Cos[x]^9 + 4 (I Sin[x])^9 Cos[x]^1) + Exp[-4 I y] (62 (I Sin[x])^5 Cos[x]^5 + 35 (I Sin[x])^7 Cos[x]^3 + 35 (I Sin[x])^3 Cos[x]^7 + 50 (I Sin[x])^4 Cos[x]^6 + 50 (I Sin[x])^6 Cos[x]^4 + 10 (I Sin[x])^2 Cos[x]^8 + 10 (I Sin[x])^8 Cos[x]^2) + Exp[-3 I y] (19 (I Sin[x])^3 Cos[x]^7 + 19 (I Sin[x])^7 Cos[x]^3 + 42 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^1 Cos[x]^9 + 4 (I Sin[x])^9 Cos[x]^1 + 33 (I Sin[x])^6 Cos[x]^4 + 33 (I Sin[x])^4 Cos[x]^6 + 6 (I Sin[x])^2 Cos[x]^8 + 6 (I Sin[x])^8 Cos[x]^2 + 1 Cos[x]^10 + 1 (I Sin[x])^10) + Exp[-2 I y] (14 (I Sin[x])^6 Cos[x]^4 + 14 (I Sin[x])^4 Cos[x]^6 + 6 (I Sin[x])^8 Cos[x]^2 + 6 (I Sin[x])^2 Cos[x]^8 + 7 (I Sin[x])^3 Cos[x]^7 + 7 (I Sin[x])^7 Cos[x]^3 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1 + 14 (I Sin[x])^5 Cos[x]^5) + Exp[-1 I y] (2 (I Sin[x])^2 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^2 + 3 (I Sin[x])^6 Cos[x]^4 + 3 (I Sin[x])^4 Cos[x]^6 + 3 (I Sin[x])^7 Cos[x]^3 + 3 (I Sin[x])^3 Cos[x]^7 + 2 (I Sin[x])^5 Cos[x]^5) + Exp[0 I y] (1 (I Sin[x])^7 Cos[x]^3 + 1 (I Sin[x])^3 Cos[x]^7));
