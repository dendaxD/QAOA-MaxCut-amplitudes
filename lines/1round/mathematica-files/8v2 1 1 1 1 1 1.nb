nqubits = 8;
name = "8v2 1 1 1 1 1 1";
nstates = 4;

amplitude[x_,y_] := (Exp[-7 I y] (1 (I Sin[x])^3 Cos[x]^5 + 1 (I Sin[x])^5 Cos[x]^3) + Exp[-6 I y] (8 (I Sin[x])^4 Cos[x]^4 + 3 (I Sin[x])^3 Cos[x]^5 + 3 (I Sin[x])^5 Cos[x]^3) + Exp[-5 I y] (6 (I Sin[x])^2 Cos[x]^6 + 6 (I Sin[x])^6 Cos[x]^2 + 12 (I Sin[x])^4 Cos[x]^4 + 9 (I Sin[x])^3 Cos[x]^5 + 9 (I Sin[x])^5 Cos[x]^3) + Exp[-4 I y] (19 (I Sin[x])^5 Cos[x]^3 + 19 (I Sin[x])^3 Cos[x]^5 + 4 (I Sin[x])^2 Cos[x]^6 + 4 (I Sin[x])^6 Cos[x]^2 + 24 (I Sin[x])^4 Cos[x]^4) + Exp[-3 I y] (5 (I Sin[x])^1 Cos[x]^7 + 5 (I Sin[x])^7 Cos[x]^1 + 14 (I Sin[x])^5 Cos[x]^3 + 14 (I Sin[x])^3 Cos[x]^5 + 16 (I Sin[x])^4 Cos[x]^4 + 8 (I Sin[x])^2 Cos[x]^6 + 8 (I Sin[x])^6 Cos[x]^2) + Exp[-2 I y] (8 (I Sin[x])^6 Cos[x]^2 + 8 (I Sin[x])^2 Cos[x]^6 + 8 (I Sin[x])^4 Cos[x]^4 + 1 (I Sin[x])^1 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^1 + 8 (I Sin[x])^3 Cos[x]^5 + 8 (I Sin[x])^5 Cos[x]^3) + Exp[-1 I y] (1 Cos[x]^8 + 1 (I Sin[x])^8 + 2 (I Sin[x])^6 Cos[x]^2 + 2 (I Sin[x])^2 Cos[x]^6 + 2 (I Sin[x])^4 Cos[x]^4 + 2 (I Sin[x])^5 Cos[x]^3 + 2 (I Sin[x])^3 Cos[x]^5 + 1 (I Sin[x])^1 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^1) + Exp[0 I y] (1 (I Sin[x])^7 Cos[x]^1 + 1 (I Sin[x])^1 Cos[x]^7))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-7 I y] (1 (I Sin[x])^3 Cos[x]^5 + 1 (I Sin[x])^5 Cos[x]^3) + Exp[-6 I y] (8 (I Sin[x])^4 Cos[x]^4 + 3 (I Sin[x])^3 Cos[x]^5 + 3 (I Sin[x])^5 Cos[x]^3) + Exp[-5 I y] (6 (I Sin[x])^2 Cos[x]^6 + 6 (I Sin[x])^6 Cos[x]^2 + 12 (I Sin[x])^4 Cos[x]^4 + 9 (I Sin[x])^3 Cos[x]^5 + 9 (I Sin[x])^5 Cos[x]^3) + Exp[-4 I y] (19 (I Sin[x])^5 Cos[x]^3 + 19 (I Sin[x])^3 Cos[x]^5 + 4 (I Sin[x])^2 Cos[x]^6 + 4 (I Sin[x])^6 Cos[x]^2 + 24 (I Sin[x])^4 Cos[x]^4) + Exp[-3 I y] (5 (I Sin[x])^1 Cos[x]^7 + 5 (I Sin[x])^7 Cos[x]^1 + 14 (I Sin[x])^5 Cos[x]^3 + 14 (I Sin[x])^3 Cos[x]^5 + 16 (I Sin[x])^4 Cos[x]^4 + 8 (I Sin[x])^2 Cos[x]^6 + 8 (I Sin[x])^6 Cos[x]^2) + Exp[-2 I y] (8 (I Sin[x])^6 Cos[x]^2 + 8 (I Sin[x])^2 Cos[x]^6 + 8 (I Sin[x])^4 Cos[x]^4 + 1 (I Sin[x])^1 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^1 + 8 (I Sin[x])^3 Cos[x]^5 + 8 (I Sin[x])^5 Cos[x]^3) + Exp[-1 I y] (1 Cos[x]^8 + 1 (I Sin[x])^8 + 2 (I Sin[x])^6 Cos[x]^2 + 2 (I Sin[x])^2 Cos[x]^6 + 2 (I Sin[x])^4 Cos[x]^4 + 2 (I Sin[x])^5 Cos[x]^3 + 2 (I Sin[x])^3 Cos[x]^5 + 1 (I Sin[x])^1 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^1) + Exp[0 I y] (1 (I Sin[x])^7 Cos[x]^1 + 1 (I Sin[x])^1 Cos[x]^7));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = NMaximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

f = probability[c, d]; n = Pi;
Plot3D[f, {c, 0, n/2}, {d, -n, n}, PlotRange -> All]
ContourPlot[probability[x, y], {x, 0, n/2}, {y, 0, n}, PlotLegends -> Automatic, Contours -> 30]
