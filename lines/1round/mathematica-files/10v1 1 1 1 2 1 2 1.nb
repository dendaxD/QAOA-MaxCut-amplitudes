amplitude[x_,y_] := (Exp[-9 I y] (1 (I Sin[x])^4 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^4) + Exp[-8 I y] (1 (I Sin[x])^3 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^3 + 8 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^4 Cos[x]^6 + 4 (I Sin[x])^6 Cos[x]^4) + Exp[-7 I y] (28 (I Sin[x])^5 Cos[x]^5 + 6 (I Sin[x])^3 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^3 + 16 (I Sin[x])^6 Cos[x]^4 + 16 (I Sin[x])^4 Cos[x]^6) + Exp[-6 I y] (40 (I Sin[x])^4 Cos[x]^6 + 40 (I Sin[x])^6 Cos[x]^4 + 4 (I Sin[x])^2 Cos[x]^8 + 4 (I Sin[x])^8 Cos[x]^2 + 18 (I Sin[x])^3 Cos[x]^7 + 18 (I Sin[x])^7 Cos[x]^3 + 44 (I Sin[x])^5 Cos[x]^5) + Exp[-5 I y] (59 (I Sin[x])^4 Cos[x]^6 + 59 (I Sin[x])^6 Cos[x]^4 + 7 (I Sin[x])^2 Cos[x]^8 + 7 (I Sin[x])^8 Cos[x]^2 + 26 (I Sin[x])^7 Cos[x]^3 + 26 (I Sin[x])^3 Cos[x]^7 + 68 (I Sin[x])^5 Cos[x]^5) + Exp[-4 I y] (48 (I Sin[x])^5 Cos[x]^5 + 38 (I Sin[x])^3 Cos[x]^7 + 38 (I Sin[x])^7 Cos[x]^3 + 42 (I Sin[x])^4 Cos[x]^6 + 42 (I Sin[x])^6 Cos[x]^4 + 18 (I Sin[x])^2 Cos[x]^8 + 18 (I Sin[x])^8 Cos[x]^2 + 4 (I Sin[x])^1 Cos[x]^9 + 4 (I Sin[x])^9 Cos[x]^1) + Exp[-3 I y] (40 (I Sin[x])^5 Cos[x]^5 + 22 (I Sin[x])^3 Cos[x]^7 + 22 (I Sin[x])^7 Cos[x]^3 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1 + 32 (I Sin[x])^6 Cos[x]^4 + 32 (I Sin[x])^4 Cos[x]^6 + 8 (I Sin[x])^2 Cos[x]^8 + 8 (I Sin[x])^8 Cos[x]^2) + Exp[-2 I y] (12 (I Sin[x])^4 Cos[x]^6 + 12 (I Sin[x])^6 Cos[x]^4 + 7 (I Sin[x])^2 Cos[x]^8 + 7 (I Sin[x])^8 Cos[x]^2 + 12 (I Sin[x])^5 Cos[x]^5 + 6 (I Sin[x])^3 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^3 + 4 (I Sin[x])^1 Cos[x]^9 + 4 (I Sin[x])^9 Cos[x]^1 + 1 Cos[x]^10 + 1 (I Sin[x])^10) + Exp[-1 I y] (4 (I Sin[x])^6 Cos[x]^4 + 4 (I Sin[x])^4 Cos[x]^6 + 1 (I Sin[x])^2 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^2 + 4 (I Sin[x])^5 Cos[x]^5 + 2 (I Sin[x])^3 Cos[x]^7 + 2 (I Sin[x])^7 Cos[x]^3) + Exp[0 I y] (1 (I Sin[x])^3 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^3))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-9 I y] (1 (I Sin[x])^4 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^4) + Exp[-8 I y] (1 (I Sin[x])^3 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^3 + 8 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^4 Cos[x]^6 + 4 (I Sin[x])^6 Cos[x]^4) + Exp[-7 I y] (28 (I Sin[x])^5 Cos[x]^5 + 6 (I Sin[x])^3 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^3 + 16 (I Sin[x])^6 Cos[x]^4 + 16 (I Sin[x])^4 Cos[x]^6) + Exp[-6 I y] (40 (I Sin[x])^4 Cos[x]^6 + 40 (I Sin[x])^6 Cos[x]^4 + 4 (I Sin[x])^2 Cos[x]^8 + 4 (I Sin[x])^8 Cos[x]^2 + 18 (I Sin[x])^3 Cos[x]^7 + 18 (I Sin[x])^7 Cos[x]^3 + 44 (I Sin[x])^5 Cos[x]^5) + Exp[-5 I y] (59 (I Sin[x])^4 Cos[x]^6 + 59 (I Sin[x])^6 Cos[x]^4 + 7 (I Sin[x])^2 Cos[x]^8 + 7 (I Sin[x])^8 Cos[x]^2 + 26 (I Sin[x])^7 Cos[x]^3 + 26 (I Sin[x])^3 Cos[x]^7 + 68 (I Sin[x])^5 Cos[x]^5) + Exp[-4 I y] (48 (I Sin[x])^5 Cos[x]^5 + 38 (I Sin[x])^3 Cos[x]^7 + 38 (I Sin[x])^7 Cos[x]^3 + 42 (I Sin[x])^4 Cos[x]^6 + 42 (I Sin[x])^6 Cos[x]^4 + 18 (I Sin[x])^2 Cos[x]^8 + 18 (I Sin[x])^8 Cos[x]^2 + 4 (I Sin[x])^1 Cos[x]^9 + 4 (I Sin[x])^9 Cos[x]^1) + Exp[-3 I y] (40 (I Sin[x])^5 Cos[x]^5 + 22 (I Sin[x])^3 Cos[x]^7 + 22 (I Sin[x])^7 Cos[x]^3 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1 + 32 (I Sin[x])^6 Cos[x]^4 + 32 (I Sin[x])^4 Cos[x]^6 + 8 (I Sin[x])^2 Cos[x]^8 + 8 (I Sin[x])^8 Cos[x]^2) + Exp[-2 I y] (12 (I Sin[x])^4 Cos[x]^6 + 12 (I Sin[x])^6 Cos[x]^4 + 7 (I Sin[x])^2 Cos[x]^8 + 7 (I Sin[x])^8 Cos[x]^2 + 12 (I Sin[x])^5 Cos[x]^5 + 6 (I Sin[x])^3 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^3 + 4 (I Sin[x])^1 Cos[x]^9 + 4 (I Sin[x])^9 Cos[x]^1 + 1 Cos[x]^10 + 1 (I Sin[x])^10) + Exp[-1 I y] (4 (I Sin[x])^6 Cos[x]^4 + 4 (I Sin[x])^4 Cos[x]^6 + 1 (I Sin[x])^2 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^2 + 4 (I Sin[x])^5 Cos[x]^5 + 2 (I Sin[x])^3 Cos[x]^7 + 2 (I Sin[x])^7 Cos[x]^3) + Exp[0 I y] (1 (I Sin[x])^3 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^3));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = NMaximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

