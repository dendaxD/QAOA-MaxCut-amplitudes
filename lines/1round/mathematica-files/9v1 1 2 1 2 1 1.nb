amplitude2[x_,y_] := (Exp[-8 I y] (1 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^3) + Exp[-6 I y] (6 (I Sin[x])^4 Cos[x]^5 + 6 (I Sin[x])^5 Cos[x]^4 + 2 (I Sin[x])^3 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^3) + Exp[-4 I y] (3 (I Sin[x])^2 Cos[x]^7 + 3 (I Sin[x])^7 Cos[x]^2 + 18 (I Sin[x])^4 Cos[x]^5 + 18 (I Sin[x])^5 Cos[x]^4 + 7 (I Sin[x])^3 Cos[x]^6 + 7 (I Sin[x])^6 Cos[x]^3) + Exp[-2 I y] (34 (I Sin[x])^5 Cos[x]^4 + 34 (I Sin[x])^4 Cos[x]^5 + 18 (I Sin[x])^6 Cos[x]^3 + 18 (I Sin[x])^3 Cos[x]^6 + 4 (I Sin[x])^2 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^2) + Exp[0 I y] (27 (I Sin[x])^3 Cos[x]^6 + 27 (I Sin[x])^6 Cos[x]^3 + 3 (I Sin[x])^1 Cos[x]^8 + 3 (I Sin[x])^8 Cos[x]^1 + 11 (I Sin[x])^2 Cos[x]^7 + 11 (I Sin[x])^7 Cos[x]^2 + 29 (I Sin[x])^5 Cos[x]^4 + 29 (I Sin[x])^4 Cos[x]^5) + Exp[2 I y] (24 (I Sin[x])^4 Cos[x]^5 + 24 (I Sin[x])^5 Cos[x]^4 + 20 (I Sin[x])^6 Cos[x]^3 + 20 (I Sin[x])^3 Cos[x]^6 + 10 (I Sin[x])^7 Cos[x]^2 + 10 (I Sin[x])^2 Cos[x]^7 + 2 (I Sin[x])^8 Cos[x]^1 + 2 (I Sin[x])^1 Cos[x]^8) + Exp[4 I y] (9 (I Sin[x])^4 Cos[x]^5 + 9 (I Sin[x])^5 Cos[x]^4 + 8 (I Sin[x])^2 Cos[x]^7 + 8 (I Sin[x])^7 Cos[x]^2 + 6 (I Sin[x])^3 Cos[x]^6 + 6 (I Sin[x])^6 Cos[x]^3 + 1 Cos[x]^9 + 1 (I Sin[x])^9 + 4 (I Sin[x])^1 Cos[x]^8 + 4 (I Sin[x])^8 Cos[x]^1) + Exp[6 I y] (6 (I Sin[x])^5 Cos[x]^4 + 6 (I Sin[x])^4 Cos[x]^5 + 2 (I Sin[x])^3 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^3) + Exp[8 I y] (1 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^3));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

