amplitude2[x_,y_] := (Exp[-9 I y] (2 (I Sin[x])^5 Cos[x]^5) + Exp[-7 I y] (5 (I Sin[x])^4 Cos[x]^6 + 5 (I Sin[x])^6 Cos[x]^4 + 6 (I Sin[x])^5 Cos[x]^5 + 1 (I Sin[x])^3 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^3) + Exp[-5 I y] (20 (I Sin[x])^6 Cos[x]^4 + 20 (I Sin[x])^4 Cos[x]^6 + 26 (I Sin[x])^5 Cos[x]^5 + 3 (I Sin[x])^7 Cos[x]^3 + 3 (I Sin[x])^3 Cos[x]^7) + Exp[-3 I y] (21 (I Sin[x])^3 Cos[x]^7 + 21 (I Sin[x])^7 Cos[x]^3 + 46 (I Sin[x])^5 Cos[x]^5 + 36 (I Sin[x])^6 Cos[x]^4 + 36 (I Sin[x])^4 Cos[x]^6 + 4 (I Sin[x])^2 Cos[x]^8 + 4 (I Sin[x])^8 Cos[x]^2) + Exp[-1 I y] (33 (I Sin[x])^7 Cos[x]^3 + 33 (I Sin[x])^3 Cos[x]^7 + 66 (I Sin[x])^5 Cos[x]^5 + 54 (I Sin[x])^4 Cos[x]^6 + 54 (I Sin[x])^6 Cos[x]^4 + 6 (I Sin[x])^2 Cos[x]^8 + 6 (I Sin[x])^8 Cos[x]^2) + Exp[1 I y] (19 (I Sin[x])^2 Cos[x]^8 + 19 (I Sin[x])^8 Cos[x]^2 + 47 (I Sin[x])^4 Cos[x]^6 + 47 (I Sin[x])^6 Cos[x]^4 + 50 (I Sin[x])^5 Cos[x]^5 + 31 (I Sin[x])^3 Cos[x]^7 + 31 (I Sin[x])^7 Cos[x]^3 + 4 (I Sin[x])^1 Cos[x]^9 + 4 (I Sin[x])^9 Cos[x]^1) + Exp[3 I y] (34 (I Sin[x])^6 Cos[x]^4 + 34 (I Sin[x])^4 Cos[x]^6 + 10 (I Sin[x])^8 Cos[x]^2 + 10 (I Sin[x])^2 Cos[x]^8 + 42 (I Sin[x])^5 Cos[x]^5 + 17 (I Sin[x])^7 Cos[x]^3 + 17 (I Sin[x])^3 Cos[x]^7 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1) + Exp[5 I y] (11 (I Sin[x])^3 Cos[x]^7 + 11 (I Sin[x])^7 Cos[x]^3 + 10 (I Sin[x])^5 Cos[x]^5 + 10 (I Sin[x])^6 Cos[x]^4 + 10 (I Sin[x])^4 Cos[x]^6 + 5 (I Sin[x])^2 Cos[x]^8 + 5 (I Sin[x])^8 Cos[x]^2 + 4 (I Sin[x])^1 Cos[x]^9 + 4 (I Sin[x])^9 Cos[x]^1 + 1 Cos[x]^10 + 1 (I Sin[x])^10) + Exp[7 I y] (3 (I Sin[x])^7 Cos[x]^3 + 3 (I Sin[x])^3 Cos[x]^7 + 4 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^4 Cos[x]^6 + 4 (I Sin[x])^6 Cos[x]^4) + Exp[9 I y] (1 (I Sin[x])^2 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^2));
