name = "10v1 1 1 1 1 2 1 1 1";
nstates = 4;

amplitude[x_,y_] := (Exp[-9 I y] (2 (I Sin[x])^5 Cos[x]^5) + Exp[-7 I y] (5 (I Sin[x])^6 Cos[x]^4 + 5 (I Sin[x])^4 Cos[x]^6 + 8 (I Sin[x])^5 Cos[x]^5) + Exp[-5 I y] (20 (I Sin[x])^4 Cos[x]^6 + 20 (I Sin[x])^6 Cos[x]^4 + 20 (I Sin[x])^5 Cos[x]^5 + 6 (I Sin[x])^3 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^3) + Exp[-3 I y] (14 (I Sin[x])^7 Cos[x]^3 + 14 (I Sin[x])^3 Cos[x]^7 + 60 (I Sin[x])^5 Cos[x]^5 + 40 (I Sin[x])^4 Cos[x]^6 + 40 (I Sin[x])^6 Cos[x]^4) + Exp[-1 I y] (34 (I Sin[x])^3 Cos[x]^7 + 34 (I Sin[x])^7 Cos[x]^3 + 64 (I Sin[x])^5 Cos[x]^5 + 49 (I Sin[x])^4 Cos[x]^6 + 49 (I Sin[x])^6 Cos[x]^4 + 11 (I Sin[x])^2 Cos[x]^8 + 11 (I Sin[x])^8 Cos[x]^2) + Exp[1 I y] (56 (I Sin[x])^6 Cos[x]^4 + 56 (I Sin[x])^4 Cos[x]^6 + 10 (I Sin[x])^2 Cos[x]^8 + 10 (I Sin[x])^8 Cos[x]^2 + 56 (I Sin[x])^5 Cos[x]^5 + 32 (I Sin[x])^7 Cos[x]^3 + 32 (I Sin[x])^3 Cos[x]^7) + Exp[3 I y] (28 (I Sin[x])^4 Cos[x]^6 + 28 (I Sin[x])^6 Cos[x]^4 + 16 (I Sin[x])^2 Cos[x]^8 + 16 (I Sin[x])^8 Cos[x]^2 + 22 (I Sin[x])^3 Cos[x]^7 + 22 (I Sin[x])^7 Cos[x]^3 + 24 (I Sin[x])^5 Cos[x]^5 + 6 (I Sin[x])^1 Cos[x]^9 + 6 (I Sin[x])^9 Cos[x]^1) + Exp[5 I y] (16 (I Sin[x])^5 Cos[x]^5 + 10 (I Sin[x])^3 Cos[x]^7 + 10 (I Sin[x])^7 Cos[x]^3 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1 + 10 (I Sin[x])^6 Cos[x]^4 + 10 (I Sin[x])^4 Cos[x]^6 + 6 (I Sin[x])^8 Cos[x]^2 + 6 (I Sin[x])^2 Cos[x]^8) + Exp[7 I y] (2 (I Sin[x])^5 Cos[x]^5 + 2 (I Sin[x])^3 Cos[x]^7 + 2 (I Sin[x])^7 Cos[x]^3 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1 + 2 (I Sin[x])^2 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^2 + 1 (I Sin[x])^4 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^4 + 1 Cos[x]^10 + 1 (I Sin[x])^10) + Exp[9 I y] (1 (I Sin[x])^4 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^4))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-9 I y] (2 (I Sin[x])^5 Cos[x]^5) + Exp[-7 I y] (5 (I Sin[x])^6 Cos[x]^4 + 5 (I Sin[x])^4 Cos[x]^6 + 8 (I Sin[x])^5 Cos[x]^5) + Exp[-5 I y] (20 (I Sin[x])^4 Cos[x]^6 + 20 (I Sin[x])^6 Cos[x]^4 + 20 (I Sin[x])^5 Cos[x]^5 + 6 (I Sin[x])^3 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^3) + Exp[-3 I y] (14 (I Sin[x])^7 Cos[x]^3 + 14 (I Sin[x])^3 Cos[x]^7 + 60 (I Sin[x])^5 Cos[x]^5 + 40 (I Sin[x])^4 Cos[x]^6 + 40 (I Sin[x])^6 Cos[x]^4) + Exp[-1 I y] (34 (I Sin[x])^3 Cos[x]^7 + 34 (I Sin[x])^7 Cos[x]^3 + 64 (I Sin[x])^5 Cos[x]^5 + 49 (I Sin[x])^4 Cos[x]^6 + 49 (I Sin[x])^6 Cos[x]^4 + 11 (I Sin[x])^2 Cos[x]^8 + 11 (I Sin[x])^8 Cos[x]^2) + Exp[1 I y] (56 (I Sin[x])^6 Cos[x]^4 + 56 (I Sin[x])^4 Cos[x]^6 + 10 (I Sin[x])^2 Cos[x]^8 + 10 (I Sin[x])^8 Cos[x]^2 + 56 (I Sin[x])^5 Cos[x]^5 + 32 (I Sin[x])^7 Cos[x]^3 + 32 (I Sin[x])^3 Cos[x]^7) + Exp[3 I y] (28 (I Sin[x])^4 Cos[x]^6 + 28 (I Sin[x])^6 Cos[x]^4 + 16 (I Sin[x])^2 Cos[x]^8 + 16 (I Sin[x])^8 Cos[x]^2 + 22 (I Sin[x])^3 Cos[x]^7 + 22 (I Sin[x])^7 Cos[x]^3 + 24 (I Sin[x])^5 Cos[x]^5 + 6 (I Sin[x])^1 Cos[x]^9 + 6 (I Sin[x])^9 Cos[x]^1) + Exp[5 I y] (16 (I Sin[x])^5 Cos[x]^5 + 10 (I Sin[x])^3 Cos[x]^7 + 10 (I Sin[x])^7 Cos[x]^3 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1 + 10 (I Sin[x])^6 Cos[x]^4 + 10 (I Sin[x])^4 Cos[x]^6 + 6 (I Sin[x])^8 Cos[x]^2 + 6 (I Sin[x])^2 Cos[x]^8) + Exp[7 I y] (2 (I Sin[x])^5 Cos[x]^5 + 2 (I Sin[x])^3 Cos[x]^7 + 2 (I Sin[x])^7 Cos[x]^3 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1 + 2 (I Sin[x])^2 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^2 + 1 (I Sin[x])^4 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^4 + 1 Cos[x]^10 + 1 (I Sin[x])^10) + Exp[9 I y] (1 (I Sin[x])^4 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^4));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = NMaximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

