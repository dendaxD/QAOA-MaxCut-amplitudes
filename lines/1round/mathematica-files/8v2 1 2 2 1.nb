nqubits = 8;
name = "8v2 1 2 2 1";
nstates = 4;

amplitude[x_,y_] := (Exp[-7 I y] (1 (I Sin[x])^3 Cos[x]^5 + 1 (I Sin[x])^5 Cos[x]^3) + Exp[-5 I y] (6 (I Sin[x])^4 Cos[x]^4 + 3 (I Sin[x])^3 Cos[x]^5 + 3 (I Sin[x])^5 Cos[x]^3 + 1 (I Sin[x])^2 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^2) + Exp[-3 I y] (5 (I Sin[x])^2 Cos[x]^6 + 5 (I Sin[x])^6 Cos[x]^2 + 14 (I Sin[x])^4 Cos[x]^4 + 1 (I Sin[x])^1 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^1 + 8 (I Sin[x])^3 Cos[x]^5 + 8 (I Sin[x])^5 Cos[x]^3) + Exp[-1 I y] (18 (I Sin[x])^3 Cos[x]^5 + 18 (I Sin[x])^5 Cos[x]^3 + 20 (I Sin[x])^4 Cos[x]^4 + 6 (I Sin[x])^2 Cos[x]^6 + 6 (I Sin[x])^6 Cos[x]^2 + 1 (I Sin[x])^1 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^1) + Exp[1 I y] (14 (I Sin[x])^3 Cos[x]^5 + 14 (I Sin[x])^5 Cos[x]^3 + 10 (I Sin[x])^2 Cos[x]^6 + 10 (I Sin[x])^6 Cos[x]^2 + 10 (I Sin[x])^4 Cos[x]^4 + 5 (I Sin[x])^1 Cos[x]^7 + 5 (I Sin[x])^7 Cos[x]^1 + 1 Cos[x]^8 + 1 (I Sin[x])^8) + Exp[3 I y] (14 (I Sin[x])^4 Cos[x]^4 + 5 (I Sin[x])^2 Cos[x]^6 + 5 (I Sin[x])^6 Cos[x]^2 + 8 (I Sin[x])^3 Cos[x]^5 + 8 (I Sin[x])^5 Cos[x]^3 + 1 (I Sin[x])^7 Cos[x]^1 + 1 (I Sin[x])^1 Cos[x]^7) + Exp[5 I y] (1 (I Sin[x])^2 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^2 + 6 (I Sin[x])^4 Cos[x]^4 + 3 (I Sin[x])^3 Cos[x]^5 + 3 (I Sin[x])^5 Cos[x]^3) + Exp[7 I y] (1 (I Sin[x])^5 Cos[x]^3 + 1 (I Sin[x])^3 Cos[x]^5))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-7 I y] (1 (I Sin[x])^3 Cos[x]^5 + 1 (I Sin[x])^5 Cos[x]^3) + Exp[-5 I y] (6 (I Sin[x])^4 Cos[x]^4 + 3 (I Sin[x])^3 Cos[x]^5 + 3 (I Sin[x])^5 Cos[x]^3 + 1 (I Sin[x])^2 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^2) + Exp[-3 I y] (5 (I Sin[x])^2 Cos[x]^6 + 5 (I Sin[x])^6 Cos[x]^2 + 14 (I Sin[x])^4 Cos[x]^4 + 1 (I Sin[x])^1 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^1 + 8 (I Sin[x])^3 Cos[x]^5 + 8 (I Sin[x])^5 Cos[x]^3) + Exp[-1 I y] (18 (I Sin[x])^3 Cos[x]^5 + 18 (I Sin[x])^5 Cos[x]^3 + 20 (I Sin[x])^4 Cos[x]^4 + 6 (I Sin[x])^2 Cos[x]^6 + 6 (I Sin[x])^6 Cos[x]^2 + 1 (I Sin[x])^1 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^1) + Exp[1 I y] (14 (I Sin[x])^3 Cos[x]^5 + 14 (I Sin[x])^5 Cos[x]^3 + 10 (I Sin[x])^2 Cos[x]^6 + 10 (I Sin[x])^6 Cos[x]^2 + 10 (I Sin[x])^4 Cos[x]^4 + 5 (I Sin[x])^1 Cos[x]^7 + 5 (I Sin[x])^7 Cos[x]^1 + 1 Cos[x]^8 + 1 (I Sin[x])^8) + Exp[3 I y] (14 (I Sin[x])^4 Cos[x]^4 + 5 (I Sin[x])^2 Cos[x]^6 + 5 (I Sin[x])^6 Cos[x]^2 + 8 (I Sin[x])^3 Cos[x]^5 + 8 (I Sin[x])^5 Cos[x]^3 + 1 (I Sin[x])^7 Cos[x]^1 + 1 (I Sin[x])^1 Cos[x]^7) + Exp[5 I y] (1 (I Sin[x])^2 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^2 + 6 (I Sin[x])^4 Cos[x]^4 + 3 (I Sin[x])^3 Cos[x]^5 + 3 (I Sin[x])^5 Cos[x]^3) + Exp[7 I y] (1 (I Sin[x])^5 Cos[x]^3 + 1 (I Sin[x])^3 Cos[x]^5));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = NMaximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

f = probability[c, d]; n = Pi;
