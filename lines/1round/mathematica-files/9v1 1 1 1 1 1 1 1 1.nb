nqubits = 9;
name = "9v1 1 1 1 1 1 1 1 1";
nstates = 2;

amplitude[x_,y_] := (Exp[-8 I y] (1 (I Sin[x])^4 Cos[x]^5 + 1 (I Sin[x])^5 Cos[x]^4) + Exp[-6 I y] (8 (I Sin[x])^5 Cos[x]^4 + 8 (I Sin[x])^4 Cos[x]^5) + Exp[-4 I y] (10 (I Sin[x])^3 Cos[x]^6 + 10 (I Sin[x])^6 Cos[x]^3 + 18 (I Sin[x])^5 Cos[x]^4 + 18 (I Sin[x])^4 Cos[x]^5) + Exp[-2 I y] (20 (I Sin[x])^6 Cos[x]^3 + 20 (I Sin[x])^3 Cos[x]^6 + 36 (I Sin[x])^4 Cos[x]^5 + 36 (I Sin[x])^5 Cos[x]^4) + Exp[0 I y] (15 (I Sin[x])^2 Cos[x]^7 + 15 (I Sin[x])^7 Cos[x]^2 + 30 (I Sin[x])^4 Cos[x]^5 + 30 (I Sin[x])^5 Cos[x]^4 + 25 (I Sin[x])^6 Cos[x]^3 + 25 (I Sin[x])^3 Cos[x]^6) + Exp[2 I y] (12 (I Sin[x])^7 Cos[x]^2 + 12 (I Sin[x])^2 Cos[x]^7 + 24 (I Sin[x])^5 Cos[x]^4 + 24 (I Sin[x])^4 Cos[x]^5 + 20 (I Sin[x])^3 Cos[x]^6 + 20 (I Sin[x])^6 Cos[x]^3) + Exp[4 I y] (7 (I Sin[x])^1 Cos[x]^8 + 7 (I Sin[x])^8 Cos[x]^1 + 7 (I Sin[x])^3 Cos[x]^6 + 7 (I Sin[x])^6 Cos[x]^3 + 7 (I Sin[x])^5 Cos[x]^4 + 7 (I Sin[x])^4 Cos[x]^5 + 7 (I Sin[x])^7 Cos[x]^2 + 7 (I Sin[x])^2 Cos[x]^7) + Exp[6 I y] (2 (I Sin[x])^8 Cos[x]^1 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^6 Cos[x]^3 + 2 (I Sin[x])^3 Cos[x]^6 + 2 (I Sin[x])^4 Cos[x]^5 + 2 (I Sin[x])^5 Cos[x]^4 + 2 (I Sin[x])^2 Cos[x]^7 + 2 (I Sin[x])^7 Cos[x]^2) + Exp[8 I y] (1 Cos[x]^9 + 1 (I Sin[x])^9))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-8 I y] (1 (I Sin[x])^4 Cos[x]^5 + 1 (I Sin[x])^5 Cos[x]^4) + Exp[-6 I y] (8 (I Sin[x])^5 Cos[x]^4 + 8 (I Sin[x])^4 Cos[x]^5) + Exp[-4 I y] (10 (I Sin[x])^3 Cos[x]^6 + 10 (I Sin[x])^6 Cos[x]^3 + 18 (I Sin[x])^5 Cos[x]^4 + 18 (I Sin[x])^4 Cos[x]^5) + Exp[-2 I y] (20 (I Sin[x])^6 Cos[x]^3 + 20 (I Sin[x])^3 Cos[x]^6 + 36 (I Sin[x])^4 Cos[x]^5 + 36 (I Sin[x])^5 Cos[x]^4) + Exp[0 I y] (15 (I Sin[x])^2 Cos[x]^7 + 15 (I Sin[x])^7 Cos[x]^2 + 30 (I Sin[x])^4 Cos[x]^5 + 30 (I Sin[x])^5 Cos[x]^4 + 25 (I Sin[x])^6 Cos[x]^3 + 25 (I Sin[x])^3 Cos[x]^6) + Exp[2 I y] (12 (I Sin[x])^7 Cos[x]^2 + 12 (I Sin[x])^2 Cos[x]^7 + 24 (I Sin[x])^5 Cos[x]^4 + 24 (I Sin[x])^4 Cos[x]^5 + 20 (I Sin[x])^3 Cos[x]^6 + 20 (I Sin[x])^6 Cos[x]^3) + Exp[4 I y] (7 (I Sin[x])^1 Cos[x]^8 + 7 (I Sin[x])^8 Cos[x]^1 + 7 (I Sin[x])^3 Cos[x]^6 + 7 (I Sin[x])^6 Cos[x]^3 + 7 (I Sin[x])^5 Cos[x]^4 + 7 (I Sin[x])^4 Cos[x]^5 + 7 (I Sin[x])^7 Cos[x]^2 + 7 (I Sin[x])^2 Cos[x]^7) + Exp[6 I y] (2 (I Sin[x])^8 Cos[x]^1 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^6 Cos[x]^3 + 2 (I Sin[x])^3 Cos[x]^6 + 2 (I Sin[x])^4 Cos[x]^5 + 2 (I Sin[x])^5 Cos[x]^4 + 2 (I Sin[x])^2 Cos[x]^7 + 2 (I Sin[x])^7 Cos[x]^2) + Exp[8 I y] (1 Cos[x]^9 + 1 (I Sin[x])^9));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = NMaximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

