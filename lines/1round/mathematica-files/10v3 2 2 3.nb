nqubits = 10;
name = "10v3 2 2 3";
nstates = 2;

amplitude[x_,y_] := (Exp[-9 I y] (2 (I Sin[x])^5 Cos[x]^5) + Exp[-8 I y] (3 (I Sin[x])^4 Cos[x]^6 + 3 (I Sin[x])^6 Cos[x]^4 + 4 (I Sin[x])^3 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^3 + 2 (I Sin[x])^2 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^2) + Exp[-7 I y] (20 (I Sin[x])^4 Cos[x]^6 + 20 (I Sin[x])^6 Cos[x]^4 + 6 (I Sin[x])^3 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^3 + 20 (I Sin[x])^5 Cos[x]^5) + Exp[-6 I y] (20 (I Sin[x])^3 Cos[x]^7 + 20 (I Sin[x])^7 Cos[x]^3 + 36 (I Sin[x])^5 Cos[x]^5 + 27 (I Sin[x])^4 Cos[x]^6 + 27 (I Sin[x])^6 Cos[x]^4 + 12 (I Sin[x])^2 Cos[x]^8 + 12 (I Sin[x])^8 Cos[x]^2 + 6 (I Sin[x])^1 Cos[x]^9 + 6 (I Sin[x])^9 Cos[x]^1 + 1 Cos[x]^10 + 1 (I Sin[x])^10) + Exp[-5 I y] (64 (I Sin[x])^5 Cos[x]^5 + 46 (I Sin[x])^4 Cos[x]^6 + 46 (I Sin[x])^6 Cos[x]^4 + 32 (I Sin[x])^3 Cos[x]^7 + 32 (I Sin[x])^7 Cos[x]^3 + 14 (I Sin[x])^2 Cos[x]^8 + 14 (I Sin[x])^8 Cos[x]^2 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1) + Exp[-4 I y] (52 (I Sin[x])^4 Cos[x]^6 + 52 (I Sin[x])^6 Cos[x]^4 + 14 (I Sin[x])^2 Cos[x]^8 + 14 (I Sin[x])^8 Cos[x]^2 + 32 (I Sin[x])^3 Cos[x]^7 + 32 (I Sin[x])^7 Cos[x]^3 + 52 (I Sin[x])^5 Cos[x]^5 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1) + Exp[-3 I y] (42 (I Sin[x])^4 Cos[x]^6 + 42 (I Sin[x])^6 Cos[x]^4 + 18 (I Sin[x])^3 Cos[x]^7 + 18 (I Sin[x])^7 Cos[x]^3 + 44 (I Sin[x])^5 Cos[x]^5 + 2 (I Sin[x])^2 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^2) + Exp[-2 I y] (8 (I Sin[x])^3 Cos[x]^7 + 8 (I Sin[x])^7 Cos[x]^3 + 24 (I Sin[x])^5 Cos[x]^5 + 15 (I Sin[x])^4 Cos[x]^6 + 15 (I Sin[x])^6 Cos[x]^4 + 1 (I Sin[x])^2 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^2) + Exp[-1 I y] (10 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^4 Cos[x]^6 + 4 (I Sin[x])^6 Cos[x]^4) + Exp[0 I y] (1 (I Sin[x])^4 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^4))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-9 I y] (2 (I Sin[x])^5 Cos[x]^5) + Exp[-8 I y] (3 (I Sin[x])^4 Cos[x]^6 + 3 (I Sin[x])^6 Cos[x]^4 + 4 (I Sin[x])^3 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^3 + 2 (I Sin[x])^2 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^2) + Exp[-7 I y] (20 (I Sin[x])^4 Cos[x]^6 + 20 (I Sin[x])^6 Cos[x]^4 + 6 (I Sin[x])^3 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^3 + 20 (I Sin[x])^5 Cos[x]^5) + Exp[-6 I y] (20 (I Sin[x])^3 Cos[x]^7 + 20 (I Sin[x])^7 Cos[x]^3 + 36 (I Sin[x])^5 Cos[x]^5 + 27 (I Sin[x])^4 Cos[x]^6 + 27 (I Sin[x])^6 Cos[x]^4 + 12 (I Sin[x])^2 Cos[x]^8 + 12 (I Sin[x])^8 Cos[x]^2 + 6 (I Sin[x])^1 Cos[x]^9 + 6 (I Sin[x])^9 Cos[x]^1 + 1 Cos[x]^10 + 1 (I Sin[x])^10) + Exp[-5 I y] (64 (I Sin[x])^5 Cos[x]^5 + 46 (I Sin[x])^4 Cos[x]^6 + 46 (I Sin[x])^6 Cos[x]^4 + 32 (I Sin[x])^3 Cos[x]^7 + 32 (I Sin[x])^7 Cos[x]^3 + 14 (I Sin[x])^2 Cos[x]^8 + 14 (I Sin[x])^8 Cos[x]^2 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1) + Exp[-4 I y] (52 (I Sin[x])^4 Cos[x]^6 + 52 (I Sin[x])^6 Cos[x]^4 + 14 (I Sin[x])^2 Cos[x]^8 + 14 (I Sin[x])^8 Cos[x]^2 + 32 (I Sin[x])^3 Cos[x]^7 + 32 (I Sin[x])^7 Cos[x]^3 + 52 (I Sin[x])^5 Cos[x]^5 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1) + Exp[-3 I y] (42 (I Sin[x])^4 Cos[x]^6 + 42 (I Sin[x])^6 Cos[x]^4 + 18 (I Sin[x])^3 Cos[x]^7 + 18 (I Sin[x])^7 Cos[x]^3 + 44 (I Sin[x])^5 Cos[x]^5 + 2 (I Sin[x])^2 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^2) + Exp[-2 I y] (8 (I Sin[x])^3 Cos[x]^7 + 8 (I Sin[x])^7 Cos[x]^3 + 24 (I Sin[x])^5 Cos[x]^5 + 15 (I Sin[x])^4 Cos[x]^6 + 15 (I Sin[x])^6 Cos[x]^4 + 1 (I Sin[x])^2 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^2) + Exp[-1 I y] (10 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^4 Cos[x]^6 + 4 (I Sin[x])^6 Cos[x]^4) + Exp[0 I y] (1 (I Sin[x])^4 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^4));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = NMaximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

