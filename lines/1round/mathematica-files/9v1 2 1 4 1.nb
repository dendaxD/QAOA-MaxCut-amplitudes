amplitude2[x_,y_] := (Exp[-8 I y] (1 (I Sin[x])^6 Cos[x]^3 + 1 (I Sin[x])^3 Cos[x]^6) + Exp[-6 I y] (2 (I Sin[x])^7 Cos[x]^2 + 2 (I Sin[x])^2 Cos[x]^7 + 3 (I Sin[x])^6 Cos[x]^3 + 3 (I Sin[x])^3 Cos[x]^6 + 3 (I Sin[x])^5 Cos[x]^4 + 3 (I Sin[x])^4 Cos[x]^5) + Exp[-4 I y] (16 (I Sin[x])^5 Cos[x]^4 + 16 (I Sin[x])^4 Cos[x]^5 + 4 (I Sin[x])^7 Cos[x]^2 + 4 (I Sin[x])^2 Cos[x]^7 + 7 (I Sin[x])^6 Cos[x]^3 + 7 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^1 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^1) + Exp[-2 I y] (19 (I Sin[x])^6 Cos[x]^3 + 19 (I Sin[x])^3 Cos[x]^6 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 29 (I Sin[x])^5 Cos[x]^4 + 29 (I Sin[x])^4 Cos[x]^5 + 6 (I Sin[x])^2 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^2) + Exp[0 I y] (30 (I Sin[x])^4 Cos[x]^5 + 30 (I Sin[x])^5 Cos[x]^4 + 24 (I Sin[x])^6 Cos[x]^3 + 24 (I Sin[x])^3 Cos[x]^6 + 11 (I Sin[x])^2 Cos[x]^7 + 11 (I Sin[x])^7 Cos[x]^2 + 4 (I Sin[x])^1 Cos[x]^8 + 4 (I Sin[x])^8 Cos[x]^1 + 1 Cos[x]^9 + 1 (I Sin[x])^9) + Exp[2 I y] (33 (I Sin[x])^5 Cos[x]^4 + 33 (I Sin[x])^4 Cos[x]^5 + 6 (I Sin[x])^7 Cos[x]^2 + 6 (I Sin[x])^2 Cos[x]^7 + 17 (I Sin[x])^6 Cos[x]^3 + 17 (I Sin[x])^3 Cos[x]^6) + Exp[4 I y] (9 (I Sin[x])^5 Cos[x]^4 + 9 (I Sin[x])^4 Cos[x]^5 + 10 (I Sin[x])^3 Cos[x]^6 + 10 (I Sin[x])^6 Cos[x]^3 + 7 (I Sin[x])^2 Cos[x]^7 + 7 (I Sin[x])^7 Cos[x]^2 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1) + Exp[6 I y] (5 (I Sin[x])^4 Cos[x]^5 + 5 (I Sin[x])^5 Cos[x]^4 + 3 (I Sin[x])^6 Cos[x]^3 + 3 (I Sin[x])^3 Cos[x]^6) + Exp[8 I y] (1 (I Sin[x])^4 Cos[x]^5 + 1 (I Sin[x])^5 Cos[x]^4));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = NMaximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

