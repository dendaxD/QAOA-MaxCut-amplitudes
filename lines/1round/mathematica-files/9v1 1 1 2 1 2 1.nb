amplitude2[x_,y_] := (Exp[-8 I y] (1 (I Sin[x])^5 Cos[x]^4 + 1 (I Sin[x])^4 Cos[x]^5) + Exp[-7 I y] (3 (I Sin[x])^6 Cos[x]^3 + 3 (I Sin[x])^3 Cos[x]^6 + 5 (I Sin[x])^5 Cos[x]^4 + 5 (I Sin[x])^4 Cos[x]^5) + Exp[-6 I y] (17 (I Sin[x])^4 Cos[x]^5 + 17 (I Sin[x])^5 Cos[x]^4 + 9 (I Sin[x])^6 Cos[x]^3 + 9 (I Sin[x])^3 Cos[x]^6 + 2 (I Sin[x])^2 Cos[x]^7 + 2 (I Sin[x])^7 Cos[x]^2) + Exp[-5 I y] (35 (I Sin[x])^5 Cos[x]^4 + 35 (I Sin[x])^4 Cos[x]^5 + 5 (I Sin[x])^2 Cos[x]^7 + 5 (I Sin[x])^7 Cos[x]^2 + 16 (I Sin[x])^6 Cos[x]^3 + 16 (I Sin[x])^3 Cos[x]^6) + Exp[-4 I y] (28 (I Sin[x])^5 Cos[x]^4 + 28 (I Sin[x])^4 Cos[x]^5 + 26 (I Sin[x])^3 Cos[x]^6 + 26 (I Sin[x])^6 Cos[x]^3 + 13 (I Sin[x])^2 Cos[x]^7 + 13 (I Sin[x])^7 Cos[x]^2 + 3 (I Sin[x])^1 Cos[x]^8 + 3 (I Sin[x])^8 Cos[x]^1) + Exp[-3 I y] (25 (I Sin[x])^4 Cos[x]^5 + 25 (I Sin[x])^5 Cos[x]^4 + 21 (I Sin[x])^3 Cos[x]^6 + 21 (I Sin[x])^6 Cos[x]^3 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 8 (I Sin[x])^2 Cos[x]^7 + 8 (I Sin[x])^7 Cos[x]^2) + Exp[-2 I y] (10 (I Sin[x])^4 Cos[x]^5 + 10 (I Sin[x])^5 Cos[x]^4 + 6 (I Sin[x])^6 Cos[x]^3 + 6 (I Sin[x])^3 Cos[x]^6 + 7 (I Sin[x])^2 Cos[x]^7 + 7 (I Sin[x])^7 Cos[x]^2 + 4 (I Sin[x])^1 Cos[x]^8 + 4 (I Sin[x])^8 Cos[x]^1 + 1 Cos[x]^9 + 1 (I Sin[x])^9) + Exp[-1 I y] (5 (I Sin[x])^5 Cos[x]^4 + 5 (I Sin[x])^4 Cos[x]^5 + 2 (I Sin[x])^3 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^3 + 1 (I Sin[x])^2 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^2) + Exp[0 I y] (1 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^3));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = NMaximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

