amplitude[x_,y_] := (Exp[-8 I y] (1 (I Sin[x])^2 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^2) + Exp[-6 I y] (2 (I Sin[x])^3 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^3 + 6 (I Sin[x])^4 Cos[x]^5 + 6 (I Sin[x])^5 Cos[x]^4) + Exp[-4 I y] (12 (I Sin[x])^3 Cos[x]^6 + 12 (I Sin[x])^6 Cos[x]^3 + 7 (I Sin[x])^4 Cos[x]^5 + 7 (I Sin[x])^5 Cos[x]^4 + 3 (I Sin[x])^1 Cos[x]^8 + 3 (I Sin[x])^8 Cos[x]^1 + 6 (I Sin[x])^2 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^2) + Exp[-2 I y] (30 (I Sin[x])^4 Cos[x]^5 + 30 (I Sin[x])^5 Cos[x]^4 + 6 (I Sin[x])^2 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^2 + 20 (I Sin[x])^3 Cos[x]^6 + 20 (I Sin[x])^6 Cos[x]^3) + Exp[0 I y] (11 (I Sin[x])^2 Cos[x]^7 + 11 (I Sin[x])^7 Cos[x]^2 + 37 (I Sin[x])^4 Cos[x]^5 + 37 (I Sin[x])^5 Cos[x]^4 + 19 (I Sin[x])^3 Cos[x]^6 + 19 (I Sin[x])^6 Cos[x]^3 + 1 Cos[x]^9 + 1 (I Sin[x])^9 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1) + Exp[2 I y] (32 (I Sin[x])^5 Cos[x]^4 + 32 (I Sin[x])^4 Cos[x]^5 + 16 (I Sin[x])^3 Cos[x]^6 + 16 (I Sin[x])^6 Cos[x]^3 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 6 (I Sin[x])^2 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^2) + Exp[4 I y] (2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 11 (I Sin[x])^3 Cos[x]^6 + 11 (I Sin[x])^6 Cos[x]^3 + 12 (I Sin[x])^5 Cos[x]^4 + 12 (I Sin[x])^4 Cos[x]^5 + 3 (I Sin[x])^2 Cos[x]^7 + 3 (I Sin[x])^7 Cos[x]^2) + Exp[6 I y] (4 (I Sin[x])^6 Cos[x]^3 + 4 (I Sin[x])^3 Cos[x]^6 + 2 (I Sin[x])^4 Cos[x]^5 + 2 (I Sin[x])^5 Cos[x]^4 + 2 (I Sin[x])^2 Cos[x]^7 + 2 (I Sin[x])^7 Cos[x]^2) + Exp[8 I y] (1 (I Sin[x])^2 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^2))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-8 I y] (1 (I Sin[x])^2 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^2) + Exp[-6 I y] (2 (I Sin[x])^3 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^3 + 6 (I Sin[x])^4 Cos[x]^5 + 6 (I Sin[x])^5 Cos[x]^4) + Exp[-4 I y] (12 (I Sin[x])^3 Cos[x]^6 + 12 (I Sin[x])^6 Cos[x]^3 + 7 (I Sin[x])^4 Cos[x]^5 + 7 (I Sin[x])^5 Cos[x]^4 + 3 (I Sin[x])^1 Cos[x]^8 + 3 (I Sin[x])^8 Cos[x]^1 + 6 (I Sin[x])^2 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^2) + Exp[-2 I y] (30 (I Sin[x])^4 Cos[x]^5 + 30 (I Sin[x])^5 Cos[x]^4 + 6 (I Sin[x])^2 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^2 + 20 (I Sin[x])^3 Cos[x]^6 + 20 (I Sin[x])^6 Cos[x]^3) + Exp[0 I y] (11 (I Sin[x])^2 Cos[x]^7 + 11 (I Sin[x])^7 Cos[x]^2 + 37 (I Sin[x])^4 Cos[x]^5 + 37 (I Sin[x])^5 Cos[x]^4 + 19 (I Sin[x])^3 Cos[x]^6 + 19 (I Sin[x])^6 Cos[x]^3 + 1 Cos[x]^9 + 1 (I Sin[x])^9 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1) + Exp[2 I y] (32 (I Sin[x])^5 Cos[x]^4 + 32 (I Sin[x])^4 Cos[x]^5 + 16 (I Sin[x])^3 Cos[x]^6 + 16 (I Sin[x])^6 Cos[x]^3 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 6 (I Sin[x])^2 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^2) + Exp[4 I y] (2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 11 (I Sin[x])^3 Cos[x]^6 + 11 (I Sin[x])^6 Cos[x]^3 + 12 (I Sin[x])^5 Cos[x]^4 + 12 (I Sin[x])^4 Cos[x]^5 + 3 (I Sin[x])^2 Cos[x]^7 + 3 (I Sin[x])^7 Cos[x]^2) + Exp[6 I y] (4 (I Sin[x])^6 Cos[x]^3 + 4 (I Sin[x])^3 Cos[x]^6 + 2 (I Sin[x])^4 Cos[x]^5 + 2 (I Sin[x])^5 Cos[x]^4 + 2 (I Sin[x])^2 Cos[x]^7 + 2 (I Sin[x])^7 Cos[x]^2) + Exp[8 I y] (1 (I Sin[x])^2 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^2));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = NMaximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

