amplitude[x_,y_] := (Exp[-9 I y] (1 (I Sin[x])^4 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^4) + Exp[-7 I y] (3 (I Sin[x])^3 Cos[x]^7 + 3 (I Sin[x])^7 Cos[x]^3 + 4 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^4 Cos[x]^6 + 4 (I Sin[x])^6 Cos[x]^4) + Exp[-5 I y] (26 (I Sin[x])^5 Cos[x]^5 + 7 (I Sin[x])^3 Cos[x]^7 + 7 (I Sin[x])^7 Cos[x]^3 + 16 (I Sin[x])^4 Cos[x]^6 + 16 (I Sin[x])^6 Cos[x]^4) + Exp[-3 I y] (7 (I Sin[x])^2 Cos[x]^8 + 7 (I Sin[x])^8 Cos[x]^2 + 37 (I Sin[x])^4 Cos[x]^6 + 37 (I Sin[x])^6 Cos[x]^4 + 50 (I Sin[x])^5 Cos[x]^5 + 15 (I Sin[x])^3 Cos[x]^7 + 15 (I Sin[x])^7 Cos[x]^3) + Exp[-1 I y] (58 (I Sin[x])^6 Cos[x]^4 + 58 (I Sin[x])^4 Cos[x]^6 + 8 (I Sin[x])^2 Cos[x]^8 + 8 (I Sin[x])^8 Cos[x]^2 + 25 (I Sin[x])^3 Cos[x]^7 + 25 (I Sin[x])^7 Cos[x]^3 + 70 (I Sin[x])^5 Cos[x]^5) + Exp[1 I y] (5 (I Sin[x])^1 Cos[x]^9 + 5 (I Sin[x])^9 Cos[x]^1 + 37 (I Sin[x])^3 Cos[x]^7 + 37 (I Sin[x])^7 Cos[x]^3 + 48 (I Sin[x])^5 Cos[x]^5 + 48 (I Sin[x])^6 Cos[x]^4 + 48 (I Sin[x])^4 Cos[x]^6 + 12 (I Sin[x])^2 Cos[x]^8 + 12 (I Sin[x])^8 Cos[x]^2) + Exp[3 I y] (21 (I Sin[x])^7 Cos[x]^3 + 21 (I Sin[x])^3 Cos[x]^7 + 42 (I Sin[x])^5 Cos[x]^5 + 34 (I Sin[x])^4 Cos[x]^6 + 34 (I Sin[x])^6 Cos[x]^4 + 6 (I Sin[x])^8 Cos[x]^2 + 6 (I Sin[x])^2 Cos[x]^8 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1) + Exp[5 I y] (10 (I Sin[x])^2 Cos[x]^8 + 10 (I Sin[x])^8 Cos[x]^2 + 9 (I Sin[x])^4 Cos[x]^6 + 9 (I Sin[x])^6 Cos[x]^4 + 9 (I Sin[x])^7 Cos[x]^3 + 9 (I Sin[x])^3 Cos[x]^7 + 10 (I Sin[x])^5 Cos[x]^5 + 1 Cos[x]^10 + 1 (I Sin[x])^10 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1) + Exp[7 I y] (2 (I Sin[x])^8 Cos[x]^2 + 2 (I Sin[x])^2 Cos[x]^8 + 3 (I Sin[x])^6 Cos[x]^4 + 3 (I Sin[x])^4 Cos[x]^6 + 3 (I Sin[x])^3 Cos[x]^7 + 3 (I Sin[x])^7 Cos[x]^3 + 2 (I Sin[x])^5 Cos[x]^5) + Exp[9 I y] (1 (I Sin[x])^1 Cos[x]^9 + 1 (I Sin[x])^9 Cos[x]^1))/Sqrt[2^nqubits];
