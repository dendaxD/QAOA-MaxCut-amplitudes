nqubits = 8;
name = "8v3 1 3 1";
nstates = 4;

amplitude[x_,y_] := (Exp[-7 I y] (1 (I Sin[x])^2 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^2) + Exp[-6 I y] (1 (I Sin[x])^1 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^1 + 3 (I Sin[x])^5 Cos[x]^3 + 3 (I Sin[x])^3 Cos[x]^5 + 1 (I Sin[x])^2 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^2 + 4 (I Sin[x])^4 Cos[x]^4) + Exp[-5 I y] (11 (I Sin[x])^3 Cos[x]^5 + 11 (I Sin[x])^5 Cos[x]^3 + 12 (I Sin[x])^4 Cos[x]^4 + 1 (I Sin[x])^1 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^1 + 3 (I Sin[x])^2 Cos[x]^6 + 3 (I Sin[x])^6 Cos[x]^2) + Exp[-4 I y] (11 (I Sin[x])^2 Cos[x]^6 + 11 (I Sin[x])^6 Cos[x]^2 + 14 (I Sin[x])^4 Cos[x]^4 + 13 (I Sin[x])^3 Cos[x]^5 + 13 (I Sin[x])^5 Cos[x]^3 + 1 Cos[x]^8 + 1 (I Sin[x])^8 + 3 (I Sin[x])^1 Cos[x]^7 + 3 (I Sin[x])^7 Cos[x]^1) + Exp[-3 I y] (24 (I Sin[x])^4 Cos[x]^4 + 7 (I Sin[x])^2 Cos[x]^6 + 7 (I Sin[x])^6 Cos[x]^2 + 15 (I Sin[x])^3 Cos[x]^5 + 15 (I Sin[x])^5 Cos[x]^3 + 1 (I Sin[x])^7 Cos[x]^1 + 1 (I Sin[x])^1 Cos[x]^7) + Exp[-2 I y] (2 (I Sin[x])^1 Cos[x]^7 + 2 (I Sin[x])^7 Cos[x]^1 + 10 (I Sin[x])^3 Cos[x]^5 + 10 (I Sin[x])^5 Cos[x]^3 + 12 (I Sin[x])^4 Cos[x]^4 + 3 (I Sin[x])^2 Cos[x]^6 + 3 (I Sin[x])^6 Cos[x]^2) + Exp[-1 I y] (4 (I Sin[x])^5 Cos[x]^3 + 4 (I Sin[x])^3 Cos[x]^5 + 4 (I Sin[x])^4 Cos[x]^4 + 1 (I Sin[x])^6 Cos[x]^2 + 1 (I Sin[x])^2 Cos[x]^6) + Exp[0 I y] (1 (I Sin[x])^2 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^2))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-7 I y] (1 (I Sin[x])^2 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^2) + Exp[-6 I y] (1 (I Sin[x])^1 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^1 + 3 (I Sin[x])^5 Cos[x]^3 + 3 (I Sin[x])^3 Cos[x]^5 + 1 (I Sin[x])^2 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^2 + 4 (I Sin[x])^4 Cos[x]^4) + Exp[-5 I y] (11 (I Sin[x])^3 Cos[x]^5 + 11 (I Sin[x])^5 Cos[x]^3 + 12 (I Sin[x])^4 Cos[x]^4 + 1 (I Sin[x])^1 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^1 + 3 (I Sin[x])^2 Cos[x]^6 + 3 (I Sin[x])^6 Cos[x]^2) + Exp[-4 I y] (11 (I Sin[x])^2 Cos[x]^6 + 11 (I Sin[x])^6 Cos[x]^2 + 14 (I Sin[x])^4 Cos[x]^4 + 13 (I Sin[x])^3 Cos[x]^5 + 13 (I Sin[x])^5 Cos[x]^3 + 1 Cos[x]^8 + 1 (I Sin[x])^8 + 3 (I Sin[x])^1 Cos[x]^7 + 3 (I Sin[x])^7 Cos[x]^1) + Exp[-3 I y] (24 (I Sin[x])^4 Cos[x]^4 + 7 (I Sin[x])^2 Cos[x]^6 + 7 (I Sin[x])^6 Cos[x]^2 + 15 (I Sin[x])^3 Cos[x]^5 + 15 (I Sin[x])^5 Cos[x]^3 + 1 (I Sin[x])^7 Cos[x]^1 + 1 (I Sin[x])^1 Cos[x]^7) + Exp[-2 I y] (2 (I Sin[x])^1 Cos[x]^7 + 2 (I Sin[x])^7 Cos[x]^1 + 10 (I Sin[x])^3 Cos[x]^5 + 10 (I Sin[x])^5 Cos[x]^3 + 12 (I Sin[x])^4 Cos[x]^4 + 3 (I Sin[x])^2 Cos[x]^6 + 3 (I Sin[x])^6 Cos[x]^2) + Exp[-1 I y] (4 (I Sin[x])^5 Cos[x]^3 + 4 (I Sin[x])^3 Cos[x]^5 + 4 (I Sin[x])^4 Cos[x]^4 + 1 (I Sin[x])^6 Cos[x]^2 + 1 (I Sin[x])^2 Cos[x]^6) + Exp[0 I y] (1 (I Sin[x])^2 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^2));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = NMaximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

