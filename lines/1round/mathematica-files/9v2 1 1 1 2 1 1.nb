amplitude[x_,y_] := (Exp[-8 I y] (1 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^3) + Exp[-7 I y] (7 (I Sin[x])^4 Cos[x]^5 + 7 (I Sin[x])^5 Cos[x]^4 + 1 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^3) + Exp[-6 I y] (4 (I Sin[x])^2 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^2 + 15 (I Sin[x])^4 Cos[x]^5 + 15 (I Sin[x])^5 Cos[x]^4 + 9 (I Sin[x])^3 Cos[x]^6 + 9 (I Sin[x])^6 Cos[x]^3) + Exp[-5 I y] (33 (I Sin[x])^5 Cos[x]^4 + 33 (I Sin[x])^4 Cos[x]^5 + 20 (I Sin[x])^6 Cos[x]^3 + 20 (I Sin[x])^3 Cos[x]^6 + 3 (I Sin[x])^2 Cos[x]^7 + 3 (I Sin[x])^7 Cos[x]^2) + Exp[-4 I y] (23 (I Sin[x])^3 Cos[x]^6 + 23 (I Sin[x])^6 Cos[x]^3 + 31 (I Sin[x])^5 Cos[x]^4 + 31 (I Sin[x])^4 Cos[x]^5 + 4 (I Sin[x])^1 Cos[x]^8 + 4 (I Sin[x])^8 Cos[x]^1 + 12 (I Sin[x])^2 Cos[x]^7 + 12 (I Sin[x])^7 Cos[x]^2) + Exp[-3 I y] (28 (I Sin[x])^4 Cos[x]^5 + 28 (I Sin[x])^5 Cos[x]^4 + 9 (I Sin[x])^7 Cos[x]^2 + 9 (I Sin[x])^2 Cos[x]^7 + 18 (I Sin[x])^6 Cos[x]^3 + 18 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^1 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^1) + Exp[-2 I y] (9 (I Sin[x])^4 Cos[x]^5 + 9 (I Sin[x])^5 Cos[x]^4 + 9 (I Sin[x])^6 Cos[x]^3 + 9 (I Sin[x])^3 Cos[x]^6 + 6 (I Sin[x])^2 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^2 + 1 Cos[x]^9 + 1 (I Sin[x])^9 + 3 (I Sin[x])^1 Cos[x]^8 + 3 (I Sin[x])^8 Cos[x]^1) + Exp[-1 I y] (3 (I Sin[x])^3 Cos[x]^6 + 3 (I Sin[x])^6 Cos[x]^3 + 2 (I Sin[x])^5 Cos[x]^4 + 2 (I Sin[x])^4 Cos[x]^5 + 1 (I Sin[x])^8 Cos[x]^1 + 1 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^7 Cos[x]^2 + 2 (I Sin[x])^2 Cos[x]^7) + Exp[0 I y] (1 (I Sin[x])^5 Cos[x]^4 + 1 (I Sin[x])^4 Cos[x]^5))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-8 I y] (1 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^3) + Exp[-7 I y] (7 (I Sin[x])^4 Cos[x]^5 + 7 (I Sin[x])^5 Cos[x]^4 + 1 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^3) + Exp[-6 I y] (4 (I Sin[x])^2 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^2 + 15 (I Sin[x])^4 Cos[x]^5 + 15 (I Sin[x])^5 Cos[x]^4 + 9 (I Sin[x])^3 Cos[x]^6 + 9 (I Sin[x])^6 Cos[x]^3) + Exp[-5 I y] (33 (I Sin[x])^5 Cos[x]^4 + 33 (I Sin[x])^4 Cos[x]^5 + 20 (I Sin[x])^6 Cos[x]^3 + 20 (I Sin[x])^3 Cos[x]^6 + 3 (I Sin[x])^2 Cos[x]^7 + 3 (I Sin[x])^7 Cos[x]^2) + Exp[-4 I y] (23 (I Sin[x])^3 Cos[x]^6 + 23 (I Sin[x])^6 Cos[x]^3 + 31 (I Sin[x])^5 Cos[x]^4 + 31 (I Sin[x])^4 Cos[x]^5 + 4 (I Sin[x])^1 Cos[x]^8 + 4 (I Sin[x])^8 Cos[x]^1 + 12 (I Sin[x])^2 Cos[x]^7 + 12 (I Sin[x])^7 Cos[x]^2) + Exp[-3 I y] (28 (I Sin[x])^4 Cos[x]^5 + 28 (I Sin[x])^5 Cos[x]^4 + 9 (I Sin[x])^7 Cos[x]^2 + 9 (I Sin[x])^2 Cos[x]^7 + 18 (I Sin[x])^6 Cos[x]^3 + 18 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^1 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^1) + Exp[-2 I y] (9 (I Sin[x])^4 Cos[x]^5 + 9 (I Sin[x])^5 Cos[x]^4 + 9 (I Sin[x])^6 Cos[x]^3 + 9 (I Sin[x])^3 Cos[x]^6 + 6 (I Sin[x])^2 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^2 + 1 Cos[x]^9 + 1 (I Sin[x])^9 + 3 (I Sin[x])^1 Cos[x]^8 + 3 (I Sin[x])^8 Cos[x]^1) + Exp[-1 I y] (3 (I Sin[x])^3 Cos[x]^6 + 3 (I Sin[x])^6 Cos[x]^3 + 2 (I Sin[x])^5 Cos[x]^4 + 2 (I Sin[x])^4 Cos[x]^5 + 1 (I Sin[x])^8 Cos[x]^1 + 1 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^7 Cos[x]^2 + 2 (I Sin[x])^2 Cos[x]^7) + Exp[0 I y] (1 (I Sin[x])^5 Cos[x]^4 + 1 (I Sin[x])^4 Cos[x]^5));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = NMaximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

