nqubits = 8;
name = "8v1 2 4 1";
nstates = 4;

amplitude[x_,y_] := (Exp[-7 I y] (1 (I Sin[x])^3 Cos[x]^5 + 1 (I Sin[x])^5 Cos[x]^3) + Exp[-5 I y] (2 (I Sin[x])^2 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^2 + 4 (I Sin[x])^4 Cos[x]^4 + 3 (I Sin[x])^3 Cos[x]^5 + 3 (I Sin[x])^5 Cos[x]^3) + Exp[-3 I y] (12 (I Sin[x])^4 Cos[x]^4 + 6 (I Sin[x])^2 Cos[x]^6 + 6 (I Sin[x])^6 Cos[x]^2 + 7 (I Sin[x])^5 Cos[x]^3 + 7 (I Sin[x])^3 Cos[x]^5 + 2 (I Sin[x])^1 Cos[x]^7 + 2 (I Sin[x])^7 Cos[x]^1) + Exp[-1 I y] (15 (I Sin[x])^3 Cos[x]^5 + 15 (I Sin[x])^5 Cos[x]^3 + 18 (I Sin[x])^4 Cos[x]^4 + 6 (I Sin[x])^2 Cos[x]^6 + 6 (I Sin[x])^6 Cos[x]^2 + 4 (I Sin[x])^1 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^1 + 1 Cos[x]^8 + 1 (I Sin[x])^8) + Exp[1 I y] (19 (I Sin[x])^5 Cos[x]^3 + 19 (I Sin[x])^3 Cos[x]^5 + 6 (I Sin[x])^6 Cos[x]^2 + 6 (I Sin[x])^2 Cos[x]^6 + 20 (I Sin[x])^4 Cos[x]^4) + Exp[3 I y] (8 (I Sin[x])^4 Cos[x]^4 + 8 (I Sin[x])^2 Cos[x]^6 + 8 (I Sin[x])^6 Cos[x]^2 + 7 (I Sin[x])^3 Cos[x]^5 + 7 (I Sin[x])^5 Cos[x]^3 + 2 (I Sin[x])^1 Cos[x]^7 + 2 (I Sin[x])^7 Cos[x]^1) + Exp[5 I y] (8 (I Sin[x])^4 Cos[x]^4 + 3 (I Sin[x])^5 Cos[x]^3 + 3 (I Sin[x])^3 Cos[x]^5) + Exp[7 I y] (1 (I Sin[x])^3 Cos[x]^5 + 1 (I Sin[x])^5 Cos[x]^3))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-7 I y] (1 (I Sin[x])^3 Cos[x]^5 + 1 (I Sin[x])^5 Cos[x]^3) + Exp[-5 I y] (2 (I Sin[x])^2 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^2 + 4 (I Sin[x])^4 Cos[x]^4 + 3 (I Sin[x])^3 Cos[x]^5 + 3 (I Sin[x])^5 Cos[x]^3) + Exp[-3 I y] (12 (I Sin[x])^4 Cos[x]^4 + 6 (I Sin[x])^2 Cos[x]^6 + 6 (I Sin[x])^6 Cos[x]^2 + 7 (I Sin[x])^5 Cos[x]^3 + 7 (I Sin[x])^3 Cos[x]^5 + 2 (I Sin[x])^1 Cos[x]^7 + 2 (I Sin[x])^7 Cos[x]^1) + Exp[-1 I y] (15 (I Sin[x])^3 Cos[x]^5 + 15 (I Sin[x])^5 Cos[x]^3 + 18 (I Sin[x])^4 Cos[x]^4 + 6 (I Sin[x])^2 Cos[x]^6 + 6 (I Sin[x])^6 Cos[x]^2 + 4 (I Sin[x])^1 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^1 + 1 Cos[x]^8 + 1 (I Sin[x])^8) + Exp[1 I y] (19 (I Sin[x])^5 Cos[x]^3 + 19 (I Sin[x])^3 Cos[x]^5 + 6 (I Sin[x])^6 Cos[x]^2 + 6 (I Sin[x])^2 Cos[x]^6 + 20 (I Sin[x])^4 Cos[x]^4) + Exp[3 I y] (8 (I Sin[x])^4 Cos[x]^4 + 8 (I Sin[x])^2 Cos[x]^6 + 8 (I Sin[x])^6 Cos[x]^2 + 7 (I Sin[x])^3 Cos[x]^5 + 7 (I Sin[x])^5 Cos[x]^3 + 2 (I Sin[x])^1 Cos[x]^7 + 2 (I Sin[x])^7 Cos[x]^1) + Exp[5 I y] (8 (I Sin[x])^4 Cos[x]^4 + 3 (I Sin[x])^5 Cos[x]^3 + 3 (I Sin[x])^3 Cos[x]^5) + Exp[7 I y] (1 (I Sin[x])^3 Cos[x]^5 + 1 (I Sin[x])^5 Cos[x]^3));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = NMaximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

f = probability[c, d]; n = Pi;
