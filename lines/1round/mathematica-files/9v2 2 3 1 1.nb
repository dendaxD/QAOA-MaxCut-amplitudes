amplitude2[x_,y_] := (Exp[-8 I y] (1 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^3) + Exp[-7 I y] (6 (I Sin[x])^4 Cos[x]^5 + 6 (I Sin[x])^5 Cos[x]^4 + 2 (I Sin[x])^3 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^3) + Exp[-6 I y] (5 (I Sin[x])^2 Cos[x]^7 + 5 (I Sin[x])^7 Cos[x]^2 + 15 (I Sin[x])^4 Cos[x]^5 + 15 (I Sin[x])^5 Cos[x]^4 + 7 (I Sin[x])^3 Cos[x]^6 + 7 (I Sin[x])^6 Cos[x]^3 + 1 (I Sin[x])^1 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^1) + Exp[-5 I y] (27 (I Sin[x])^5 Cos[x]^4 + 27 (I Sin[x])^4 Cos[x]^5 + 20 (I Sin[x])^6 Cos[x]^3 + 20 (I Sin[x])^3 Cos[x]^6 + 8 (I Sin[x])^7 Cos[x]^2 + 8 (I Sin[x])^2 Cos[x]^7 + 1 (I Sin[x])^1 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^1) + Exp[-4 I y] (26 (I Sin[x])^3 Cos[x]^6 + 26 (I Sin[x])^6 Cos[x]^3 + 26 (I Sin[x])^5 Cos[x]^4 + 26 (I Sin[x])^4 Cos[x]^5 + 5 (I Sin[x])^1 Cos[x]^8 + 5 (I Sin[x])^8 Cos[x]^1 + 12 (I Sin[x])^2 Cos[x]^7 + 12 (I Sin[x])^7 Cos[x]^2 + 1 Cos[x]^9 + 1 (I Sin[x])^9) + Exp[-3 I y] (17 (I Sin[x])^6 Cos[x]^3 + 17 (I Sin[x])^3 Cos[x]^6 + 33 (I Sin[x])^5 Cos[x]^4 + 33 (I Sin[x])^4 Cos[x]^5 + 5 (I Sin[x])^2 Cos[x]^7 + 5 (I Sin[x])^7 Cos[x]^2 + 1 (I Sin[x])^8 Cos[x]^1 + 1 (I Sin[x])^1 Cos[x]^8) + Exp[-2 I y] (5 (I Sin[x])^2 Cos[x]^7 + 5 (I Sin[x])^7 Cos[x]^2 + 15 (I Sin[x])^4 Cos[x]^5 + 15 (I Sin[x])^5 Cos[x]^4 + 7 (I Sin[x])^6 Cos[x]^3 + 7 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^1 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^1) + Exp[-1 I y] (4 (I Sin[x])^5 Cos[x]^4 + 4 (I Sin[x])^4 Cos[x]^5 + 1 (I Sin[x])^7 Cos[x]^2 + 1 (I Sin[x])^2 Cos[x]^7 + 3 (I Sin[x])^6 Cos[x]^3 + 3 (I Sin[x])^3 Cos[x]^6) + Exp[0 I y] (1 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^3));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = NMaximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

