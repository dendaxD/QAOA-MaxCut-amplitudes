amplitude2[x_,y_] := (Exp[-9 I y] (2 (I Sin[x])^5 Cos[x]^5) + Exp[-7 I y] (5 (I Sin[x])^6 Cos[x]^4 + 5 (I Sin[x])^4 Cos[x]^6 + 8 (I Sin[x])^5 Cos[x]^5) + Exp[-5 I y] (20 (I Sin[x])^4 Cos[x]^6 + 20 (I Sin[x])^6 Cos[x]^4 + 4 (I Sin[x])^3 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^3 + 24 (I Sin[x])^5 Cos[x]^5) + Exp[-3 I y] (56 (I Sin[x])^5 Cos[x]^5 + 16 (I Sin[x])^3 Cos[x]^7 + 16 (I Sin[x])^7 Cos[x]^3 + 40 (I Sin[x])^6 Cos[x]^4 + 40 (I Sin[x])^4 Cos[x]^6) + Exp[-1 I y] (56 (I Sin[x])^5 Cos[x]^5 + 38 (I Sin[x])^3 Cos[x]^7 + 38 (I Sin[x])^7 Cos[x]^3 + 10 (I Sin[x])^2 Cos[x]^8 + 10 (I Sin[x])^8 Cos[x]^2 + 50 (I Sin[x])^6 Cos[x]^4 + 50 (I Sin[x])^4 Cos[x]^6) + Exp[1 I y] (55 (I Sin[x])^4 Cos[x]^6 + 55 (I Sin[x])^6 Cos[x]^4 + 11 (I Sin[x])^2 Cos[x]^8 + 11 (I Sin[x])^8 Cos[x]^2 + 28 (I Sin[x])^7 Cos[x]^3 + 28 (I Sin[x])^3 Cos[x]^7 + 64 (I Sin[x])^5 Cos[x]^5) + Exp[3 I y] (26 (I Sin[x])^6 Cos[x]^4 + 26 (I Sin[x])^4 Cos[x]^6 + 18 (I Sin[x])^2 Cos[x]^8 + 18 (I Sin[x])^8 Cos[x]^2 + 6 (I Sin[x])^1 Cos[x]^9 + 6 (I Sin[x])^9 Cos[x]^1 + 20 (I Sin[x])^7 Cos[x]^3 + 20 (I Sin[x])^3 Cos[x]^7 + 28 (I Sin[x])^5 Cos[x]^5) + Exp[5 I y] (12 (I Sin[x])^3 Cos[x]^7 + 12 (I Sin[x])^7 Cos[x]^3 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1 + 12 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^8 Cos[x]^2 + 4 (I Sin[x])^2 Cos[x]^8 + 12 (I Sin[x])^6 Cos[x]^4 + 12 (I Sin[x])^4 Cos[x]^6) + Exp[7 I y] (2 (I Sin[x])^7 Cos[x]^3 + 2 (I Sin[x])^3 Cos[x]^7 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1 + 2 (I Sin[x])^5 Cos[x]^5 + 1 Cos[x]^10 + 1 (I Sin[x])^10 + 2 (I Sin[x])^6 Cos[x]^4 + 2 (I Sin[x])^4 Cos[x]^6 + 1 (I Sin[x])^2 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^2) + Exp[9 I y] (1 (I Sin[x])^2 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^2));
