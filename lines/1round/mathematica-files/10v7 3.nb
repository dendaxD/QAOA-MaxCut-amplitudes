amplitude[x_,y_] := (Exp[-9 I y] (1 (I Sin[x])^3 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^3) + Exp[-7 I y] (2 (I Sin[x])^2 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^2 + 2 (I Sin[x])^6 Cos[x]^4 + 2 (I Sin[x])^4 Cos[x]^6 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1 + 2 (I Sin[x])^5 Cos[x]^5 + 1 Cos[x]^10 + 1 (I Sin[x])^10 + 1 (I Sin[x])^3 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^3) + Exp[-5 I y] (6 (I Sin[x])^2 Cos[x]^8 + 6 (I Sin[x])^8 Cos[x]^2 + 14 (I Sin[x])^4 Cos[x]^6 + 14 (I Sin[x])^6 Cos[x]^4 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1 + 14 (I Sin[x])^5 Cos[x]^5 + 7 (I Sin[x])^3 Cos[x]^7 + 7 (I Sin[x])^7 Cos[x]^3) + Exp[-3 I y] (6 (I Sin[x])^1 Cos[x]^9 + 6 (I Sin[x])^9 Cos[x]^1 + 26 (I Sin[x])^5 Cos[x]^5 + 25 (I Sin[x])^3 Cos[x]^7 + 25 (I Sin[x])^7 Cos[x]^3 + 16 (I Sin[x])^2 Cos[x]^8 + 16 (I Sin[x])^8 Cos[x]^2 + 24 (I Sin[x])^4 Cos[x]^6 + 24 (I Sin[x])^6 Cos[x]^4) + Exp[-1 I y] (35 (I Sin[x])^3 Cos[x]^7 + 35 (I Sin[x])^7 Cos[x]^3 + 62 (I Sin[x])^5 Cos[x]^5 + 10 (I Sin[x])^2 Cos[x]^8 + 10 (I Sin[x])^8 Cos[x]^2 + 50 (I Sin[x])^6 Cos[x]^4 + 50 (I Sin[x])^4 Cos[x]^6) + Exp[1 I y] (11 (I Sin[x])^2 Cos[x]^8 + 11 (I Sin[x])^8 Cos[x]^2 + 55 (I Sin[x])^4 Cos[x]^6 + 55 (I Sin[x])^6 Cos[x]^4 + 31 (I Sin[x])^3 Cos[x]^7 + 31 (I Sin[x])^7 Cos[x]^3 + 58 (I Sin[x])^5 Cos[x]^5) + Exp[3 I y] (44 (I Sin[x])^4 Cos[x]^6 + 44 (I Sin[x])^6 Cos[x]^4 + 13 (I Sin[x])^3 Cos[x]^7 + 13 (I Sin[x])^7 Cos[x]^3 + 54 (I Sin[x])^5 Cos[x]^5) + Exp[5 I y] (7 (I Sin[x])^3 Cos[x]^7 + 7 (I Sin[x])^7 Cos[x]^3 + 26 (I Sin[x])^5 Cos[x]^5 + 16 (I Sin[x])^4 Cos[x]^6 + 16 (I Sin[x])^6 Cos[x]^4) + Exp[7 I y] (10 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^4 Cos[x]^6 + 4 (I Sin[x])^6 Cos[x]^4) + Exp[9 I y] (1 (I Sin[x])^4 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^4))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-9 I y] (1 (I Sin[x])^3 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^3) + Exp[-7 I y] (2 (I Sin[x])^2 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^2 + 2 (I Sin[x])^6 Cos[x]^4 + 2 (I Sin[x])^4 Cos[x]^6 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1 + 2 (I Sin[x])^5 Cos[x]^5 + 1 Cos[x]^10 + 1 (I Sin[x])^10 + 1 (I Sin[x])^3 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^3) + Exp[-5 I y] (6 (I Sin[x])^2 Cos[x]^8 + 6 (I Sin[x])^8 Cos[x]^2 + 14 (I Sin[x])^4 Cos[x]^6 + 14 (I Sin[x])^6 Cos[x]^4 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1 + 14 (I Sin[x])^5 Cos[x]^5 + 7 (I Sin[x])^3 Cos[x]^7 + 7 (I Sin[x])^7 Cos[x]^3) + Exp[-3 I y] (6 (I Sin[x])^1 Cos[x]^9 + 6 (I Sin[x])^9 Cos[x]^1 + 26 (I Sin[x])^5 Cos[x]^5 + 25 (I Sin[x])^3 Cos[x]^7 + 25 (I Sin[x])^7 Cos[x]^3 + 16 (I Sin[x])^2 Cos[x]^8 + 16 (I Sin[x])^8 Cos[x]^2 + 24 (I Sin[x])^4 Cos[x]^6 + 24 (I Sin[x])^6 Cos[x]^4) + Exp[-1 I y] (35 (I Sin[x])^3 Cos[x]^7 + 35 (I Sin[x])^7 Cos[x]^3 + 62 (I Sin[x])^5 Cos[x]^5 + 10 (I Sin[x])^2 Cos[x]^8 + 10 (I Sin[x])^8 Cos[x]^2 + 50 (I Sin[x])^6 Cos[x]^4 + 50 (I Sin[x])^4 Cos[x]^6) + Exp[1 I y] (11 (I Sin[x])^2 Cos[x]^8 + 11 (I Sin[x])^8 Cos[x]^2 + 55 (I Sin[x])^4 Cos[x]^6 + 55 (I Sin[x])^6 Cos[x]^4 + 31 (I Sin[x])^3 Cos[x]^7 + 31 (I Sin[x])^7 Cos[x]^3 + 58 (I Sin[x])^5 Cos[x]^5) + Exp[3 I y] (44 (I Sin[x])^4 Cos[x]^6 + 44 (I Sin[x])^6 Cos[x]^4 + 13 (I Sin[x])^3 Cos[x]^7 + 13 (I Sin[x])^7 Cos[x]^3 + 54 (I Sin[x])^5 Cos[x]^5) + Exp[5 I y] (7 (I Sin[x])^3 Cos[x]^7 + 7 (I Sin[x])^7 Cos[x]^3 + 26 (I Sin[x])^5 Cos[x]^5 + 16 (I Sin[x])^4 Cos[x]^6 + 16 (I Sin[x])^6 Cos[x]^4) + Exp[7 I y] (10 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^4 Cos[x]^6 + 4 (I Sin[x])^6 Cos[x]^4) + Exp[9 I y] (1 (I Sin[x])^4 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^4));
