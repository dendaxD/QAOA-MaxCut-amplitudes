nstates = 4;

amplitude[x_,y_] := (Exp[-9 I y] (1 (I Sin[x])^2 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^2) + Exp[-7 I y] (2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1 + 2 (I Sin[x])^7 Cos[x]^3 + 2 (I Sin[x])^3 Cos[x]^7 + 1 Cos[x]^10 + 1 (I Sin[x])^10 + 2 (I Sin[x])^6 Cos[x]^4 + 2 (I Sin[x])^4 Cos[x]^6 + 2 (I Sin[x])^5 Cos[x]^5 + 1 (I Sin[x])^2 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^2) + Exp[-5 I y] (2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1 + 12 (I Sin[x])^3 Cos[x]^7 + 12 (I Sin[x])^7 Cos[x]^3 + 4 (I Sin[x])^2 Cos[x]^8 + 4 (I Sin[x])^8 Cos[x]^2 + 12 (I Sin[x])^4 Cos[x]^6 + 12 (I Sin[x])^6 Cos[x]^4 + 12 (I Sin[x])^5 Cos[x]^5) + Exp[-3 I y] (18 (I Sin[x])^2 Cos[x]^8 + 18 (I Sin[x])^8 Cos[x]^2 + 26 (I Sin[x])^6 Cos[x]^4 + 26 (I Sin[x])^4 Cos[x]^6 + 6 (I Sin[x])^1 Cos[x]^9 + 6 (I Sin[x])^9 Cos[x]^1 + 28 (I Sin[x])^5 Cos[x]^5 + 20 (I Sin[x])^3 Cos[x]^7 + 20 (I Sin[x])^7 Cos[x]^3) + Exp[-1 I y] (11 (I Sin[x])^2 Cos[x]^8 + 11 (I Sin[x])^8 Cos[x]^2 + 55 (I Sin[x])^4 Cos[x]^6 + 55 (I Sin[x])^6 Cos[x]^4 + 28 (I Sin[x])^3 Cos[x]^7 + 28 (I Sin[x])^7 Cos[x]^3 + 64 (I Sin[x])^5 Cos[x]^5) + Exp[1 I y] (38 (I Sin[x])^3 Cos[x]^7 + 38 (I Sin[x])^7 Cos[x]^3 + 56 (I Sin[x])^5 Cos[x]^5 + 10 (I Sin[x])^2 Cos[x]^8 + 10 (I Sin[x])^8 Cos[x]^2 + 50 (I Sin[x])^4 Cos[x]^6 + 50 (I Sin[x])^6 Cos[x]^4) + Exp[3 I y] (16 (I Sin[x])^3 Cos[x]^7 + 16 (I Sin[x])^7 Cos[x]^3 + 56 (I Sin[x])^5 Cos[x]^5 + 40 (I Sin[x])^4 Cos[x]^6 + 40 (I Sin[x])^6 Cos[x]^4) + Exp[5 I y] (20 (I Sin[x])^4 Cos[x]^6 + 20 (I Sin[x])^6 Cos[x]^4 + 4 (I Sin[x])^3 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^3 + 24 (I Sin[x])^5 Cos[x]^5) + Exp[7 I y] (5 (I Sin[x])^4 Cos[x]^6 + 5 (I Sin[x])^6 Cos[x]^4 + 8 (I Sin[x])^5 Cos[x]^5) + Exp[9 I y] (2 (I Sin[x])^5 Cos[x]^5))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-9 I y] (1 (I Sin[x])^2 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^2) + Exp[-7 I y] (2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1 + 2 (I Sin[x])^7 Cos[x]^3 + 2 (I Sin[x])^3 Cos[x]^7 + 1 Cos[x]^10 + 1 (I Sin[x])^10 + 2 (I Sin[x])^6 Cos[x]^4 + 2 (I Sin[x])^4 Cos[x]^6 + 2 (I Sin[x])^5 Cos[x]^5 + 1 (I Sin[x])^2 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^2) + Exp[-5 I y] (2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1 + 12 (I Sin[x])^3 Cos[x]^7 + 12 (I Sin[x])^7 Cos[x]^3 + 4 (I Sin[x])^2 Cos[x]^8 + 4 (I Sin[x])^8 Cos[x]^2 + 12 (I Sin[x])^4 Cos[x]^6 + 12 (I Sin[x])^6 Cos[x]^4 + 12 (I Sin[x])^5 Cos[x]^5) + Exp[-3 I y] (18 (I Sin[x])^2 Cos[x]^8 + 18 (I Sin[x])^8 Cos[x]^2 + 26 (I Sin[x])^6 Cos[x]^4 + 26 (I Sin[x])^4 Cos[x]^6 + 6 (I Sin[x])^1 Cos[x]^9 + 6 (I Sin[x])^9 Cos[x]^1 + 28 (I Sin[x])^5 Cos[x]^5 + 20 (I Sin[x])^3 Cos[x]^7 + 20 (I Sin[x])^7 Cos[x]^3) + Exp[-1 I y] (11 (I Sin[x])^2 Cos[x]^8 + 11 (I Sin[x])^8 Cos[x]^2 + 55 (I Sin[x])^4 Cos[x]^6 + 55 (I Sin[x])^6 Cos[x]^4 + 28 (I Sin[x])^3 Cos[x]^7 + 28 (I Sin[x])^7 Cos[x]^3 + 64 (I Sin[x])^5 Cos[x]^5) + Exp[1 I y] (38 (I Sin[x])^3 Cos[x]^7 + 38 (I Sin[x])^7 Cos[x]^3 + 56 (I Sin[x])^5 Cos[x]^5 + 10 (I Sin[x])^2 Cos[x]^8 + 10 (I Sin[x])^8 Cos[x]^2 + 50 (I Sin[x])^4 Cos[x]^6 + 50 (I Sin[x])^6 Cos[x]^4) + Exp[3 I y] (16 (I Sin[x])^3 Cos[x]^7 + 16 (I Sin[x])^7 Cos[x]^3 + 56 (I Sin[x])^5 Cos[x]^5 + 40 (I Sin[x])^4 Cos[x]^6 + 40 (I Sin[x])^6 Cos[x]^4) + Exp[5 I y] (20 (I Sin[x])^4 Cos[x]^6 + 20 (I Sin[x])^6 Cos[x]^4 + 4 (I Sin[x])^3 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^3 + 24 (I Sin[x])^5 Cos[x]^5) + Exp[7 I y] (5 (I Sin[x])^4 Cos[x]^6 + 5 (I Sin[x])^6 Cos[x]^4 + 8 (I Sin[x])^5 Cos[x]^5) + Exp[9 I y] (2 (I Sin[x])^5 Cos[x]^5));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = NMaximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

