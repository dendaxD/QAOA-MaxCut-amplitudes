amplitude[x_,y_] := (Exp[-9 I y] (2 (I Sin[x])^5 Cos[x]^5) + Exp[-7 I y] (4 (I Sin[x])^4 Cos[x]^6 + 4 (I Sin[x])^6 Cos[x]^4 + 4 (I Sin[x])^3 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^3 + 1 (I Sin[x])^2 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^2) + Exp[-5 I y] (20 (I Sin[x])^4 Cos[x]^6 + 20 (I Sin[x])^6 Cos[x]^4 + 24 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^3 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^3) + Exp[-3 I y] (44 (I Sin[x])^5 Cos[x]^5 + 20 (I Sin[x])^3 Cos[x]^7 + 20 (I Sin[x])^7 Cos[x]^3 + 10 (I Sin[x])^2 Cos[x]^8 + 10 (I Sin[x])^8 Cos[x]^2 + 30 (I Sin[x])^4 Cos[x]^6 + 30 (I Sin[x])^6 Cos[x]^4 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1) + Exp[-1 I y] (30 (I Sin[x])^3 Cos[x]^7 + 30 (I Sin[x])^7 Cos[x]^3 + 72 (I Sin[x])^5 Cos[x]^5 + 54 (I Sin[x])^4 Cos[x]^6 + 54 (I Sin[x])^6 Cos[x]^4 + 6 (I Sin[x])^2 Cos[x]^8 + 6 (I Sin[x])^8 Cos[x]^2) + Exp[1 I y] (49 (I Sin[x])^4 Cos[x]^6 + 49 (I Sin[x])^6 Cos[x]^4 + 32 (I Sin[x])^3 Cos[x]^7 + 32 (I Sin[x])^7 Cos[x]^3 + 44 (I Sin[x])^5 Cos[x]^5 + 16 (I Sin[x])^2 Cos[x]^8 + 16 (I Sin[x])^8 Cos[x]^2 + 6 (I Sin[x])^1 Cos[x]^9 + 6 (I Sin[x])^9 Cos[x]^1 + 1 Cos[x]^10 + 1 (I Sin[x])^10) + Exp[3 I y] (34 (I Sin[x])^4 Cos[x]^6 + 34 (I Sin[x])^6 Cos[x]^4 + 36 (I Sin[x])^5 Cos[x]^5 + 20 (I Sin[x])^3 Cos[x]^7 + 20 (I Sin[x])^7 Cos[x]^3 + 10 (I Sin[x])^2 Cos[x]^8 + 10 (I Sin[x])^8 Cos[x]^2 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1) + Exp[5 I y] (24 (I Sin[x])^5 Cos[x]^5 + 8 (I Sin[x])^3 Cos[x]^7 + 8 (I Sin[x])^7 Cos[x]^3 + 2 (I Sin[x])^2 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^2 + 14 (I Sin[x])^4 Cos[x]^6 + 14 (I Sin[x])^6 Cos[x]^4) + Exp[7 I y] (2 (I Sin[x])^3 Cos[x]^7 + 2 (I Sin[x])^7 Cos[x]^3 + 6 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^6 Cos[x]^4 + 4 (I Sin[x])^4 Cos[x]^6) + Exp[9 I y] (1 (I Sin[x])^6 Cos[x]^4 + 1 (I Sin[x])^4 Cos[x]^6))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-9 I y] (2 (I Sin[x])^5 Cos[x]^5) + Exp[-7 I y] (4 (I Sin[x])^4 Cos[x]^6 + 4 (I Sin[x])^6 Cos[x]^4 + 4 (I Sin[x])^3 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^3 + 1 (I Sin[x])^2 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^2) + Exp[-5 I y] (20 (I Sin[x])^4 Cos[x]^6 + 20 (I Sin[x])^6 Cos[x]^4 + 24 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^3 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^3) + Exp[-3 I y] (44 (I Sin[x])^5 Cos[x]^5 + 20 (I Sin[x])^3 Cos[x]^7 + 20 (I Sin[x])^7 Cos[x]^3 + 10 (I Sin[x])^2 Cos[x]^8 + 10 (I Sin[x])^8 Cos[x]^2 + 30 (I Sin[x])^4 Cos[x]^6 + 30 (I Sin[x])^6 Cos[x]^4 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1) + Exp[-1 I y] (30 (I Sin[x])^3 Cos[x]^7 + 30 (I Sin[x])^7 Cos[x]^3 + 72 (I Sin[x])^5 Cos[x]^5 + 54 (I Sin[x])^4 Cos[x]^6 + 54 (I Sin[x])^6 Cos[x]^4 + 6 (I Sin[x])^2 Cos[x]^8 + 6 (I Sin[x])^8 Cos[x]^2) + Exp[1 I y] (49 (I Sin[x])^4 Cos[x]^6 + 49 (I Sin[x])^6 Cos[x]^4 + 32 (I Sin[x])^3 Cos[x]^7 + 32 (I Sin[x])^7 Cos[x]^3 + 44 (I Sin[x])^5 Cos[x]^5 + 16 (I Sin[x])^2 Cos[x]^8 + 16 (I Sin[x])^8 Cos[x]^2 + 6 (I Sin[x])^1 Cos[x]^9 + 6 (I Sin[x])^9 Cos[x]^1 + 1 Cos[x]^10 + 1 (I Sin[x])^10) + Exp[3 I y] (34 (I Sin[x])^4 Cos[x]^6 + 34 (I Sin[x])^6 Cos[x]^4 + 36 (I Sin[x])^5 Cos[x]^5 + 20 (I Sin[x])^3 Cos[x]^7 + 20 (I Sin[x])^7 Cos[x]^3 + 10 (I Sin[x])^2 Cos[x]^8 + 10 (I Sin[x])^8 Cos[x]^2 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1) + Exp[5 I y] (24 (I Sin[x])^5 Cos[x]^5 + 8 (I Sin[x])^3 Cos[x]^7 + 8 (I Sin[x])^7 Cos[x]^3 + 2 (I Sin[x])^2 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^2 + 14 (I Sin[x])^4 Cos[x]^6 + 14 (I Sin[x])^6 Cos[x]^4) + Exp[7 I y] (2 (I Sin[x])^3 Cos[x]^7 + 2 (I Sin[x])^7 Cos[x]^3 + 6 (I Sin[x])^5 Cos[x]^5 + 4 (I Sin[x])^6 Cos[x]^4 + 4 (I Sin[x])^4 Cos[x]^6) + Exp[9 I y] (1 (I Sin[x])^6 Cos[x]^4 + 1 (I Sin[x])^4 Cos[x]^6));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = NMaximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

