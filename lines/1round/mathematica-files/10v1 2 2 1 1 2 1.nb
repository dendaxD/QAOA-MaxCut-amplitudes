amplitude2[x_,y_] := (Exp[-9 I y] (2 (I Sin[x])^5 Cos[x]^5) + Exp[-8 I y] (5 (I Sin[x])^6 Cos[x]^4 + 5 (I Sin[x])^4 Cos[x]^6 + 8 (I Sin[x])^5 Cos[x]^5) + Exp[-7 I y] (20 (I Sin[x])^4 Cos[x]^6 + 20 (I Sin[x])^6 Cos[x]^4 + 6 (I Sin[x])^3 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^3 + 20 (I Sin[x])^5 Cos[x]^5) + Exp[-6 I y] (44 (I Sin[x])^5 Cos[x]^5 + 22 (I Sin[x])^7 Cos[x]^3 + 22 (I Sin[x])^3 Cos[x]^7 + 36 (I Sin[x])^6 Cos[x]^4 + 36 (I Sin[x])^4 Cos[x]^6 + 4 (I Sin[x])^2 Cos[x]^8 + 4 (I Sin[x])^8 Cos[x]^2) + Exp[-5 I y] (60 (I Sin[x])^5 Cos[x]^5 + 34 (I Sin[x])^3 Cos[x]^7 + 34 (I Sin[x])^7 Cos[x]^3 + 15 (I Sin[x])^2 Cos[x]^8 + 15 (I Sin[x])^8 Cos[x]^2 + 45 (I Sin[x])^4 Cos[x]^6 + 45 (I Sin[x])^6 Cos[x]^4 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1) + Exp[-4 I y] (56 (I Sin[x])^4 Cos[x]^6 + 56 (I Sin[x])^6 Cos[x]^4 + 10 (I Sin[x])^8 Cos[x]^2 + 10 (I Sin[x])^2 Cos[x]^8 + 24 (I Sin[x])^7 Cos[x]^3 + 24 (I Sin[x])^3 Cos[x]^7 + 68 (I Sin[x])^5 Cos[x]^5 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1) + Exp[-3 I y] (30 (I Sin[x])^6 Cos[x]^4 + 30 (I Sin[x])^4 Cos[x]^6 + 22 (I Sin[x])^3 Cos[x]^7 + 22 (I Sin[x])^7 Cos[x]^3 + 24 (I Sin[x])^5 Cos[x]^5 + 13 (I Sin[x])^2 Cos[x]^8 + 13 (I Sin[x])^8 Cos[x]^2 + 6 (I Sin[x])^1 Cos[x]^9 + 6 (I Sin[x])^9 Cos[x]^1 + 1 Cos[x]^10 + 1 (I Sin[x])^10) + Exp[-2 I y] (20 (I Sin[x])^5 Cos[x]^5 + 10 (I Sin[x])^3 Cos[x]^7 + 10 (I Sin[x])^7 Cos[x]^3 + 14 (I Sin[x])^6 Cos[x]^4 + 14 (I Sin[x])^4 Cos[x]^6 + 2 (I Sin[x])^2 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^2) + Exp[-1 I y] (6 (I Sin[x])^5 Cos[x]^5 + 2 (I Sin[x])^3 Cos[x]^7 + 2 (I Sin[x])^7 Cos[x]^3 + 1 (I Sin[x])^2 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^2 + 3 (I Sin[x])^4 Cos[x]^6 + 3 (I Sin[x])^6 Cos[x]^4) + Exp[0 I y] (1 (I Sin[x])^4 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^4));
