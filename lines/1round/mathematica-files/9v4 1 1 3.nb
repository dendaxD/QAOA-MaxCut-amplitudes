amplitude2[x_,y_] := (Exp[-8 I y] (1 (I Sin[x])^4 Cos[x]^5 + 1 (I Sin[x])^5 Cos[x]^4) + Exp[-6 I y] (2 (I Sin[x])^3 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^3 + 1 (I Sin[x])^4 Cos[x]^5 + 1 (I Sin[x])^5 Cos[x]^4 + 3 (I Sin[x])^2 Cos[x]^7 + 3 (I Sin[x])^7 Cos[x]^2 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1) + Exp[-4 I y] (11 (I Sin[x])^3 Cos[x]^6 + 11 (I Sin[x])^6 Cos[x]^3 + 13 (I Sin[x])^5 Cos[x]^4 + 13 (I Sin[x])^4 Cos[x]^5 + 4 (I Sin[x])^2 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^2) + Exp[-2 I y] (11 (I Sin[x])^2 Cos[x]^7 + 11 (I Sin[x])^7 Cos[x]^2 + 20 (I Sin[x])^3 Cos[x]^6 + 20 (I Sin[x])^6 Cos[x]^3 + 22 (I Sin[x])^4 Cos[x]^5 + 22 (I Sin[x])^5 Cos[x]^4 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 1 Cos[x]^9 + 1 (I Sin[x])^9) + Exp[0 I y] (41 (I Sin[x])^4 Cos[x]^5 + 41 (I Sin[x])^5 Cos[x]^4 + 21 (I Sin[x])^6 Cos[x]^3 + 21 (I Sin[x])^3 Cos[x]^6 + 6 (I Sin[x])^2 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^2 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1) + Exp[2 I y] (3 (I Sin[x])^1 Cos[x]^8 + 3 (I Sin[x])^8 Cos[x]^1 + 30 (I Sin[x])^4 Cos[x]^5 + 30 (I Sin[x])^5 Cos[x]^4 + 17 (I Sin[x])^3 Cos[x]^6 + 17 (I Sin[x])^6 Cos[x]^3 + 6 (I Sin[x])^2 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^2) + Exp[4 I y] (15 (I Sin[x])^5 Cos[x]^4 + 15 (I Sin[x])^4 Cos[x]^5 + 4 (I Sin[x])^7 Cos[x]^2 + 4 (I Sin[x])^2 Cos[x]^7 + 9 (I Sin[x])^3 Cos[x]^6 + 9 (I Sin[x])^6 Cos[x]^3) + Exp[6 I y] (2 (I Sin[x])^2 Cos[x]^7 + 2 (I Sin[x])^7 Cos[x]^2 + 3 (I Sin[x])^5 Cos[x]^4 + 3 (I Sin[x])^4 Cos[x]^5 + 3 (I Sin[x])^3 Cos[x]^6 + 3 (I Sin[x])^6 Cos[x]^3) + Exp[8 I y] (1 (I Sin[x])^6 Cos[x]^3 + 1 (I Sin[x])^3 Cos[x]^6));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = NMaximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

