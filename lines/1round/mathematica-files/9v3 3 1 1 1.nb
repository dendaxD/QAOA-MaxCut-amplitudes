amplitude[x_,y_] := (Exp[-8 I y] (1 (I Sin[x])^4 Cos[x]^5 + 1 (I Sin[x])^5 Cos[x]^4) + Exp[-6 I y] (5 (I Sin[x])^5 Cos[x]^4 + 5 (I Sin[x])^4 Cos[x]^5 + 2 (I Sin[x])^3 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^3 + 1 (I Sin[x])^2 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^2) + Exp[-4 I y] (11 (I Sin[x])^3 Cos[x]^6 + 11 (I Sin[x])^6 Cos[x]^3 + 9 (I Sin[x])^5 Cos[x]^4 + 9 (I Sin[x])^4 Cos[x]^5 + 6 (I Sin[x])^2 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^2 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1) + Exp[-2 I y] (21 (I Sin[x])^6 Cos[x]^3 + 21 (I Sin[x])^3 Cos[x]^6 + 28 (I Sin[x])^4 Cos[x]^5 + 28 (I Sin[x])^5 Cos[x]^4 + 6 (I Sin[x])^2 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^2 + 1 (I Sin[x])^1 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^1) + Exp[0 I y] (11 (I Sin[x])^2 Cos[x]^7 + 11 (I Sin[x])^7 Cos[x]^2 + 35 (I Sin[x])^4 Cos[x]^5 + 35 (I Sin[x])^5 Cos[x]^4 + 20 (I Sin[x])^3 Cos[x]^6 + 20 (I Sin[x])^6 Cos[x]^3 + 3 (I Sin[x])^1 Cos[x]^8 + 3 (I Sin[x])^8 Cos[x]^1 + 1 Cos[x]^9 + 1 (I Sin[x])^9) + Exp[2 I y] (34 (I Sin[x])^5 Cos[x]^4 + 34 (I Sin[x])^4 Cos[x]^5 + 15 (I Sin[x])^3 Cos[x]^6 + 15 (I Sin[x])^6 Cos[x]^3 + 6 (I Sin[x])^2 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^2 + 1 (I Sin[x])^8 Cos[x]^1 + 1 (I Sin[x])^1 Cos[x]^8) + Exp[4 I y] (2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 11 (I Sin[x])^3 Cos[x]^6 + 11 (I Sin[x])^6 Cos[x]^3 + 11 (I Sin[x])^5 Cos[x]^4 + 11 (I Sin[x])^4 Cos[x]^5 + 4 (I Sin[x])^2 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^2) + Exp[6 I y] (4 (I Sin[x])^6 Cos[x]^3 + 4 (I Sin[x])^3 Cos[x]^6 + 3 (I Sin[x])^5 Cos[x]^4 + 3 (I Sin[x])^4 Cos[x]^5 + 1 (I Sin[x])^7 Cos[x]^2 + 1 (I Sin[x])^2 Cos[x]^7) + Exp[8 I y] (1 (I Sin[x])^2 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^2))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-8 I y] (1 (I Sin[x])^4 Cos[x]^5 + 1 (I Sin[x])^5 Cos[x]^4) + Exp[-6 I y] (5 (I Sin[x])^5 Cos[x]^4 + 5 (I Sin[x])^4 Cos[x]^5 + 2 (I Sin[x])^3 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^3 + 1 (I Sin[x])^2 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^2) + Exp[-4 I y] (11 (I Sin[x])^3 Cos[x]^6 + 11 (I Sin[x])^6 Cos[x]^3 + 9 (I Sin[x])^5 Cos[x]^4 + 9 (I Sin[x])^4 Cos[x]^5 + 6 (I Sin[x])^2 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^2 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1) + Exp[-2 I y] (21 (I Sin[x])^6 Cos[x]^3 + 21 (I Sin[x])^3 Cos[x]^6 + 28 (I Sin[x])^4 Cos[x]^5 + 28 (I Sin[x])^5 Cos[x]^4 + 6 (I Sin[x])^2 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^2 + 1 (I Sin[x])^1 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^1) + Exp[0 I y] (11 (I Sin[x])^2 Cos[x]^7 + 11 (I Sin[x])^7 Cos[x]^2 + 35 (I Sin[x])^4 Cos[x]^5 + 35 (I Sin[x])^5 Cos[x]^4 + 20 (I Sin[x])^3 Cos[x]^6 + 20 (I Sin[x])^6 Cos[x]^3 + 3 (I Sin[x])^1 Cos[x]^8 + 3 (I Sin[x])^8 Cos[x]^1 + 1 Cos[x]^9 + 1 (I Sin[x])^9) + Exp[2 I y] (34 (I Sin[x])^5 Cos[x]^4 + 34 (I Sin[x])^4 Cos[x]^5 + 15 (I Sin[x])^3 Cos[x]^6 + 15 (I Sin[x])^6 Cos[x]^3 + 6 (I Sin[x])^2 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^2 + 1 (I Sin[x])^8 Cos[x]^1 + 1 (I Sin[x])^1 Cos[x]^8) + Exp[4 I y] (2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 11 (I Sin[x])^3 Cos[x]^6 + 11 (I Sin[x])^6 Cos[x]^3 + 11 (I Sin[x])^5 Cos[x]^4 + 11 (I Sin[x])^4 Cos[x]^5 + 4 (I Sin[x])^2 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^2) + Exp[6 I y] (4 (I Sin[x])^6 Cos[x]^3 + 4 (I Sin[x])^3 Cos[x]^6 + 3 (I Sin[x])^5 Cos[x]^4 + 3 (I Sin[x])^4 Cos[x]^5 + 1 (I Sin[x])^7 Cos[x]^2 + 1 (I Sin[x])^2 Cos[x]^7) + Exp[8 I y] (1 (I Sin[x])^2 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^2));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = NMaximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

