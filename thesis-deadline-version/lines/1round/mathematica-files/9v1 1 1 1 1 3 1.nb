amplitude[x_,y_] := (Exp[-8 I y] (1 (I Sin[x])^5 Cos[x]^4 + 1 (I Sin[x])^4 Cos[x]^5) + Exp[-7 I y] (4 (I Sin[x])^6 Cos[x]^3 + 4 (I Sin[x])^3 Cos[x]^6 + 4 (I Sin[x])^5 Cos[x]^4 + 4 (I Sin[x])^4 Cos[x]^5) + Exp[-6 I y] (16 (I Sin[x])^4 Cos[x]^5 + 16 (I Sin[x])^5 Cos[x]^4 + 9 (I Sin[x])^6 Cos[x]^3 + 9 (I Sin[x])^3 Cos[x]^6 + 3 (I Sin[x])^2 Cos[x]^7 + 3 (I Sin[x])^7 Cos[x]^2) + Exp[-5 I y] (34 (I Sin[x])^5 Cos[x]^4 + 34 (I Sin[x])^4 Cos[x]^5 + 7 (I Sin[x])^2 Cos[x]^7 + 7 (I Sin[x])^7 Cos[x]^2 + 15 (I Sin[x])^3 Cos[x]^6 + 15 (I Sin[x])^6 Cos[x]^3) + Exp[-4 I y] (25 (I Sin[x])^3 Cos[x]^6 + 25 (I Sin[x])^6 Cos[x]^3 + 30 (I Sin[x])^5 Cos[x]^4 + 30 (I Sin[x])^4 Cos[x]^5 + 11 (I Sin[x])^7 Cos[x]^2 + 11 (I Sin[x])^2 Cos[x]^7 + 4 (I Sin[x])^1 Cos[x]^8 + 4 (I Sin[x])^8 Cos[x]^1) + Exp[-3 I y] (20 (I Sin[x])^6 Cos[x]^3 + 20 (I Sin[x])^3 Cos[x]^6 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 30 (I Sin[x])^4 Cos[x]^5 + 30 (I Sin[x])^5 Cos[x]^4 + 4 (I Sin[x])^2 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^2) + Exp[-2 I y] (8 (I Sin[x])^2 Cos[x]^7 + 8 (I Sin[x])^7 Cos[x]^2 + 9 (I Sin[x])^4 Cos[x]^5 + 9 (I Sin[x])^5 Cos[x]^4 + 8 (I Sin[x])^6 Cos[x]^3 + 8 (I Sin[x])^3 Cos[x]^6 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 1 Cos[x]^9 + 1 (I Sin[x])^9) + Exp[-1 I y] (3 (I Sin[x])^7 Cos[x]^2 + 3 (I Sin[x])^2 Cos[x]^7 + 2 (I Sin[x])^5 Cos[x]^4 + 2 (I Sin[x])^4 Cos[x]^5 + 3 (I Sin[x])^3 Cos[x]^6 + 3 (I Sin[x])^6 Cos[x]^3) + Exp[0 I y] (1 (I Sin[x])^1 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^1))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-8 I y] (1 (I Sin[x])^5 Cos[x]^4 + 1 (I Sin[x])^4 Cos[x]^5) + Exp[-7 I y] (4 (I Sin[x])^6 Cos[x]^3 + 4 (I Sin[x])^3 Cos[x]^6 + 4 (I Sin[x])^5 Cos[x]^4 + 4 (I Sin[x])^4 Cos[x]^5) + Exp[-6 I y] (16 (I Sin[x])^4 Cos[x]^5 + 16 (I Sin[x])^5 Cos[x]^4 + 9 (I Sin[x])^6 Cos[x]^3 + 9 (I Sin[x])^3 Cos[x]^6 + 3 (I Sin[x])^2 Cos[x]^7 + 3 (I Sin[x])^7 Cos[x]^2) + Exp[-5 I y] (34 (I Sin[x])^5 Cos[x]^4 + 34 (I Sin[x])^4 Cos[x]^5 + 7 (I Sin[x])^2 Cos[x]^7 + 7 (I Sin[x])^7 Cos[x]^2 + 15 (I Sin[x])^3 Cos[x]^6 + 15 (I Sin[x])^6 Cos[x]^3) + Exp[-4 I y] (25 (I Sin[x])^3 Cos[x]^6 + 25 (I Sin[x])^6 Cos[x]^3 + 30 (I Sin[x])^5 Cos[x]^4 + 30 (I Sin[x])^4 Cos[x]^5 + 11 (I Sin[x])^7 Cos[x]^2 + 11 (I Sin[x])^2 Cos[x]^7 + 4 (I Sin[x])^1 Cos[x]^8 + 4 (I Sin[x])^8 Cos[x]^1) + Exp[-3 I y] (20 (I Sin[x])^6 Cos[x]^3 + 20 (I Sin[x])^3 Cos[x]^6 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 30 (I Sin[x])^4 Cos[x]^5 + 30 (I Sin[x])^5 Cos[x]^4 + 4 (I Sin[x])^2 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^2) + Exp[-2 I y] (8 (I Sin[x])^2 Cos[x]^7 + 8 (I Sin[x])^7 Cos[x]^2 + 9 (I Sin[x])^4 Cos[x]^5 + 9 (I Sin[x])^5 Cos[x]^4 + 8 (I Sin[x])^6 Cos[x]^3 + 8 (I Sin[x])^3 Cos[x]^6 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 1 Cos[x]^9 + 1 (I Sin[x])^9) + Exp[-1 I y] (3 (I Sin[x])^7 Cos[x]^2 + 3 (I Sin[x])^2 Cos[x]^7 + 2 (I Sin[x])^5 Cos[x]^4 + 2 (I Sin[x])^4 Cos[x]^5 + 3 (I Sin[x])^3 Cos[x]^6 + 3 (I Sin[x])^6 Cos[x]^3) + Exp[0 I y] (1 (I Sin[x])^1 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^1));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = NMaximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

