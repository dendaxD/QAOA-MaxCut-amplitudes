nqubits = 9;
name = "9v7 2";
nstates = 4;

amplitude[x_,y_] := (Exp[-8 I y] (1 (I Sin[x])^2 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^2) + Exp[-7 I y] (2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 2 (I Sin[x])^6 Cos[x]^3 + 2 (I Sin[x])^3 Cos[x]^6 + 1 Cos[x]^9 + 1 (I Sin[x])^9 + 2 (I Sin[x])^5 Cos[x]^4 + 2 (I Sin[x])^4 Cos[x]^5 + 1 (I Sin[x])^2 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^2) + Exp[-6 I y] (2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 11 (I Sin[x])^3 Cos[x]^6 + 11 (I Sin[x])^6 Cos[x]^3 + 4 (I Sin[x])^2 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^2 + 11 (I Sin[x])^4 Cos[x]^5 + 11 (I Sin[x])^5 Cos[x]^4) + Exp[-5 I y] (15 (I Sin[x])^2 Cos[x]^7 + 15 (I Sin[x])^7 Cos[x]^2 + 20 (I Sin[x])^5 Cos[x]^4 + 20 (I Sin[x])^4 Cos[x]^5 + 5 (I Sin[x])^1 Cos[x]^8 + 5 (I Sin[x])^8 Cos[x]^1 + 16 (I Sin[x])^3 Cos[x]^6 + 16 (I Sin[x])^6 Cos[x]^3) + Exp[-4 I y] (9 (I Sin[x])^2 Cos[x]^7 + 9 (I Sin[x])^7 Cos[x]^2 + 39 (I Sin[x])^4 Cos[x]^5 + 39 (I Sin[x])^5 Cos[x]^4 + 22 (I Sin[x])^3 Cos[x]^6 + 22 (I Sin[x])^6 Cos[x]^3) + Exp[-3 I y] (23 (I Sin[x])^3 Cos[x]^6 + 23 (I Sin[x])^6 Cos[x]^3 + 27 (I Sin[x])^4 Cos[x]^5 + 27 (I Sin[x])^5 Cos[x]^4 + 6 (I Sin[x])^2 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^2) + Exp[-2 I y] (9 (I Sin[x])^3 Cos[x]^6 + 9 (I Sin[x])^6 Cos[x]^3 + 19 (I Sin[x])^5 Cos[x]^4 + 19 (I Sin[x])^4 Cos[x]^5) + Exp[-1 I y] (7 (I Sin[x])^4 Cos[x]^5 + 7 (I Sin[x])^5 Cos[x]^4 + 1 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^3) + Exp[0 I y] (1 (I Sin[x])^4 Cos[x]^5 + 1 (I Sin[x])^5 Cos[x]^4))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-8 I y] (1 (I Sin[x])^2 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^2) + Exp[-7 I y] (2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 2 (I Sin[x])^6 Cos[x]^3 + 2 (I Sin[x])^3 Cos[x]^6 + 1 Cos[x]^9 + 1 (I Sin[x])^9 + 2 (I Sin[x])^5 Cos[x]^4 + 2 (I Sin[x])^4 Cos[x]^5 + 1 (I Sin[x])^2 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^2) + Exp[-6 I y] (2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 11 (I Sin[x])^3 Cos[x]^6 + 11 (I Sin[x])^6 Cos[x]^3 + 4 (I Sin[x])^2 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^2 + 11 (I Sin[x])^4 Cos[x]^5 + 11 (I Sin[x])^5 Cos[x]^4) + Exp[-5 I y] (15 (I Sin[x])^2 Cos[x]^7 + 15 (I Sin[x])^7 Cos[x]^2 + 20 (I Sin[x])^5 Cos[x]^4 + 20 (I Sin[x])^4 Cos[x]^5 + 5 (I Sin[x])^1 Cos[x]^8 + 5 (I Sin[x])^8 Cos[x]^1 + 16 (I Sin[x])^3 Cos[x]^6 + 16 (I Sin[x])^6 Cos[x]^3) + Exp[-4 I y] (9 (I Sin[x])^2 Cos[x]^7 + 9 (I Sin[x])^7 Cos[x]^2 + 39 (I Sin[x])^4 Cos[x]^5 + 39 (I Sin[x])^5 Cos[x]^4 + 22 (I Sin[x])^3 Cos[x]^6 + 22 (I Sin[x])^6 Cos[x]^3) + Exp[-3 I y] (23 (I Sin[x])^3 Cos[x]^6 + 23 (I Sin[x])^6 Cos[x]^3 + 27 (I Sin[x])^4 Cos[x]^5 + 27 (I Sin[x])^5 Cos[x]^4 + 6 (I Sin[x])^2 Cos[x]^7 + 6 (I Sin[x])^7 Cos[x]^2) + Exp[-2 I y] (9 (I Sin[x])^3 Cos[x]^6 + 9 (I Sin[x])^6 Cos[x]^3 + 19 (I Sin[x])^5 Cos[x]^4 + 19 (I Sin[x])^4 Cos[x]^5) + Exp[-1 I y] (7 (I Sin[x])^4 Cos[x]^5 + 7 (I Sin[x])^5 Cos[x]^4 + 1 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^3) + Exp[0 I y] (1 (I Sin[x])^4 Cos[x]^5 + 1 (I Sin[x])^5 Cos[x]^4));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = NMaximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

