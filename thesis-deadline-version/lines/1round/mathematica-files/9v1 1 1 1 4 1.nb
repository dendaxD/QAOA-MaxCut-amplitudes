amplitude[x_,y_] := (Exp[-8 I y] (1 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^3) + Exp[-7 I y] (1 (I Sin[x])^2 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^2 + 4 (I Sin[x])^5 Cos[x]^4 + 4 (I Sin[x])^4 Cos[x]^5 + 3 (I Sin[x])^3 Cos[x]^6 + 3 (I Sin[x])^6 Cos[x]^3) + Exp[-6 I y] (14 (I Sin[x])^4 Cos[x]^5 + 14 (I Sin[x])^5 Cos[x]^4 + 5 (I Sin[x])^2 Cos[x]^7 + 5 (I Sin[x])^7 Cos[x]^2 + 9 (I Sin[x])^3 Cos[x]^6 + 9 (I Sin[x])^6 Cos[x]^3) + Exp[-5 I y] (16 (I Sin[x])^3 Cos[x]^6 + 16 (I Sin[x])^6 Cos[x]^3 + 29 (I Sin[x])^4 Cos[x]^5 + 29 (I Sin[x])^5 Cos[x]^4 + 8 (I Sin[x])^7 Cos[x]^2 + 8 (I Sin[x])^2 Cos[x]^7 + 3 (I Sin[x])^1 Cos[x]^8 + 3 (I Sin[x])^8 Cos[x]^1) + Exp[-4 I y] (42 (I Sin[x])^5 Cos[x]^4 + 42 (I Sin[x])^4 Cos[x]^5 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 22 (I Sin[x])^3 Cos[x]^6 + 22 (I Sin[x])^6 Cos[x]^3 + 4 (I Sin[x])^2 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^2) + Exp[-3 I y] (21 (I Sin[x])^4 Cos[x]^5 + 21 (I Sin[x])^5 Cos[x]^4 + 22 (I Sin[x])^3 Cos[x]^6 + 22 (I Sin[x])^6 Cos[x]^3 + 10 (I Sin[x])^2 Cos[x]^7 + 10 (I Sin[x])^7 Cos[x]^2 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 1 Cos[x]^9 + 1 (I Sin[x])^9) + Exp[-2 I y] (14 (I Sin[x])^4 Cos[x]^5 + 14 (I Sin[x])^5 Cos[x]^4 + 5 (I Sin[x])^2 Cos[x]^7 + 5 (I Sin[x])^7 Cos[x]^2 + 9 (I Sin[x])^6 Cos[x]^3 + 9 (I Sin[x])^3 Cos[x]^6) + Exp[-1 I y] (2 (I Sin[x])^5 Cos[x]^4 + 2 (I Sin[x])^4 Cos[x]^5 + 3 (I Sin[x])^2 Cos[x]^7 + 3 (I Sin[x])^7 Cos[x]^2 + 1 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^3 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1) + Exp[0 I y] (1 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^3))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-8 I y] (1 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^3) + Exp[-7 I y] (1 (I Sin[x])^2 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^2 + 4 (I Sin[x])^5 Cos[x]^4 + 4 (I Sin[x])^4 Cos[x]^5 + 3 (I Sin[x])^3 Cos[x]^6 + 3 (I Sin[x])^6 Cos[x]^3) + Exp[-6 I y] (14 (I Sin[x])^4 Cos[x]^5 + 14 (I Sin[x])^5 Cos[x]^4 + 5 (I Sin[x])^2 Cos[x]^7 + 5 (I Sin[x])^7 Cos[x]^2 + 9 (I Sin[x])^3 Cos[x]^6 + 9 (I Sin[x])^6 Cos[x]^3) + Exp[-5 I y] (16 (I Sin[x])^3 Cos[x]^6 + 16 (I Sin[x])^6 Cos[x]^3 + 29 (I Sin[x])^4 Cos[x]^5 + 29 (I Sin[x])^5 Cos[x]^4 + 8 (I Sin[x])^7 Cos[x]^2 + 8 (I Sin[x])^2 Cos[x]^7 + 3 (I Sin[x])^1 Cos[x]^8 + 3 (I Sin[x])^8 Cos[x]^1) + Exp[-4 I y] (42 (I Sin[x])^5 Cos[x]^4 + 42 (I Sin[x])^4 Cos[x]^5 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 22 (I Sin[x])^3 Cos[x]^6 + 22 (I Sin[x])^6 Cos[x]^3 + 4 (I Sin[x])^2 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^2) + Exp[-3 I y] (21 (I Sin[x])^4 Cos[x]^5 + 21 (I Sin[x])^5 Cos[x]^4 + 22 (I Sin[x])^3 Cos[x]^6 + 22 (I Sin[x])^6 Cos[x]^3 + 10 (I Sin[x])^2 Cos[x]^7 + 10 (I Sin[x])^7 Cos[x]^2 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1 + 1 Cos[x]^9 + 1 (I Sin[x])^9) + Exp[-2 I y] (14 (I Sin[x])^4 Cos[x]^5 + 14 (I Sin[x])^5 Cos[x]^4 + 5 (I Sin[x])^2 Cos[x]^7 + 5 (I Sin[x])^7 Cos[x]^2 + 9 (I Sin[x])^6 Cos[x]^3 + 9 (I Sin[x])^3 Cos[x]^6) + Exp[-1 I y] (2 (I Sin[x])^5 Cos[x]^4 + 2 (I Sin[x])^4 Cos[x]^5 + 3 (I Sin[x])^2 Cos[x]^7 + 3 (I Sin[x])^7 Cos[x]^2 + 1 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^3 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1) + Exp[0 I y] (1 (I Sin[x])^3 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^3));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = NMaximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

