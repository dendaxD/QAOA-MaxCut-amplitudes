amplitude2[x_,y_] := (Exp[-8 I y] (1 (I Sin[x])^4 Cos[x]^5 + 1 (I Sin[x])^5 Cos[x]^4) + Exp[-7 I y] (3 (I Sin[x])^3 Cos[x]^6 + 3 (I Sin[x])^6 Cos[x]^3 + 5 (I Sin[x])^4 Cos[x]^5 + 5 (I Sin[x])^5 Cos[x]^4) + Exp[-6 I y] (12 (I Sin[x])^5 Cos[x]^4 + 12 (I Sin[x])^4 Cos[x]^5 + 10 (I Sin[x])^3 Cos[x]^6 + 10 (I Sin[x])^6 Cos[x]^3 + 5 (I Sin[x])^2 Cos[x]^7 + 5 (I Sin[x])^7 Cos[x]^2 + 1 (I Sin[x])^1 Cos[x]^8 + 1 (I Sin[x])^8 Cos[x]^1) + Exp[-5 I y] (21 (I Sin[x])^4 Cos[x]^5 + 21 (I Sin[x])^5 Cos[x]^4 + 16 (I Sin[x])^6 Cos[x]^3 + 16 (I Sin[x])^3 Cos[x]^6 + 13 (I Sin[x])^2 Cos[x]^7 + 13 (I Sin[x])^7 Cos[x]^2 + 5 (I Sin[x])^1 Cos[x]^8 + 5 (I Sin[x])^8 Cos[x]^1 + 1 Cos[x]^9 + 1 (I Sin[x])^9) + Exp[-4 I y] (24 (I Sin[x])^6 Cos[x]^3 + 24 (I Sin[x])^3 Cos[x]^6 + 38 (I Sin[x])^4 Cos[x]^5 + 38 (I Sin[x])^5 Cos[x]^4 + 7 (I Sin[x])^2 Cos[x]^7 + 7 (I Sin[x])^7 Cos[x]^2 + 1 (I Sin[x])^8 Cos[x]^1 + 1 (I Sin[x])^1 Cos[x]^8) + Exp[-3 I y] (21 (I Sin[x])^3 Cos[x]^6 + 21 (I Sin[x])^6 Cos[x]^3 + 25 (I Sin[x])^4 Cos[x]^5 + 25 (I Sin[x])^5 Cos[x]^4 + 8 (I Sin[x])^2 Cos[x]^7 + 8 (I Sin[x])^7 Cos[x]^2 + 2 (I Sin[x])^1 Cos[x]^8 + 2 (I Sin[x])^8 Cos[x]^1) + Exp[-2 I y] (19 (I Sin[x])^5 Cos[x]^4 + 19 (I Sin[x])^4 Cos[x]^5 + 7 (I Sin[x])^3 Cos[x]^6 + 7 (I Sin[x])^6 Cos[x]^3 + 2 (I Sin[x])^7 Cos[x]^2 + 2 (I Sin[x])^2 Cos[x]^7) + Exp[-1 I y] (1 (I Sin[x])^2 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^2 + 5 (I Sin[x])^5 Cos[x]^4 + 5 (I Sin[x])^4 Cos[x]^5 + 2 (I Sin[x])^3 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^3) + Exp[0 I y] (1 (I Sin[x])^6 Cos[x]^3 + 1 (I Sin[x])^3 Cos[x]^6));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = NMaximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

