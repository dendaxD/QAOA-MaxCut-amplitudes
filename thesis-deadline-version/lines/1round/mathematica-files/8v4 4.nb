nqubits = 8;
name = "8v4 4";
nstates = 2;

amplitude[x_,y_] := (Exp[-7 I y] (2 (I Sin[x])^4 Cos[x]^4) + Exp[-6 I y] (2 (I Sin[x])^3 Cos[x]^5 + 2 (I Sin[x])^5 Cos[x]^3 + 2 (I Sin[x])^2 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^2 + 2 (I Sin[x])^1 Cos[x]^7 + 2 (I Sin[x])^7 Cos[x]^1 + 1 Cos[x]^8 + 1 (I Sin[x])^8) + Exp[-5 I y] (10 (I Sin[x])^3 Cos[x]^5 + 10 (I Sin[x])^5 Cos[x]^3 + 6 (I Sin[x])^2 Cos[x]^6 + 6 (I Sin[x])^6 Cos[x]^2 + 2 (I Sin[x])^1 Cos[x]^7 + 2 (I Sin[x])^7 Cos[x]^1 + 6 (I Sin[x])^4 Cos[x]^4) + Exp[-4 I y] (10 (I Sin[x])^2 Cos[x]^6 + 10 (I Sin[x])^6 Cos[x]^2 + 4 (I Sin[x])^1 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^1 + 18 (I Sin[x])^4 Cos[x]^4 + 12 (I Sin[x])^3 Cos[x]^5 + 12 (I Sin[x])^5 Cos[x]^3) + Exp[-3 I y] (6 (I Sin[x])^2 Cos[x]^6 + 6 (I Sin[x])^6 Cos[x]^2 + 26 (I Sin[x])^4 Cos[x]^4 + 16 (I Sin[x])^3 Cos[x]^5 + 16 (I Sin[x])^5 Cos[x]^3) + Exp[-2 I y] (12 (I Sin[x])^3 Cos[x]^5 + 12 (I Sin[x])^5 Cos[x]^3 + 4 (I Sin[x])^2 Cos[x]^6 + 4 (I Sin[x])^6 Cos[x]^2 + 10 (I Sin[x])^4 Cos[x]^4) + Exp[-1 I y] (4 (I Sin[x])^3 Cos[x]^5 + 4 (I Sin[x])^5 Cos[x]^3 + 6 (I Sin[x])^4 Cos[x]^4) + Exp[0 I y] (2 (I Sin[x])^4 Cos[x]^4))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-7 I y] (2 (I Sin[x])^4 Cos[x]^4) + Exp[-6 I y] (2 (I Sin[x])^3 Cos[x]^5 + 2 (I Sin[x])^5 Cos[x]^3 + 2 (I Sin[x])^2 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^2 + 2 (I Sin[x])^1 Cos[x]^7 + 2 (I Sin[x])^7 Cos[x]^1 + 1 Cos[x]^8 + 1 (I Sin[x])^8) + Exp[-5 I y] (10 (I Sin[x])^3 Cos[x]^5 + 10 (I Sin[x])^5 Cos[x]^3 + 6 (I Sin[x])^2 Cos[x]^6 + 6 (I Sin[x])^6 Cos[x]^2 + 2 (I Sin[x])^1 Cos[x]^7 + 2 (I Sin[x])^7 Cos[x]^1 + 6 (I Sin[x])^4 Cos[x]^4) + Exp[-4 I y] (10 (I Sin[x])^2 Cos[x]^6 + 10 (I Sin[x])^6 Cos[x]^2 + 4 (I Sin[x])^1 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^1 + 18 (I Sin[x])^4 Cos[x]^4 + 12 (I Sin[x])^3 Cos[x]^5 + 12 (I Sin[x])^5 Cos[x]^3) + Exp[-3 I y] (6 (I Sin[x])^2 Cos[x]^6 + 6 (I Sin[x])^6 Cos[x]^2 + 26 (I Sin[x])^4 Cos[x]^4 + 16 (I Sin[x])^3 Cos[x]^5 + 16 (I Sin[x])^5 Cos[x]^3) + Exp[-2 I y] (12 (I Sin[x])^3 Cos[x]^5 + 12 (I Sin[x])^5 Cos[x]^3 + 4 (I Sin[x])^2 Cos[x]^6 + 4 (I Sin[x])^6 Cos[x]^2 + 10 (I Sin[x])^4 Cos[x]^4) + Exp[-1 I y] (4 (I Sin[x])^3 Cos[x]^5 + 4 (I Sin[x])^5 Cos[x]^3 + 6 (I Sin[x])^4 Cos[x]^4) + Exp[0 I y] (2 (I Sin[x])^4 Cos[x]^4));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = NMaximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

f = probability[c, d]; n = Pi;
Plot3D[f, {c, 0, n/2}, {d, -n, n}, PlotRange -> All]
ContourPlot[probability[x, y], {x, 0, n/2}, {y, 0, n}, PlotLegends -> Automatic, Contours -> 30]
