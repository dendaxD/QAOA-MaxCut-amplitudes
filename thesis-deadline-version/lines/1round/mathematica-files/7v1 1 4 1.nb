nqubits = 7;
name = "7v1 1 4 1";
nstates = 4;

amplitude[x_,y_] := (Exp[-6 I y] (1 (I Sin[x])^2 Cos[x]^5 + 1 (I Sin[x])^5 Cos[x]^2) + Exp[-5 I y] (1 (I Sin[x])^1 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^1 + 3 (I Sin[x])^4 Cos[x]^3 + 3 (I Sin[x])^3 Cos[x]^4 + 2 (I Sin[x])^2 Cos[x]^5 + 2 (I Sin[x])^5 Cos[x]^2) + Exp[-4 I y] (9 (I Sin[x])^3 Cos[x]^4 + 9 (I Sin[x])^4 Cos[x]^3 + 2 (I Sin[x])^1 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^1 + 4 (I Sin[x])^2 Cos[x]^5 + 4 (I Sin[x])^5 Cos[x]^2) + Exp[-3 I y] (6 (I Sin[x])^2 Cos[x]^5 + 6 (I Sin[x])^5 Cos[x]^2 + 11 (I Sin[x])^3 Cos[x]^4 + 11 (I Sin[x])^4 Cos[x]^3 + 2 (I Sin[x])^1 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^1 + 1 Cos[x]^7 + 1 (I Sin[x])^7) + Exp[-2 I y] (10 (I Sin[x])^4 Cos[x]^3 + 10 (I Sin[x])^3 Cos[x]^4 + 5 (I Sin[x])^2 Cos[x]^5 + 5 (I Sin[x])^5 Cos[x]^2) + Exp[-1 I y] (1 (I Sin[x])^3 Cos[x]^4 + 1 (I Sin[x])^4 Cos[x]^3 + 3 (I Sin[x])^2 Cos[x]^5 + 3 (I Sin[x])^5 Cos[x]^2 + 2 (I Sin[x])^1 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^1) + Exp[0 I y] (1 (I Sin[x])^3 Cos[x]^4 + 1 (I Sin[x])^4 Cos[x]^3))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-6 I y] (1 (I Sin[x])^2 Cos[x]^5 + 1 (I Sin[x])^5 Cos[x]^2) + Exp[-5 I y] (1 (I Sin[x])^1 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^1 + 3 (I Sin[x])^4 Cos[x]^3 + 3 (I Sin[x])^3 Cos[x]^4 + 2 (I Sin[x])^2 Cos[x]^5 + 2 (I Sin[x])^5 Cos[x]^2) + Exp[-4 I y] (9 (I Sin[x])^3 Cos[x]^4 + 9 (I Sin[x])^4 Cos[x]^3 + 2 (I Sin[x])^1 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^1 + 4 (I Sin[x])^2 Cos[x]^5 + 4 (I Sin[x])^5 Cos[x]^2) + Exp[-3 I y] (6 (I Sin[x])^2 Cos[x]^5 + 6 (I Sin[x])^5 Cos[x]^2 + 11 (I Sin[x])^3 Cos[x]^4 + 11 (I Sin[x])^4 Cos[x]^3 + 2 (I Sin[x])^1 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^1 + 1 Cos[x]^7 + 1 (I Sin[x])^7) + Exp[-2 I y] (10 (I Sin[x])^4 Cos[x]^3 + 10 (I Sin[x])^3 Cos[x]^4 + 5 (I Sin[x])^2 Cos[x]^5 + 5 (I Sin[x])^5 Cos[x]^2) + Exp[-1 I y] (1 (I Sin[x])^3 Cos[x]^4 + 1 (I Sin[x])^4 Cos[x]^3 + 3 (I Sin[x])^2 Cos[x]^5 + 3 (I Sin[x])^5 Cos[x]^2 + 2 (I Sin[x])^1 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^1) + Exp[0 I y] (1 (I Sin[x])^3 Cos[x]^4 + 1 (I Sin[x])^4 Cos[x]^3));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = NMaximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

f = probability[c, d]; n = Pi;
Plot3D[f, {c, 0, n/2}, {d, -n, n}, PlotRange -> All]
ContourPlot[probability[x, y], {x, 0, n/2}, {y, 0, n}, PlotLegends -> Automatic, Contours -> 30]
