ammount = 11;
name = "11v2 1 2 1 2 2 1";
states = 4;

k = 0.1;


max = function[0, 0];
x = 0;
y = 0;


For[β = 0 , β <= Pi/2, β = β + k,
 	For[γ = 0 , γ <= 2Pi - 4β, γ = γ + k,
 	
  		max2 = function[β, γ];
  		If[max2 > max, {x = β, y = γ}];
  		max = Max[max, max2];
  	]
 ]

result = NMaximize[{Re[states*function[a, b]/(2^ammount)], x - k < a < x + k, y - k < b < y + k}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 3}];

Print[name,  ": ", result]

