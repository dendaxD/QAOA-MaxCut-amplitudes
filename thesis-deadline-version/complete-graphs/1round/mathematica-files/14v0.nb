nqubits = 14;
name = "14v0";
nstates = 2;

amplitude[x_,y_] := (Exp[-91.0 I y] (1 Cos[x]^14 + 1 (I Sin[x])^14) + Exp[-65.0 I y] (14 (I Sin[x])^1 Cos[x]^13 + 14 (I Sin[x])^13 Cos[x]^1) + Exp[-43.0 I y] (91 (I Sin[x])^2 Cos[x]^12 + 91 (I Sin[x])^12 Cos[x]^2) + Exp[-25.0 I y] (364 (I Sin[x])^3 Cos[x]^11 + 364 (I Sin[x])^11 Cos[x]^3) + Exp[-11.0 I y] (1001 (I Sin[x])^4 Cos[x]^10 + 1001 (I Sin[x])^10 Cos[x]^4) + Exp[-1.0 I y] (2002 (I Sin[x])^5 Cos[x]^9 + 2002 (I Sin[x])^9 Cos[x]^5) + Exp[5.0 I y] (3003 (I Sin[x])^6 Cos[x]^8 + 3003 (I Sin[x])^8 Cos[x]^6) + Exp[7.0 I y] (3432 (I Sin[x])^7 Cos[x]^7))/Sqrt[2^nqubits];
amplitude2[x_,y_] := (Exp[-91.0 I y] (1 Cos[x]^14 + 1 (I Sin[x])^14) + Exp[-65.0 I y] (14 (I Sin[x])^1 Cos[x]^13 + 14 (I Sin[x])^13 Cos[x]^1) + Exp[-43.0 I y] (91 (I Sin[x])^2 Cos[x]^12 + 91 (I Sin[x])^12 Cos[x]^2) + Exp[-25.0 I y] (364 (I Sin[x])^3 Cos[x]^11 + 364 (I Sin[x])^11 Cos[x]^3) + Exp[-11.0 I y] (1001 (I Sin[x])^4 Cos[x]^10 + 1001 (I Sin[x])^10 Cos[x]^4) + Exp[-1.0 I y] (2002 (I Sin[x])^5 Cos[x]^9 + 2002 (I Sin[x])^9 Cos[x]^5) + Exp[5.0 I y] (3003 (I Sin[x])^6 Cos[x]^8 + 3003 (I Sin[x])^8 Cos[x]^6) + Exp[7.0 I y] (3432 (I Sin[x])^7 Cos[x]^7));
probability[x_, y_] := Abs[amplitude[x, y]]^2;

result = Maximize[{nstates*probability[a, b], 0 < a < Pi/2, 0 < b < Pi}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 15}];
Print[name,  ": ", result]

f = probability[c, d]; n = Pi;

Plot3D[f, {c, 0, n/2}, {d, -n, n}, PlotRange -> All]
ContourPlot[probability[x, y], {x, 0, n/2}, {y, 0, n}, PlotLegends -> Automatic, Contours -> 30]
