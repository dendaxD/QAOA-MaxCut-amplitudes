amplitude2[x_,y_] := (Exp[-120.0 I y] (1 (I Sin[x])^3 Cos[x]^13 + 1 (I Sin[x])^13 Cos[x]^3) + Exp[-105.0 I y] (3 (I Sin[x])^2 Cos[x]^14 + 3 (I Sin[x])^14 Cos[x]^2 + 13 (I Sin[x])^4 Cos[x]^12 + 13 (I Sin[x])^12 Cos[x]^4) + Exp[-92.0 I y] (3 (I Sin[x])^1 Cos[x]^15 + 3 (I Sin[x])^15 Cos[x]^1 + 39 (I Sin[x])^3 Cos[x]^13 + 39 (I Sin[x])^13 Cos[x]^3 + 78 (I Sin[x])^5 Cos[x]^11 + 78 (I Sin[x])^11 Cos[x]^5) + Exp[-81.0 I y] (1 Cos[x]^16 + 1 (I Sin[x])^16 + 39 (I Sin[x])^2 Cos[x]^14 + 39 (I Sin[x])^14 Cos[x]^2 + 234 (I Sin[x])^4 Cos[x]^12 + 234 (I Sin[x])^12 Cos[x]^4 + 286 (I Sin[x])^6 Cos[x]^10 + 286 (I Sin[x])^10 Cos[x]^6) + Exp[-72.0 I y] (13 (I Sin[x])^1 Cos[x]^15 + 13 (I Sin[x])^15 Cos[x]^1 + 234 (I Sin[x])^3 Cos[x]^13 + 234 (I Sin[x])^13 Cos[x]^3 + 858 (I Sin[x])^5 Cos[x]^11 + 858 (I Sin[x])^11 Cos[x]^5 + 715 (I Sin[x])^7 Cos[x]^9 + 715 (I Sin[x])^9 Cos[x]^7) + Exp[-65.0 I y] (78 (I Sin[x])^2 Cos[x]^14 + 78 (I Sin[x])^14 Cos[x]^2 + 858 (I Sin[x])^4 Cos[x]^12 + 858 (I Sin[x])^12 Cos[x]^4 + 2145 (I Sin[x])^6 Cos[x]^10 + 2145 (I Sin[x])^10 Cos[x]^6 + 2574 (I Sin[x])^8 Cos[x]^8) + Exp[-60.0 I y] (286 (I Sin[x])^3 Cos[x]^13 + 286 (I Sin[x])^13 Cos[x]^3 + 2145 (I Sin[x])^5 Cos[x]^11 + 2145 (I Sin[x])^11 Cos[x]^5 + 5577 (I Sin[x])^7 Cos[x]^9 + 5577 (I Sin[x])^9 Cos[x]^7) + Exp[-57.0 I y] (715 (I Sin[x])^4 Cos[x]^12 + 715 (I Sin[x])^12 Cos[x]^4 + 5577 (I Sin[x])^6 Cos[x]^10 + 5577 (I Sin[x])^10 Cos[x]^6 + 10296 (I Sin[x])^8 Cos[x]^8) + Exp[-56.0 I y] (1287 (I Sin[x])^5 Cos[x]^11 + 1287 (I Sin[x])^11 Cos[x]^5 + 5148 (I Sin[x])^7 Cos[x]^9 + 5148 (I Sin[x])^9 Cos[x]^7));
