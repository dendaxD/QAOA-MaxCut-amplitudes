(Exp[-10 I y] (1 (I Sin[x])^4 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^4) + Exp[-6 I y] (13 (I Sin[x])^3 Cos[x]^7 + 13 (I Sin[x])^7 Cos[x]^3 + 20 (I Sin[x])^5 Cos[x]^5 + 7 (I Sin[x])^2 Cos[x]^8 + 7 (I Sin[x])^8 Cos[x]^2 + 13 (I Sin[x])^4 Cos[x]^6 + 13 (I Sin[x])^6 Cos[x]^4 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1) + Exp[-2 I y] (19 (I Sin[x])^2 Cos[x]^8 + 19 (I Sin[x])^8 Cos[x]^2 + 90 (I Sin[x])^4 Cos[x]^6 + 90 (I Sin[x])^6 Cos[x]^4 + 50 (I Sin[x])^3 Cos[x]^7 + 50 (I Sin[x])^7 Cos[x]^3 + 4 (I Sin[x])^1 Cos[x]^9 + 4 (I Sin[x])^9 Cos[x]^1 + 92 (I Sin[x])^5 Cos[x]^5 + 1 Cos[x]^10 + 1 (I Sin[x])^10) + Exp[2 I y] (4 (I Sin[x])^1 Cos[x]^9 + 4 (I Sin[x])^9 Cos[x]^1 + 120 (I Sin[x])^5 Cos[x]^5 + 46 (I Sin[x])^3 Cos[x]^7 + 46 (I Sin[x])^7 Cos[x]^3 + 85 (I Sin[x])^4 Cos[x]^6 + 85 (I Sin[x])^6 Cos[x]^4 + 15 (I Sin[x])^2 Cos[x]^8 + 15 (I Sin[x])^8 Cos[x]^2) + Exp[6 I y] (4 (I Sin[x])^2 Cos[x]^8 + 4 (I Sin[x])^8 Cos[x]^2 + 21 (I Sin[x])^6 Cos[x]^4 + 21 (I Sin[x])^4 Cos[x]^6 + 20 (I Sin[x])^5 Cos[x]^5 + 10 (I Sin[x])^3 Cos[x]^7 + 10 (I Sin[x])^7 Cos[x]^3) + Exp[10 I y] (1 (I Sin[x])^3 Cos[x]^7 + 1 (I Sin[x])^7 Cos[x]^3))

