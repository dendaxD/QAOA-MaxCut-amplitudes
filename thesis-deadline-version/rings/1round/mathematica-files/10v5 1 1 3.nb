ammount = 10;
name = "10v5 1 1 3";
states = 40;


k = 0.1;


max = function[0, 0];
x = 0;
y = 0;


For[β = 0 , β <= Pi/2, β = β + k,
 	For[γ = 0 , γ <= Pi/2 - β, γ = γ + k,
 	
  		max2 = function[β, γ];
