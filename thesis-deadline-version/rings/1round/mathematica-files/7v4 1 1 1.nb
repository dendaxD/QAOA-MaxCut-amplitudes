$Conjugate[x_] := x /. Complex[a_, b_] :> a - I b;
function[x_, y_] := $Conjugate[Exp[-7 I y] (1 (I Sin[x])^2 Cos[x]^5 + 1 (I Sin[x])^5 Cos[x]^2) + Exp[-3 I y] (3 (I Sin[x])^1 Cos[x]^6 + 3 (I Sin[x])^6 Cos[x]^1 + 12 (I Sin[x])^3 Cos[x]^4 + 12 (I Sin[x])^4 Cos[x]^3 + 6 (I Sin[x])^2 Cos[x]^5 + 6 (I Sin[x])^5 Cos[x]^2) + Exp[1 I y] (1 Cos[x]^7 + 1 (I Sin[x])^7 + 21 (I Sin[x])^4 Cos[x]^3 + 21 (I Sin[x])^3 Cos[x]^4 + 11 (I Sin[x])^2 Cos[x]^5 + 11 (I Sin[x])^5 Cos[x]^2 + 2 (I Sin[x])^1 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^1) + Exp[5 I y] (2 (I Sin[x])^1 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^1 + 2 (I Sin[x])^3 Cos[x]^4 + 2 (I Sin[x])^4 Cos[x]^3 + 3 (I Sin[x])^5 Cos[x]^2 + 3 (I Sin[x])^2 Cos[x]^5)]*
(Exp[-7 I y] (1 (I Sin[x])^2 Cos[x]^5 + 1 (I Sin[x])^5 Cos[x]^2) + Exp[-3 I y] (3 (I Sin[x])^1 Cos[x]^6 + 3 (I Sin[x])^6 Cos[x]^1 + 12 (I Sin[x])^3 Cos[x]^4 + 12 (I Sin[x])^4 Cos[x]^3 + 6 (I Sin[x])^2 Cos[x]^5 + 6 (I Sin[x])^5 Cos[x]^2) + Exp[1 I y] (1 Cos[x]^7 + 1 (I Sin[x])^7 + 21 (I Sin[x])^4 Cos[x]^3 + 21 (I Sin[x])^3 Cos[x]^4 + 11 (I Sin[x])^2 Cos[x]^5 + 11 (I Sin[x])^5 Cos[x]^2 + 2 (I Sin[x])^1 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^1) + Exp[5 I y] (2 (I Sin[x])^1 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^1 + 2 (I Sin[x])^3 Cos[x]^4 + 2 (I Sin[x])^4 Cos[x]^3 + 3 (I Sin[x])^5 Cos[x]^2 + 3 (I Sin[x])^2 Cos[x]^5))

amplitude[x_,y_] := Exp[-7 I y] (1 (I Sin[x])^2 Cos[x]^5 + 1 (I Sin[x])^5 Cos[x]^2) + Exp[-3 I y] (3 (I Sin[x])^1 Cos[x]^6 + 3 (I Sin[x])^6 Cos[x]^1 + 12 (I Sin[x])^3 Cos[x]^4 + 12 (I Sin[x])^4 Cos[x]^3 + 6 (I Sin[x])^2 Cos[x]^5 + 6 (I Sin[x])^5 Cos[x]^2) + Exp[1 I y] (1 Cos[x]^7 + 1 (I Sin[x])^7 + 21 (I Sin[x])^4 Cos[x]^3 + 21 (I Sin[x])^3 Cos[x]^4 + 11 (I Sin[x])^2 Cos[x]^5 + 11 (I Sin[x])^5 Cos[x]^2 + 2 (I Sin[x])^1 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^1) + Exp[5 I y] (2 (I Sin[x])^1 Cos[x]^6 + 2 (I Sin[x])^6 Cos[x]^1 + 2 (I Sin[x])^3 Cos[x]^4 + 2 (I Sin[x])^4 Cos[x]^3 + 3 (I Sin[x])^5 Cos[x]^2 + 3 (I Sin[x])^2 Cos[x]^5)

ammount = 7;
name = "7v4 1 1 1";
states = 14;


k = 0.1;


max = function[0, 0];
x = 0;
y = 0;


For[β = 0 , β <= Pi/2, β = β + k,
 	For[γ = 0 , γ <= Pi/2 - β, γ = γ + k,
 	
  		max2 = function[β, γ];
  		If[max2 > max, {x = β, y = γ}];
  		max = Max[max, max2];
  	]
 ]

result = NMaximize[{Re[states*function[a, b]/(2^ammount)], x - k < a < x + k, y - k < b < y + k}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 3}];

Print[name,  ": ", result]

f = function[c, d]; n = Pi;
Plot3D[f,{c,0,n},{d,0,n}, PlotRange -> All]

ContourPlot[function[x, y], {x, 0, n}, {y, 0, n}, PlotLegends -> Automatic, Contours -> 30]
