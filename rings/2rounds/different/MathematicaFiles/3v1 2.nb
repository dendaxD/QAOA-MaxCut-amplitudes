$Conjugate[x_] := x /. Complex[a_, b_] :> a - I b;
function[x_, y_, x2_, y2_] := $Conjugate[Exp[3 I y2] (Exp[3 I y] (Cos[x]^3 + (I Sin[x])^3) + Exp[-1 I y] (3 (I Sin[x])^1 Cos[x]^2 + 3 (I Sin[x])^2 Cos[x]^1)) ((I Sin[x2])^1 Cos[x2]^2 + (I Sin[x2])^2 Cos[x2]^1) + Exp[-1 I y2] (Exp[3 I y] ((I Sin[x])^1 Cos[x]^2 + (I Sin[x])^2 Cos[x]^1) + Exp[-1 I y] (2 (I Sin[x])^1 Cos[x]^2 + 2 (I Sin[x])^2 Cos[x]^1 + Cos[x]^3 + (I Sin[x])^3)) (2 (I Sin[x2])^1 Cos[x2]^2 + 2 (I Sin[x2])^2 Cos[x2]^1 + Cos[x2]^3 + (I Sin[x2])^3)]*(Exp[3 I y2] (Exp[3 I y] (Cos[x]^3 + (I Sin[x])^3) + Exp[-1 I y] (3 (I Sin[x])^1 Cos[x]^2 + 3 (I Sin[x])^2 Cos[x]^1)) ((I Sin[x2])^1 Cos[x2]^2 + (I Sin[x2])^2 Cos[x2]^1) + Exp[-1 I y2] (Exp[3 I y] ((I Sin[x])^1 Cos[x]^2 + (I Sin[x])^2 Cos[x]^1) + Exp[-1 I y] (2 (I Sin[x])^1 Cos[x]^2 + 2 (I Sin[x])^2 Cos[x]^1 + Cos[x]^3 + (I Sin[x])^3)) (2 (I Sin[x2])^1 Cos[x2]^2 + 2 (I Sin[x2])^2 Cos[x2]^1 + Cos[x2]^3 + (I Sin[x2])^3))
ammount = 3;
name = "3v1 2";
states = 6;

k = 0.1;

max = function[0,0,0,0];x=0;y=0;z=0;w=0;
For[β = 0 , β <= Pi, β = β+k,
	For[γ = 0 , γ <= Pi, γ = γ+k,
		For[δ = 0 , δ <= Pi, δ = δ+k,
			For[α = 0 , α <= Pi, α = α+k,
				max2 = function[β,γ,δ,α];
				If[max2>max,{x = β, y = γ, z = δ, w = α}];
				max=Max[max, max2];
			]
		]
	]	
]

result = NMaximize[{Re[states*function[a, b, c, d]/(2^ammount)], x - k < a < x + k, y - k < b < y + k, z-k < c < z+k, w-k < d < w+k}, {a, b, c, d}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 3}];
Print[name,  ": ", result]


Clear[x,y,v,w]; n = Pi;

Manipulate[Plot3D[function[x, y, v, w],{x,-n,n},{y,-n,n}, PlotRange -> All],{v,-n,n},{w,-n,n}]

Clear[x,y,v,w]; n = Pi;
Manipulate[ContourPlot[function[x, y, v, w], {x, -n, n}, {y, -n, n}, PlotLegends -> Automatic, Contours -> 30],{v,-n,n},{w,-n,n}]
