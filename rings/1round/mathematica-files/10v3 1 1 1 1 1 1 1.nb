function[x_, y_] := $Conjugate[Exp[-10 I y] (1 (I Sin[x])^4 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^4) + Exp[-6 I y] (10 (I Sin[x])^3 Cos[x]^7 + 10 (I Sin[x])^7 Cos[x]^3 + 30 (I Sin[x])^5 Cos[x]^5 + 20 (I Sin[x])^4 Cos[x]^6 + 20 (I Sin[x])^6 Cos[x]^4) + Exp[-2 I y] (15 (I Sin[x])^2 Cos[x]^8 + 15 (I Sin[x])^8 Cos[x]^2 + 95 (I Sin[x])^6 Cos[x]^4 + 95 (I Sin[x])^4 Cos[x]^6 + 120 (I Sin[x])^5 Cos[x]^5 + 40 (I Sin[x])^3 Cos[x]^7 + 40 (I Sin[x])^7 Cos[x]^3) + Exp[2 I y] (7 (I Sin[x])^1 Cos[x]^9 + 7 (I Sin[x])^9 Cos[x]^1 + 58 (I Sin[x])^7 Cos[x]^3 + 58 (I Sin[x])^3 Cos[x]^7 + 90 (I Sin[x])^5 Cos[x]^5 + 82 (I Sin[x])^6 Cos[x]^4 + 82 (I Sin[x])^4 Cos[x]^6 + 18 (I Sin[x])^2 Cos[x]^8 + 18 (I Sin[x])^8 Cos[x]^2) + Exp[6 I y] (1 Cos[x]^10 + 1 (I Sin[x])^10 + 12 (I Sin[x])^8 Cos[x]^2 + 12 (I Sin[x])^2 Cos[x]^8 + 12 (I Sin[x])^6 Cos[x]^4 + 12 (I Sin[x])^4 Cos[x]^6 + 12 (I Sin[x])^7 Cos[x]^3 + 12 (I Sin[x])^3 Cos[x]^7 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1 + 12 (I Sin[x])^5 Cos[x]^5) + Exp[10 I y] (1 (I Sin[x])^1 Cos[x]^9 + 1 (I Sin[x])^9 Cos[x]^1)]*
(Exp[-10 I y] (1 (I Sin[x])^4 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^4) + Exp[-6 I y] (10 (I Sin[x])^3 Cos[x]^7 + 10 (I Sin[x])^7 Cos[x]^3 + 30 (I Sin[x])^5 Cos[x]^5 + 20 (I Sin[x])^4 Cos[x]^6 + 20 (I Sin[x])^6 Cos[x]^4) + Exp[-2 I y] (15 (I Sin[x])^2 Cos[x]^8 + 15 (I Sin[x])^8 Cos[x]^2 + 95 (I Sin[x])^6 Cos[x]^4 + 95 (I Sin[x])^4 Cos[x]^6 + 120 (I Sin[x])^5 Cos[x]^5 + 40 (I Sin[x])^3 Cos[x]^7 + 40 (I Sin[x])^7 Cos[x]^3) + Exp[2 I y] (7 (I Sin[x])^1 Cos[x]^9 + 7 (I Sin[x])^9 Cos[x]^1 + 58 (I Sin[x])^7 Cos[x]^3 + 58 (I Sin[x])^3 Cos[x]^7 + 90 (I Sin[x])^5 Cos[x]^5 + 82 (I Sin[x])^6 Cos[x]^4 + 82 (I Sin[x])^4 Cos[x]^6 + 18 (I Sin[x])^2 Cos[x]^8 + 18 (I Sin[x])^8 Cos[x]^2) + Exp[6 I y] (1 Cos[x]^10 + 1 (I Sin[x])^10 + 12 (I Sin[x])^8 Cos[x]^2 + 12 (I Sin[x])^2 Cos[x]^8 + 12 (I Sin[x])^6 Cos[x]^4 + 12 (I Sin[x])^4 Cos[x]^6 + 12 (I Sin[x])^7 Cos[x]^3 + 12 (I Sin[x])^3 Cos[x]^7 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1 + 12 (I Sin[x])^5 Cos[x]^5) + Exp[10 I y] (1 (I Sin[x])^1 Cos[x]^9 + 1 (I Sin[x])^9 Cos[x]^1))

amplitude[x_,y_] := Exp[-10 I y] (1 (I Sin[x])^4 Cos[x]^6 + 1 (I Sin[x])^6 Cos[x]^4) + Exp[-6 I y] (10 (I Sin[x])^3 Cos[x]^7 + 10 (I Sin[x])^7 Cos[x]^3 + 30 (I Sin[x])^5 Cos[x]^5 + 20 (I Sin[x])^4 Cos[x]^6 + 20 (I Sin[x])^6 Cos[x]^4) + Exp[-2 I y] (15 (I Sin[x])^2 Cos[x]^8 + 15 (I Sin[x])^8 Cos[x]^2 + 95 (I Sin[x])^6 Cos[x]^4 + 95 (I Sin[x])^4 Cos[x]^6 + 120 (I Sin[x])^5 Cos[x]^5 + 40 (I Sin[x])^3 Cos[x]^7 + 40 (I Sin[x])^7 Cos[x]^3) + Exp[2 I y] (7 (I Sin[x])^1 Cos[x]^9 + 7 (I Sin[x])^9 Cos[x]^1 + 58 (I Sin[x])^7 Cos[x]^3 + 58 (I Sin[x])^3 Cos[x]^7 + 90 (I Sin[x])^5 Cos[x]^5 + 82 (I Sin[x])^6 Cos[x]^4 + 82 (I Sin[x])^4 Cos[x]^6 + 18 (I Sin[x])^2 Cos[x]^8 + 18 (I Sin[x])^8 Cos[x]^2) + Exp[6 I y] (1 Cos[x]^10 + 1 (I Sin[x])^10 + 12 (I Sin[x])^8 Cos[x]^2 + 12 (I Sin[x])^2 Cos[x]^8 + 12 (I Sin[x])^6 Cos[x]^4 + 12 (I Sin[x])^4 Cos[x]^6 + 12 (I Sin[x])^7 Cos[x]^3 + 12 (I Sin[x])^3 Cos[x]^7 + 2 (I Sin[x])^1 Cos[x]^9 + 2 (I Sin[x])^9 Cos[x]^1 + 12 (I Sin[x])^5 Cos[x]^5) + Exp[10 I y] (1 (I Sin[x])^1 Cos[x]^9 + 1 (I Sin[x])^9 Cos[x]^1)

ammount = 10;
name = "10v3 1 1 1 1 1 1 1";
states = 20;


k = 0.1;


max = function[0, 0];
x = 0;
y = 0;


For[β = 0 , β <= Pi/2, β = β + k,
 	For[γ = 0 , γ <= Pi/2 - β, γ = γ + k,
 	
  		max2 = function[β, γ];
  		If[max2 > max, {x = β, y = γ}];
  		max = Max[max, max2];
  	]
 ]

result = NMaximize[{Re[states*function[a, b]/(2^ammount)], x - k < a < x + k, y - k < b < y + k}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 3}];

Print[name,  ": ", result]

f = function[c, d]; n = Pi;
