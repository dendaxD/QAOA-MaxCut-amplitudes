$Conjugate[x_] := x /. Complex[a_, b_] :> a - I b;
function[x_, y_] := $Conjugate[Exp[-8 I y] (2 (I Sin[x])^4 Cos[x]^4) + Exp[-6 I y] (16 (I Sin[x])^3 Cos[x]^5 + 16 (I Sin[x])^5 Cos[x]^3 + 3 (I Sin[x])^2 Cos[x]^6 + 3 (I Sin[x])^6 Cos[x]^2 + 18 (I Sin[x])^4 Cos[x]^4) + Exp[-4 I y] (38 (I Sin[x])^4 Cos[x]^4 + 19 (I Sin[x])^2 Cos[x]^6 + 19 (I Sin[x])^6 Cos[x]^2 + 4 (I Sin[x])^1 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^1 + 28 (I Sin[x])^3 Cos[x]^5 + 28 (I Sin[x])^5 Cos[x]^3) + Exp[-2 I y] (12 (I Sin[x])^5 Cos[x]^3 + 12 (I Sin[x])^3 Cos[x]^5 + 4 (I Sin[x])^1 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^1 + 1 Cos[x]^8 + 1 (I Sin[x])^8 + 12 (I Sin[x])^4 Cos[x]^4 + 5 (I Sin[x])^6 Cos[x]^2 + 5 (I Sin[x])^2 Cos[x]^6) + Exp[0 I y] (1 (I Sin[x])^6 Cos[x]^2 + 1 (I Sin[x])^2 Cos[x]^6)]*
(Exp[-8 I y] (2 (I Sin[x])^4 Cos[x]^4) + Exp[-6 I y] (16 (I Sin[x])^3 Cos[x]^5 + 16 (I Sin[x])^5 Cos[x]^3 + 3 (I Sin[x])^2 Cos[x]^6 + 3 (I Sin[x])^6 Cos[x]^2 + 18 (I Sin[x])^4 Cos[x]^4) + Exp[-4 I y] (38 (I Sin[x])^4 Cos[x]^4 + 19 (I Sin[x])^2 Cos[x]^6 + 19 (I Sin[x])^6 Cos[x]^2 + 4 (I Sin[x])^1 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^1 + 28 (I Sin[x])^3 Cos[x]^5 + 28 (I Sin[x])^5 Cos[x]^3) + Exp[-2 I y] (12 (I Sin[x])^5 Cos[x]^3 + 12 (I Sin[x])^3 Cos[x]^5 + 4 (I Sin[x])^1 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^1 + 1 Cos[x]^8 + 1 (I Sin[x])^8 + 12 (I Sin[x])^4 Cos[x]^4 + 5 (I Sin[x])^6 Cos[x]^2 + 5 (I Sin[x])^2 Cos[x]^6) + Exp[0 I y] (1 (I Sin[x])^6 Cos[x]^2 + 1 (I Sin[x])^2 Cos[x]^6))

amplitude[x_,y_] := Exp[-8 I y] (2 (I Sin[x])^4 Cos[x]^4) + Exp[-6 I y] (16 (I Sin[x])^3 Cos[x]^5 + 16 (I Sin[x])^5 Cos[x]^3 + 3 (I Sin[x])^2 Cos[x]^6 + 3 (I Sin[x])^6 Cos[x]^2 + 18 (I Sin[x])^4 Cos[x]^4) + Exp[-4 I y] (38 (I Sin[x])^4 Cos[x]^4 + 19 (I Sin[x])^2 Cos[x]^6 + 19 (I Sin[x])^6 Cos[x]^2 + 4 (I Sin[x])^1 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^1 + 28 (I Sin[x])^3 Cos[x]^5 + 28 (I Sin[x])^5 Cos[x]^3) + Exp[-2 I y] (12 (I Sin[x])^5 Cos[x]^3 + 12 (I Sin[x])^3 Cos[x]^5 + 4 (I Sin[x])^1 Cos[x]^7 + 4 (I Sin[x])^7 Cos[x]^1 + 1 Cos[x]^8 + 1 (I Sin[x])^8 + 12 (I Sin[x])^4 Cos[x]^4 + 5 (I Sin[x])^6 Cos[x]^2 + 5 (I Sin[x])^2 Cos[x]^6) + Exp[0 I y] (1 (I Sin[x])^6 Cos[x]^2 + 1 (I Sin[x])^2 Cos[x]^6)

ammount = 8;
name = "8v2 1 1 1 1 2";
states = 16;


k = 0.1;


max = function[0, 0];
x = 0;
y = 0;


For[β = 0 , β <= Pi/2, β = β + k,
 	For[γ = 0 , γ <= Pi/2 - β, γ = γ + k,
 	
  		max2 = function[β, γ];
  		If[max2 > max, {x = β, y = γ}];
  		max = Max[max, max2];
  	]
 ]

result = NMaximize[{Re[states*function[a, b]/(2^ammount)], x - k < a < x + k, y - k < b < y + k}, {a, b}, Method -> {"SimulatedAnnealing", "PerturbationScale" -> 3}];

Print[name,  ": ", result]

f = function[c, d]; n = Pi;
Plot3D[f,{c,0,n},{d,0,n}, PlotRange -> All]

ContourPlot[function[x, y], {x, 0, n}, {y, 0, n}, PlotLegends -> Automatic, Contours -> 30]
